          if (!this.get("controllers.application.loggedIn")) {
            this.get("controllers.application").send("doLogin");
            return;
          }
          return ajax.setBookmark({
            "recipeId": this.get("model.recipeId"),
            "setBookmark": prm
          }, (function(_this) {
            return function(data) {
              var tmpIncDecBookmark, tmpJustBookmarked;
              if (data) {
                tmpJustBookmarked = !!+data;
                tmpIncDecBookmark = _this.get("model.bookmarkCnt");
                _this.set("model.isLiked", tmpJustBookmarked);
                tmpIncDecBookmark += tmpJustBookmarked ? 1 : -1;
                return _this.set("model.bookmarkCnt", tmpIncDecBookmark);
              }
            };
          })(this));
        },
        randomOther: function() {
          return this.set("randomWatch", (new Date).getTime());
        },
        gotoHome: function() {
          this.transitionToRoute('index');
          return this.set("searchQuery", "");
        }
      }
    });

    __exports__["default"] = RecipeController;;
  });
define("v2/controllers/profile", 
  ["ember","v2/utils/ajax-resolver","v2/models/user","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ajax = __dependency2__["default"];

    var user = __dependency3__["default"];

    var ProfileController;

    ProfileController = Ember.Controller.extend({
      needs: ["application"],
      isSelf: (function() {
        if (this.get("self") === "My") {
          return true;
        }
        return this.get("self") === this.get("controllers.application.user.id");
      }).property("self"),
      actions: {
        toggleFollow: function(state, id) {
          var modelId;
          if (!this.get("controllers.application.loggedIn")) {
            this.get("controllers.application").send("doLogin");
            return;
          }
          modelId = id || this.get("model.id");
          return ajax.setFollow(modelId, state).then((function(_this) {
            return function(res) {
              var toBeRemoved;
              _this.set("model.alreadyFollow", res);
              _this.set("model.followSum", _this.get("model.followSum") + (res ? 1 : -1));
              if (_this.get("social")) {
                if (state) {
                  return ajax.getUser({
                    userId: _this.get("controllers.application.user.id")
                  }).then(function(data) {
                    var me;
                    me = user.create(data);
                    me.set("isMe", true);
                    return _this.get("social").addObject(me);
                  });
                } else {
                  toBeRemoved = _this.get("social").filter(function(i) {
                    return i.id === _this.get("controllers.application.user.id");
                  });
                  return _this.get("social").removeObjects(toBeRemoved);
                }
              }
            };
          })(this));
        }
      }
    });

    __exports__["default"] = ProfileController;
  });
define("v2/controllers/profile/profile-follow", 
  ["ember","v2/utils/ajax-resolver","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ajax = __dependency2__["default"];

    var ProfileFollowController;

    ProfileFollowController = Ember.Controller.extend({
      needs: ["application", "profile"],
      user: "controllers.application.user",
      actions: {
        toggleFollow: function(state, user) {
          var isSelf;
          if (!this.get("controllers.application.loggedIn")) {
            this.get("controllers.application").send("doLogin");
            return;
          }
          isSelf = this.get("controllers.profile.isSelf");
          return ajax.setFollow(user.id, state).then((function(_this) {
            return function(res) {
              _this.model.following.map(function(i) {
                if (i.id === user.id) {
                  return i.set("following", !!state);
                }
              });
              _this.model.followers.map(function(i) {
                if (i.id === user.id) {
                  return i.set("following", !!state);
                }
              });
              if (isSelf) {
                if (state) {
                  return _this.model.following.addObject(user);
                } else {
                  return _this.model.following.map(function(i) {
                    if (i.id === user.id) {
                      return _this.model.following.removeObject(user);
                    }
                  });
                }
              }
            };
          })(this));
        }
      }
    });

    __exports__["default"] = ProfileFollowController;
  });
define("v2/controllers/profile/profile-settings", 
  ["ember","v2/utils/ajax-resolver","v2/utils/recipe-utils","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ajax = __dependency2__["default"];

    var RUtils = __dependency3__["default"];

    var ProfileFollowController;

    ProfileFollowController = Ember.Controller.extend({
      needs: ["application", "profile"],
      user: Ember.computed.alias('controllers.application.user'),
      actions: {
        uploadPicture: function() {
          var $uploadField;
          window.ga && window.ga('send', 'event', 'Editor', 'uploadPicture', "true");
          $uploadField = $(".fileClick");
          $uploadField.unbind("change");
          $uploadField.bind("change", (function(_this) {
            return function(event) {
              var files;
              files = event.target.files;
              return RUtils.uploadFiles(files, function(newURL) {
                return ajax.updateUserPicture(newURL.filePath, function(res) {
                  if (res) {
                    _this.set("user.avatar", newURL.filePath);
                    _this.get("user").saveLocalStorage();
                    return console.log(newURL);
                  }
                });
              });
            };
          })(this));
          return $uploadField.click();
        }
      }
    });

    __exports__["default"] = ProfileFollowController;
  });
define("v2/controllers/quick_view", 
  ["v2/utils/ajax-resolver","v2/utils/recipe-utils","v2/utils/recipe-request-resolver","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var ajax = __dependency1__["default"];

    var recipeModel = __dependency2__["default"];

    var RecipeRequestController = __dependency3__["default"];

    var ENV = __dependency4__["default"];

    var QuickViewController;

    QuickViewController = Ember.ObjectController.extend({
      queryParams: ["activeStyle"],
      needs: ["application"],
      activeStyle: "arrow",
      viewOnly: true,
      hoverEnabled: false,
      recipe: Ember.computed.alias("content"),
      currStepNum: 0,
      displayArr: (function() {
        var i, j, tmp, _i, _j, _len, _len1, _ref, _ref1;
        tmp = [];
        _ref = this.get('model.stepsArr');
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          i = _ref[_i];
          if (i.side) {
            _ref1 = i.subs;
            for (_j = 0, _len1 = _ref1.length; _j < _len1; _j++) {
              j = _ref1[_j];
              tmp.push(j);
            }
          } else {
            tmp.push(i);
          }
        }
        return (function() {
          var _k, _len2, _results;
          _results = [];
          for (_k = 0, _len2 = tmp.length; _k < _len2; _k++) {
            i = tmp[_k];
            if (!i.isResult) {
              _results.push(i);
            }
          }
          return _results;
        })();
      }).property('model.stepsArr'),
      nextStepDisabled: (function() {
        if (this.get('currStepNum') + 1 === this.get('maxStepNum')) {
          return true;
        }
        return false;
      }).property('currStepNum'),
      previousStepDisabled: (function() {
        if (this.get('currStepNum') - 1 < 0) {
          return true;
        }
        return false;
      }).property('currStepNum'),
      maxStepNum: (function() {
        return this.get('displayArr.length');
      }).property('displayArr'),
      stepNumRange: (function() {
        var arr, curr, i, res, tmp, _i, _ref;
        res = [];
        curr = this.get('currStepNum');
        arr = this.get('displayArr');
        for (i = _i = 1, _ref = this.get('displayArr.length'); 1 <= _ref ? _i <= _ref : _i >= _ref; i = 1 <= _ref ? ++_i : --_i) {
          tmp = {};
          tmp.index = i;
          tmp.selected = i === 1 + curr;
          tmp["class"] = this.sideClassPicker(arr[i - 1]._sideCnt);
          res.pushObject(tmp);
        }
        return res;
      }).property('displayArr', 'currStepNum'),
      sideClassPicker: function(arg) {
        if (arg !== 0) {
          return "side-step-" + ((arg - 1) % ENV.nrOfSideColors);
        } else {
          return "main-step";
        }
      },
      currStep: (function() {
        return this.get('displayArr')[this.get('currStepNum')];
      }).property('displayArr', 'currStepNum'),
      keysEnabled: false,
      initKeys: (function() {
        return this.set('keysEnabled', true);
      }).on('init'),
      keysEnabledWatch: (function() {
        if (this.get("keysEnabled")) {
          return Ember.$(document).bind('keydown.quickView', (function(_this) {
            return function(evt) {
              switch (evt.keyCode) {
                case 37:
                  return _this.send("previousStep");
                case 39:
                  return _this.send("nextStep");
              }
            };
          })(this));
        } else {
          return Ember.$(document).unbind('keydown.quickView');
        }
      }).observes("keysEnabled"),
      actions: {
        disableKeys: function() {
          return this.set("keysEnabled", false);
        },
        nextStep: function() {
          if (this.get("nextStepDisabled")) {
            return;
          }
          return this.set('currStepNum', this.get('currStepNum') + 1);
        },
        previousStep: function() {
          if (this.get("previousStepDisabled")) {
            return;
          }
          return this.set('currStepNum', this.get('currStepNum') - 1);
        },
        setStepNum: function(index) {
          this.set("keysEnabled", true);
          return this.set('currStepNum', index - 1);
        }
      }
    });

    __exports__["default"] = QuickViewController;;
  });
define("v2/controllers/recipe", 
  ["v2/utils/ajax-resolver","v2/utils/recipe-utils","v2/utils/recipe-request-resolver","v2/models/timer","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var ajax = __dependency1__["default"];

    var recipeModel = __dependency2__["default"];

    var RecipeRequestController = __dependency3__["default"];

    var Timer = __dependency4__["default"];

    var RecipeController;

    RecipeController = Ember.ObjectController.extend({
      queryParams: ["activeStyle", "flip", "unit"],
      needs: ["application"],
      timerArray: [],
      recipe: Ember.computed.alias("content"),
      rerenderSwitch: 1,
      viewOnly: true,
      embedLink: "http://gracipe-test1.s3.amazonaws.com/recipe_preview/testinggracipecomrecipe",
      embedToggle: false,
      activeStyle: "arrow",
      flip: "none",
      unit: "author",
      imageModalSrc: "",
      experimentSidebarActive: (function() {
        if (window.mobilecheck()) {
          return false;
        }
        return +sessionStorage["sUKiVQckRUG1ZM4wK7dOXg-varID"] === 1;
      }).property(""),
      allowEdit: (function() {
        var u;
        u = this.get("controllers.application.user");
        return u.get("id") === this.get("model.user") || u.get("isAdmin");
      }).property("controllers.application.user.id", "controllers.application.user.secretAT", "model.user"),
      randoms: (function() {
        var rndCtrl;
        rndCtrl = new RecipeRequestController();
        rndCtrl.opt = {
          random: 1
        };
        rndCtrl.getNext();
        return rndCtrl.results;
      }).property("randomWatch"),
      pubSteps: function() {
        return this.get("model.stepsArr");
      },
      clearTimers: (function() {
        return this.set('timerArray', []);
      }).observes('currentPath'),
      isUnitMetric: Ember.computed("unit", function() {
        return this.get("unit") === "metric";
      }),
      isUnitUS: Ember.computed("unit", function() {
        return this.get("unit") === "us";
      }),
      actions: {
        setUnit: function(u) {
          var allConversion, selectedConversion;
          allConversion = this.get("recipe.conversion.availConv");
          selectedConversion = allConversion.filter(function(i) {
            return i["id"] === u;
          });
          if (selectedConversion.length > 0) {
            this.set("recipe.conversion.currConv", selectedConversion[0]);
          }
          return this.set("unit", u);
        },
        changeServingSize: function(num) {
          num = +num;
          return this.set("recipe.servings", Math.max(this.get("recipe.servings") + num, 1));
        },
        addTimer: function(time, text, stepNum, recipe) {
          var i, match, timer, _i, _len, _ref;
          _ref = this.get('timerArray');
          for (_i = 0, _len = _ref.length; _i < _len; _i++) {
            i = _ref[_i];
            if (i.stepNumber === stepNum) {
              console.log('Timer already spawned');
              i.startToggle();
              return;
            }
          }
          match = time.match(/\d+\.?\d*/);
          if (match === null) {
            time = 5;
          } else {
            time = +match;
          }
          timer = new Timer;
          timer.set('recipe', recipe);
          timer.initTimer(60 * time);
          timer.set('stepNumber', stepNum);
          if (text === void 0) {
            timer.set('stepTitle', '');
          } else {
            timer.set('stepTitle', String(text));
          }
          this.get('timerArray').pushObject(timer);
          if (match !== null) {
            return timer.runFirst();
          }
        },
        setBookmark: function(prm) {
          if (!this.get("controllers.application.loggedIn")) {
            this.get("controllers.application").send("doLogin");
            return;
          }
          return ajax.setBookmark({
            "recipeId": this.get("model.recipeId"),
            "setBookmark": prm
          }, (function(_this) {
            return function(data) {
              var tmpIncDecBookmark, tmpJustBookmarked;
              if (data) {
                tmpJustBookmarked = !!+data;
                tmpIncDecBookmark = _this.get("model.bookmarkCnt");
                _this.set("model.isLiked", tmpJustBookmarked);
                tmpIncDecBookmark += tmpJustBookmarked ? 1 : -1;
                return _this.set("model.bookmarkCnt", tmpIncDecBookmark);
              }
            };
          })(this));
        },
        randomOther: function() {
          return this.set("randomWatch", (new Date).getTime());
        },
        onEmbed: function() {
          var text;
          text = "<a href='" + $(location).attr('href') + "'>";
          text += "<img src='http://" + $(location).attr('host') + '/embed?id=' + this.get("model.id") + "' alt='" + this.get("model.title") + "'/></a>";
          this.set("embedText", text);
          this.set("embedToggle", !this.get("embedToggle"));
          return setTimeout(function() {
            return $("#embed-link").select();
          }, 500);
        },
        showModalImage: function(src) {
          return this.set("imageModalSrc", src);
        }
      }
    });

    __exports__["default"] = RecipeController;;
  });
define("v2/controllers/recipe_edit", 
  ["v2/utils/recipe-editor-search-resolver","v2/config/environment","v2/utils/ajax-resolver","v2/utils/recipe-utils","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var nodeDis = __dependency1__["default"];

    var V2ENV = __dependency2__["default"];

    var ajax = __dependency3__["default"];

    var recipeModel = __dependency4__["default"];

    var RecipeController, RecipeWatchdog, uploadFiles;

    window.GQuickAdd = [
