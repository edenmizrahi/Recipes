    "use strict";
    var Ember = __dependency1__["default"];
    var CharlRoute;

    CharlRoute = Ember.Route.extend({
      beforeModel: function(t, qp) {
        localStorage.clear();
        if (!t.queryParams.track) {
          localStorage.noGA = true;
        }
        return this.transitionTo('index');
      }
    });

    __exports__["default"] = CharlRoute;
  });
define("v2/routes/embed", 
  ["ember","v2/utils/recipe-utils","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var recipes = __dependency2__["default"];

    var RecipeUrlForwardRoute;

    RecipeUrlForwardRoute = Ember.Route.extend({
      model: function(p) {
        return recipes.findBySlug(p.gracipe_slug);
      },
      afterModel: function() {
        var appCtrl;
        appCtrl = this.controllerFor("application");
        return appCtrl.set("hideMenu", true);
      }
    });

    __exports__["default"] = RecipeUrlForwardRoute;
