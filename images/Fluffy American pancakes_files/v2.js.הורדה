define("v2/mixins/all-ingred", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var AllIngredMxn;

    AllIngredMxn = Ember.Mixin.create({
      cats: ["veg_fruit", "meat_egg", "grains", "milk", "spices", "other"],
      catsDisplay: ["Vegetables & Fruits", "Meat & Eggs", "Grains", "Dairy", "Spices", "Others"],
      typeID: 1,
      allowUpdate: true,
      intervalCleanup: Ember.on("willDestroy", function() {
        return clearInterval(this.get("refreshIntervalId"));
      }),
      intervalUpdate: Ember.on("init", function() {
        var calcUpdateHash, refreshIntervalId;
        calcUpdateHash = function(arr) {
          var arrSorted, arrTrans, x;
          arrSorted = arr.sortBy("id");
          arrTrans = (function() {
            var _i, _len, _results;
            _results = [];
            for (_i = 0, _len = arrSorted.length; _i < _len; _i++) {
              x = arrSorted[_i];
              _results.push(x.get("id") + x.get("svgObj.type") + x.get("type"));
            }
            return _results;
          })();
          return arrTrans.join(" ");
        };
        refreshIntervalId = setInterval((function(_this) {
          return function() {
            var tmpHash;
            if (!_this.get("allowUpdate")) {
              return;
            }
            _this.get("controller").notifyPropertyChange("updateHelperItems");
            tmpHash = _this.get("hashUpdateCheck");
            if (!tmpHash || tmpHash !== calcUpdateHash(_this.get("combinedModels"))) {
              _this.get("controller").notifyPropertyChange("updateAccu");
              return _this.set("hashUpdateCheck", calcUpdateHash(_this.get("combinedModels")));
            }
          };
        })(this), 1500);
        return this.set("refreshIntervalId", refreshIntervalId);
      }),
      accu: (function() {
        this.get("allIngred");
        return this.get("tmpAccu");
      }).property("controller.updateAccu"),
      combinedModels: (function() {
        this.get("allIngred");
        return this.get("tmpCombinedModels");
      }).property("controller.updateHelperItems"),
      allIngred: (function() {
        var accu, allIngredArr, cleanedUpModel, combineAlgorithm, combinedModels, copyNewName, copyWithNewAttributes, getAllIngred, key, keyToObj, retArr, step, stringToProperCase, tmpAccu, tmpDisplay, x, _i, _j, _len, _len1, _ref, _ref1;
        if (!this.item) {
          return [];
        }
        getAllIngred = (function(_this) {
          return function(step) {
            var retArr, _i, _len, _ref;
            if (!step) {
              return;
            }
            retArr = [];
            if (!step.isResult) {
              if (step.svgObj.type) {
                if (step && +step.svgObj.type === _this.get("typeID")) {
                  retArr.push(step);
                }
              } else {
                if (step && +step.type === _this.get("typeID")) {
                  retArr.push(step);
                }
              }
            }
            if (step.subs === void 0) {
              return retArr;
            }
            _ref = step.subs;
            for (_i = 0, _len = _ref.length; _i < _len; _i++) {
              step = _ref[_i];
              retArr = retArr.concat(getAllIngred(step));
            }
            return retArr;
          };
        })(this);
        stringToProperCase = function(s) {
          var tmp;
          if (!s) {
            s = "";
          }
          tmp = s.trim();
          return tmp.replace(/\w\S*/g, function(txt) {
            return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
          });
        };
        copyNewName = function(x, _this) {
          var recipe, tmp, _ref, _ref1, _ref2;
          tmp = {
            id: x.id,
            name: stringToProperCase(x.name),
            amount: (_ref = x.amount) != null ? _ref : 0,
            unit: ((_ref1 = x.unit) != null ? _ref1 : "").trim().toLowerCase(),
            type: (x != null ? (_ref2 = x.svgObj) != null ? _ref2.el_type : void 0 : void 0) === void 0 || x.svgObj.el_type.trim() === "" ? "other" : x.svgObj.el_type,
            ref: x
          };
          recipe = _this.get("controller.recipe") || _this.get("recipe");
          if (recipe.get("overriddenIngredConfig")) {
            if (recipe.get("overriddenIngredConfig." + x.id)) {
              tmp["type"] = recipe.get("overriddenIngredConfig." + x.id);
            }
          }
          return tmp;
        };
        keyToObj = function(k) {
          var m;
          m = k.match(/(\d+);(.*)/);
          return {
            id: m[1],
            name: m[2]
          };
        };
        copyWithNewAttributes = function(ingredRef, x) {
          var clone, k2, v2;
          clone = ingredRef.ref.cloneWithSameSVG();
          for (k2 in x) {
            v2 = x[k2];
            clone.set(k2, v2);
          }
          return clone;
        };
        combineAlgorithm = function(cleanedUpModel) {
          var accu, groupByName, groupByNameAndUnit, hasPushed, k, k1, tmpIngredRef, tmpResult, tmpX, v, v1, _ref, _ref1;
          accu = [];
          groupByName = _.groupBy(cleanedUpModel, function(x) {
            return x.id + ";" + x.name;
          });
          for (k in groupByName) {
            v = groupByName[k];
            groupByNameAndUnit = _.groupBy(v, function(x) {
              return x.unit;
            });
            hasPushed = false;
            for (k1 in groupByNameAndUnit) {
              v1 = groupByNameAndUnit[k1];
              tmpResult = _.reduce(v1, (function(x, y) {
                return {
                  amount: +x.amount + +y.amount
                };
              }), {
                amount: 0
              });
              if (tmpResult.amount > 0) {
                hasPushed = true;
                tmpIngredRef = v[0];
                tmpX = {
                  name: keyToObj(k).name,
                  amount: tmpResult.amount,
                  unit: k1,
                  id: keyToObj(k).id,
                  type: (_ref = v[0].type) != null ? _ref : "other"
                };
                accu.push(copyWithNewAttributes(tmpIngredRef, tmpX));
              }
            }
            if (!hasPushed) {
              accu.push;
              tmpIngredRef = v[0];
              tmpX = {
                name: keyToObj(k).name,
                amount: void 0,
                unit: void 0,
                id: keyToObj(k).id,
                type: (_ref1 = v[0].type) != null ? _ref1 : "other"
              };
              accu.push(copyWithNewAttributes(tmpIngredRef, tmpX));
            }
          }
          return accu;
        };
        allIngredArr = [];
        _ref = this.item;
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          step = _ref[_i];
          allIngredArr = allIngredArr.concat(getAllIngred(step));
        }
        cleanedUpModel = (function() {
          var _j, _len1, _results;
          _results = [];
          for (_j = 0, _len1 = allIngredArr.length; _j < _len1; _j++) {
            x = allIngredArr[_j];
            _results.push(copyNewName(x, this));
          }
          return _results;
        }).call(this);
        combinedModels = combineAlgorithm(cleanedUpModel);
        this.set("tmpCombinedModels", combinedModels);
        accu = _.groupBy(combinedModels, function(x) {
          return x.type;
        });
        retArr = [];
        tmpAccu = [];
        _ref1 = this.get("cats");
        for (_j = 0, _len1 = _ref1.length; _j < _len1; _j++) {
          key = _ref1[_j];
          if (accu[key]) {
            retArr = retArr.concat(accu[key]);
            tmpDisplay = this.get("catsDisplay")[this.get("cats").indexOf(key)];
            tmpAccu.push({
              key: key,
              display: tmpDisplay,
              val: accu[key]
            });
          }
        }
        this.set("tmpAccu", tmpAccu);
        return retArr;
      }).property("controller.updateHelperItems")
    });

    __exports__["default"] = AllIngredMxn;
  });
define("v2/mixins/bottom-reload", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var BottomReloadMixin;

    BottomReloadMixin = Ember.Mixin.create({
      isLoading: (function() {
        return this.get("controller.rrc.status.isLoading");
      }).property("controller.rrc.status.isLoading"),
      setScroll: (function() {
        return $(window).bind('scroll touchmove', (function(_this) {
          return function() {
            var tmpRatio, _ref;
            if (window.mobilecheck()) {
              tmpRatio = window.devicePixelRatio;
            } else {
              tmpRatio = 1;
            }
            if ($(window).scrollTop() + $(window).height() * tmpRatio > 0.9 * $(document).height()) {
              console.log("bottom reached");
              return (_ref = _this.get("controller.rrc")) != null ? _ref.getNext() : void 0;
            }
          };
        })(this));
      }).on('didInsertElement'),
      removeScroll: (function() {
        $(window).unbind('scroll touchmove');
        return 1;
      }).on("willDestroyElement")
    });

    __exports__["default"] = BottomReloadMixin;
  });
define("v2/mixins/freewall", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ACTIVE, FreewallMixin;

    ACTIVE = false;

    FreewallMixin = Ember.Mixin.create({
      fitTiles: function() {
        return setTimeout((function(_this) {
          return function() {
            return _this.wall.fitWidth();
          };
        })(this), 100);
      },
      itemRenderedWatch: (function() {
        if (!ACTIVE) {
          return;
        }
        if (window.mobilecheck()) {
          return;
        }
        return this.fitTiles();
      }).observes("controller.itemRendered"),
      loadWall: (function() {
        if (!ACTIVE) {
          return;
        }
        if (window.mobilecheck()) {
          return;
        }
        this.wall = new freewall("#container");
        return this.wall.reset({
          selector: '.dummy',
          animate: true,
          cellW: 275,
          cellH: 'auto',
          onResize: (function(_this) {
            return function() {
              return _this.wall.fitWidth();
            };
          })(this)
        });
      }).on('didInsertElement')
    });

    __exports__["default"] = FreewallMixin;
  });
define("v2/mixins/json-serializer", 
  ["exports"],
  function(__exports__) {
    "use strict";
    var m;

    m = Ember.Mixin.create({
      serialize: function() {
        var key, result;
        result = {};
        for (key in $.extend(true, {}, this)) {
          if (key === "isInstance" || key === "isDestroyed" || key === "isDestroying" || key === "concatenatedProperties" || typeof this[key] === "function") {
            continue;
          }
          result[key] = this[key];
        }
        return result;
      }
    });

    __exports__["default"] = m;;
  });
