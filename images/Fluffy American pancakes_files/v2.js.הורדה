define("v2/views/style/attr-view", 
  ["v2/models/svg-element","v2/mixins/template-switcher","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var svgElement = __dependency1__["default"];

    var TemplateSwitchMxn = __dependency2__["default"];

    var AttributeView;

    AttributeView = Em.View.extend(TemplateSwitchMxn, {
      templateId: "attr-view",
      classNames: ["attrView"],
      classNameBindings: ['isActive:isActive'],
      attributeBindings: ['title'],
      title: (function() {
        var _ref;
        return (_ref = this.get("tgtObject")) != null ? _ref.label : void 0;
      }).property("tgtObject"),
      svgView: (function() {
        var e;
        try {
          return this.get("tgtObject").svg;
        } catch (_error) {
          e = _error;
          return "";
        }
      }).property("key"),
      tgtObject: (function() {
        if (this.get("attr")) {
          return this.get("attr");
        }
        if (this.get("key")) {
          return window.GAttributes[this.get("key")];
        }
      }).property("key", "attr"),
      isActive: (function() {
        var i, lastItem, o, _i, _len, _ref;
        o = this.get("tgtObject");
        lastItem = null;
        if (this.get("attributeLst") == null) {
          return null;
        }
        _ref = this.get("attributeLst");
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          i = _ref[_i];
          if (o.attr === i.attr) {
            lastItem = i;
          }
        }
        return lastItem;
      }).property("attributeLst.@each", "tgtObject"),
      toggleAttr: function(o) {
        var lastItem, oref, tmpSvgEl;
        oref = this.get("tgtObject");
        o = JSON.parse(JSON.stringify(oref));
        if (this.get("attributeLst") == null) {
          this.set("attributeLst", []);
        }
        lastItem = this.get("isActive");
        if (lastItem) {
          return this.get("attributeLst").removeObject(lastItem);
        } else {
          tmpSvgEl = svgElement.create();
          tmpSvgEl.set("id", o.attr);
          o.svgObj = tmpSvgEl;
          return this.get("attributeLst").addObject(o);
        }
      },
      click: function() {
        return this.toggleAttr();
      }
    });

    __exports__["default"] = AttributeView;;
  });
define("v2/views/style/attribute-small", 
  ["ember","v2/mixins/template-switcher","v2/models/svg-element","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var TemplateSwitchMxn = __dependency2__["default"];

    var svgElement = __dependency3__["default"];

    var RecipeIngredientAttributeSmallView, Timer, addTimer;

    addTimer = function(div, min) {
      var MIN, SEC, arc, arcTween, circ, get, height, isRunning, radius, svg, tick, width;
      MIN = min;
      width = 28;
      height = width;
      SEC = MIN * 60;
      radius = Math.min(width, height) / 2;
      isRunning = true;
      arc = d3.svg.arc().startAngle(0).startAngle(function(d) {
        if (!d.isH) {
          return 0;
        }
        if (d.value > 0) {
          return d.value * 2 * Math.PI - 0.1;
        }
      }).endAngle(function(d) {
        if (!d.isH) {
          return d.value * 2 * Math.PI;
        }
        if (d.value > 0) {
          return d.value * 2 * Math.PI + 0.1;
        }
      }).innerRadius(function(d) {
        return d.r * radius;
      }).outerRadius(function(d) {
        return d.w * radius;
      });
      arcTween = function(d) {
        var i;
        i = d3.interpolateNumber(d.pre, d.value);
        return function(t) {
          d.value = i(t);
          return arc(d);
        };
      };
      get = function() {
        var getH, getM, getS, now, tmp;
        getM = function(s) {
          return Math.floor(SEC / 60);
        };
        getS = function(s) {
          return SEC % 60;
        };
        getH = function(s) {
          return Math.floor(SEC / 3600);
        };
        now = new Date;
        return tmp = [
          {
            r: 0.86,
            value: 1 - 0.00001,
            w: 0.9,
            f: "#999"
          }, {
            r: 0.0,
            value: getM(SEC) / 60 - 0.00001,
            w: 0.85,
            f: "#111"
          }, {
            r: 0.0,
            value: getH(SEC) / 12 - 0.00001,
            w: 0.56,
            f: "#666",
            isH: true
          }
        ];
      };
      svg = d3.select(div).append("svg").attr("width", width).attr("height", height).on("click", function() {
        isRunning = !isRunning;
        return;
        return svg.style("background-color", "green");
      });
      circ = svg.append("g").attr("transform", "translate(" + (width / 2 - 1) + "," + (height / 2 + 3) + ")").selectAll("path").data(get()).enter().append("path").attr("d", arc).style("fill", function(d) {
        return d.f;
      });
      tick = function() {
        console.log;
        if (isRunning && false) {
          circ.data(get());
          circ.attr("d", arc);
          circ.style("fill", function(d) {
            return d.f;
          });
          SEC -= 1;
        }
        return setTimeout(tick, 1000 - Date.now() % 1000);
      };
      return tick();
    };

    Timer = function(container, view) {
      this.view = view;
      this.container = container;
      this.innerContainer = null;
      this.timerContainer = {};
      this.timerStatus = {};
      this.timer = {};
      this.open = false;
      this.config = {
        animation: "smooth",
        bg_width: 0.0001,
        fg_width: 0.03,
        count_past_zero: false,
        circle_bg_color: "#FFF",
        time: {
          value: (this.view.get("attribute.prm") * 60) + 1,
          Days: {
            show: false
          },
          Hours: {
            text: "Hours",
            color: "#FFF",
            show: false
          },
          Minutes: {
            text: "Minutes",
            color: "#FFF",
            show: false
          },
          Seconds: {
            text: "Seconds",
            color: "#FFF",
            show: true
          }
        },
        css: {
          open: {
            "height": "120px",
            "margin-top": "-55px"
          },
          close: {
            "height": "0",
            "margin-top": "0"
          }
        }
      };
      this.spawn();
      this.events();
    };

    Timer.prototype = {
      spawn: function() {
        var inner;
        inner = this.view.$().parent();
        inner.parent().addClass("hasTimer");
        inner.before("<div class='timerToggle'>Start</div>");
        this.view.$().after("<div class='timer'> <div class='timerContainer'></div> <div class='blinkingDot blink'></div> <div class='timerMenu'> <p class='addMin'>+1’</p> <span class='glyphicon glyphicon-repeat timerRestart' aria-hidden='true'></span> </div> </div>");
        this.innerContainer = this.container.find(".timer");
        this.timerContainer = this.container.find(".timerContainer");
        this.timerStatus = inner.parent().find(".timerToggle");
        return this.center(inner);
      },
      events: function() {
        this.container.parent().find(".timerToggle").click((function(_this) {
          return function() {
            if (!_this.isOpen()) {
              return _this.build(_this.config.time.value);
            } else {
              if (_this.isOn()) {
                return _this.stop();
              } else {
                return _this.start();
              }
            }
          };
        })(this));
        this.container.find(".timerRestart").click((function(_this) {
          return function() {
            return _this.restart();
          };
        })(this));
        return this.container.find(".addMin").click((function(_this) {
          return function() {
            return _this.addTime(61);
          };
        })(this));
      },
      build: function(secounds) {
        this.innerContainer.css(this.config.css.open);
        this.timerContainer.empty().append("<div id='DateCountdown' data-timer='" + secounds + "' style='width: 100%;height: 100%;'></div>");
        this.timer = this.container.find("#DateCountdown");
        this.timer.TimeCircles(this.config).addListener((function(_this) {
          return function(unit, amount, total) {
            _this.tick(unit, amount, total, _this);
          };
        })(this));
        this.tick(null, null, secounds, this);
        this.open = true;
        return this.timerStatus.html("Pause");
      },
      center: function(container) {
        var height, pHeight, statusHeight;
        pHeight = parseInt(container.parent().css("height"), 10);
        height = parseInt(container.css("height"), 10);
        statusHeight = parseInt(this.timerStatus.css("height"), 10);
        if (pHeight < height) {
          return container.parent().css("height", height + statusHeight);
        }
      },
      tick: function(unit, amount, total, instance) {
        instance.config.time.Seconds.show = false;
        instance.config.time.Minutes.show = false;
        instance.config.time.Hours.show = false;
        if (total < 60) {
          instance.config.time.Seconds.show = true;
        } else if (total < 3600) {
          instance.config.time.Minutes.show = true;
        } else {
          instance.config.time.Hours.show = true;
        }
        instance.timer.TimeCircles(this.config).rebuild();
        if (total === 0) {
          instance.timer.TimeCircles().destroy();
          return instance.timerContainer.empty().append("<p class='done'>DONE!</p>");
        }
      },
      isOpen: function() {
        return this.open;
      },
      isOn: function() {
        return this.timerStatus.html() === "Pause";
      },
      isDone: function() {
        return this.timerContainer.find('.done').length > 0;
      },
      destroy: function() {
        this.innerContainer.css(this.config.css.close);
        this.open = false;
        return this.timer.TimeCircles().destroy();
      },
      stop: function() {
        this.timerStatus.html("Start");
        this.timer.TimeCircles().stop();
        return this.container.find(".blinkingDot").removeClass("blink");
      },
      start: function() {
        this.timerStatus.html("Pause");
        this.timer.TimeCircles().start();
        return this.container.find(".blinkingDot").addClass("blink");
      },
      restart: function() {
        this.timer.TimeCircles().destroy();
        this.build(this.config.time.value);
        this.tick(null, null, this.timer.TimeCircles().getTime(), this);
        return this.stop();
      },
      addTime: function(time) {
        if (this.isDone()) {
          this.build(time);
        } else {
          this.timer.TimeCircles().addTime(time);
        }
        return this.tick(null, null, this.timer.TimeCircles().getTime(), this);
      }
    };

    RecipeIngredientAttributeSmallView = Ember.View.extend(TemplateSwitchMxn, {
      templateId: "attribute-small",
      classNames: ['rAttr_elSmall', 'attrib'],
      enableTimer: true,
      enableAnimatedTimers: true,
      didInsertElement: (function() {
        var timer, tmpEl, tmpID;
        if (this.get("isTimer")) {
          if (this.get("controller.activeStyle") !== "block") {
            return false;
          }
          if (this.get("controller.viewOnly")) {
            timer = new Timer(this.$().parent(), this);
            this.$().click(function() {
              if (!timer.isOpen()) {
                return timer.build(timer.config.time.value);
              } else {
                if (timer.isOn()) {
                  return timer.stop();
                } else {
                  return timer.start();
                }
              }
            });
          }
          if (+this.get("attribute.prm") < 60) {
            this.$().find(".pic > svg").remove();
            tmpEl = this.$().children(".pic");
            tmpID = "temptimer" + (new Date()).getTime() + Math.floor(Math.random() * 10000);
            tmpEl.attr("id", tmpID);
            return addTimer("#" + tmpID, +this.get("attribute.prm"));
          }
        }
      }),
      isTimer: (function() {
        return +this.get("attribute.id") === 26;
      }).property("attribute"),
      calcPrm: (function(k, v, ov) {
        var tmpCelsius, tmpFahrenheit;
        if (arguments.length !== 1) {
          this.set("attribute.prm", v);
        }
        this.set("attribute.prm", "" + this.attribute.prm);
        if (+this.attribute.attr === 290 && this.attribute.prm.match(/^(\d+)(\s*°C)?$/)) {
          tmpCelsius = this.attribute.prm.match(/(\d+)/)[1];
          if (this.get("controller.content.conversion.currConv.id") === "us") {
            console.log("asdofaosdfoasodfoasdfo");
            return (tmpCelsius * 1.8 + 32).toFixed(0) + "°F";
          } else {
            return tmpCelsius + "°C";
          }
        }
        if (+this.attribute.attr === 446 && this.attribute.prm.match(/^(\d+)(\s*°F)?$/)) {
          tmpFahrenheit = this.attribute.prm.match(/(\d+)/)[1];
          if (this.get("controller.content.conversion.currConv.id") === "metric") {
            return ((tmpFahrenheit - 32) / 1.8).toFixed(0) + "°C";
          } else {
            return tmpFahrenheit + "°F";
          }
        }
        if (+this.attribute.attr === 26) {
          if (this.attribute.prm.match(/^(\s*\d+(\s*-\s*\d+\s*)?$)/)) {
            return this.attribute.prm + ' min';
          }
        }
        return this.attribute.prm;
      }).property("controller.content.conversion.currConv", "attribute.prm"),
      timerDoneStatic: (function() {
        return '<svg version="1.0" xmlns="http://www.w3.org/2000/svg" width="980.000000pt" height="1179.000000pt" viewBox="0 0 980.000000 1179.000000" preserveAspectRatio="xMidYMid meet"> <metadata> Created by potrace 1.10, written by Peter Selinger 2001-2011 </metadata> <g transform="translate(0.000000,1179.000000) scale(0.100000,-0.100000)" stroke="none"> <path d="M4102 11764 c-57 -28 -97 -73 -118 -131 -17 -50 -20 -1129 -3 -1198 14 -54 67 -118 123 -148 37 -20 59 -22 234 -25 l192 -4 -2 -236 -3 -237 -160 -17 c-770 -84 -1496 -343 -2144 -765 -95 -62 -338 -240 -447 -328 l-24 -19 -60 59 -60 59 94 96 c103 104 124 146 111 217 -6 33 -44 75 -288 323 -155 157 -295 295 -311 307 -39 30 -89 38 -139 23 -35 -10 -98 -68 -401 -368 -200 -200 -364 -371 -372 -389 -18 -42 -18 -107 0 -141 22 -41 587 -600 619 -612 36 -13 88 -13 123 0 15 6 70 52 121 102 l93 92 55 -54 55 -54 -68 -71 c-165 -169 -367 -425 -525 -666 -752 -1147 -985 -2556 -645 -3894 386 -1521 1499 -2777 2968 -3350 1020 -397 2165 -440 3210 -119 1271 390 2328 1279 2934 2468 265 519 430 1069 503 1681 14 120 18 227 18 535 0 411 -8 524 -60 826 -211 1229 -883 2331 -1880 3085 -524 396 -1153 695 -1787 848 -224 54 -505 101 -698 116 -41 3 -90 8 -107 11 l-33 5 0 234 0 235 168 0 c194 0 246 11 309 65 80 68 78 52 78 710 0 577 0 580 -22 621 -29 54 -93 110 -140 123 -25 7 -278 11 -749 11 l-710 0 -52 -26z m1298 -2728 c859 -113 1611 -451 2243 -1006 709 -623 1181 -1454 1351 -2380 50 -274 61 -401 61 -750 0 -349 -11 -476 -61 -750 -308 -1675 -1608 -2998 -3276 -3334 -602 -121 -1241 -106 -1839 44 -1173 295 -2164 1092 -2708 2178 -228 455 -370 949 -423 1472 -16 161 -16 619 0 780 92 909 453 1729 1058 2401 688 766 1641 1247 2678 1354 184 19 746 14 916 -9z" transform="rotate(227.15 4850 4905)"><animateTransform attributeType="xml" attributeName="transform" type="rotate" from="360 4850 4905" to="0 4850 4905" dur="0.25s" repeatCount="indefinite"></animateTransform></path> <path d="M4850 8620 l0 -220 45 0 45 0 0 220 0 220 -45 0 -45 0 0 -220z"></path> <path d="M2930 8319 c-19 -12 -37 -23 -38 -24 -3 -3 161 -293 205 -363 l14 -23 37 22 c20 12 38 23 40 25 4 2 -205 365 -216 376 -4 4 -23 -2 -42 -13z"></path> <path d="M6714 8144 c-114 -198 -111 -187 -54 -218 l26 -14 104 180 c58 100 106 187 108 194 3 12 -45 44 -67 44 -5 0 -58 -84 -117 -186z"></path> <path d="M4827 8093 l-65 -128 -31 -1130 c-17 -621 -33 -1198 -37 -1282 l-6 -152 -65 -38 c-289 -168 -351 -573 -126 -823 237 -264 660 -227 855 74 109 168 109 404 0 572 -46 71 -137 156 -202 189 l-47 24 -6 188 c-3 103 -18 681 -33 1283 l-27 1095 -64 128 c-36 70 -68 127 -72 127 -5 0 -38 -57 -74 -127z m159 -3048 c48 -28 84 -91 84 -145 0 -54 -36 -117 -84 -145 -49 -29 -121 -33 -165 -10 -70 35 -110 123 -91 195 30 113 155 164 256 105z"></path> <path d="M1490 6878 c-11 -18 -20 -36 -20 -39 0 -6 363 -219 373 -219 3 0 15 16 28 36 18 30 19 38 8 45 -38 25 -359 209 -364 209 -3 0 -14 -15 -25 -32z"></path> <path d="M8100 6809 c-96 -56 -179 -105 -184 -110 -12 -11 25 -81 40 -77 6 2 92 50 191 108 l181 104 -19 38 c-11 21 -23 38 -27 38 -4 0 -86 -45 -182 -101z"></path> <path d="M960 4905 l0 -45 220 0 220 0 0 45 0 45 -220 0 -220 0 0 -45z"></path> <path d="M8390 4905 l0 -45 220 0 220 0 0 45 0 45 -220 0 -220 0 0 -45z"></path> <path d="M1666 3094 c-99 -57 -184 -108 -189 -113 -6 -6 -1 -23 13 -45 l22 -36 57 32 c254 145 321 184 321 188 0 3 -10 21 -23 41 l-22 37 -179 -104z"></path> <path d="M7925 3155 c-11 -19 -17 -36 -15 -39 3 -2 88 -52 189 -111 140 -81 186 -103 193 -93 5 7 15 25 23 40 l15 26 -184 106 c-101 58 -188 106 -193 106 -5 0 -18 -16 -28 -35z"></path> <path d="M3097 1878 c-8 -13 -49 -84 -92 -158 -42 -74 -86 -150 -97 -167 l-19 -32 33 -21 c18 -11 38 -20 43 -20 6 0 44 60 85 133 41 72 89 157 107 188 l32 56 -39 22 -39 22 -14 -23z"></path> <path d="M6647 1881 c-15 -10 -30 -20 -33 -24 -5 -5 186 -347 207 -371 9 -10 79 23 79 37 0 12 -213 377 -220 376 -3 0 -17 -8 -33 -18z"></path> <path d="M4850 1190 l0 -220 45 0 45 0 0 220 0 220 -45 0 -45 0 0 -220z"></path> </g> </svg>';
      }).property(),
      animatedTimerStatic: (function() {
        return '<svg version="1.0" id="svgTimerTest" xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 980.000000 1179.000000" preserveAspectRatio="xMidYMid meet"> <metadata> Created by potrace 1.10, written by Peter Selinger 2001-2011 </metadata> <g transform="translate(0.000000,1179.000000) scale(0.100000,-0.100000)"  stroke="none"> <path d="M4102 11764 c-57 -28 -97 -73 -118 -131 -17 -50 -20 -1129 -3 -1198 14 -54 67 -118 123 -148 37 -20 59 -22 234 -25 l192 -4 -2 -236 -3 -237 -160 -17 c-770 -84 -1496 -343 -2144 -765 -95 -62 -338 -240 -447 -328 l-24 -19 -60 59 -60 59 94 96 c103 104 124 146 111 217 -6 33 -44 75 -288 323 -155 157 -295 295 -311 307 -39 30 -89 38 -139 23 -35 -10 -98 -68 -401 -368 -200 -200 -364 -371 -372 -389 -18 -42 -18 -107 0 -141 22 -41 587 -600 619 -612 36 -13 88 -13 123 0 15 6 70 52 121 102 l93 92 55 -54 55 -54 -68 -71 c-165 -169 -367 -425 -525 -666 -752 -1147 -985 -2556 -645 -3894 386 -1521 1499 -2777 2968 -3350 1020 -397 2165 -440 3210 -119 1271 390 2328 1279 2934 2468 265 519 430 1069 503 1681 14 120 18 227 18 535 0 411 -8 524 -60 826 -211 1229 -883 2331 -1880 3085 -524 396 -1153 695 -1787 848 -224 54 -505 101 -698 116 -41 3 -90 8 -107 11 l-33 5 0 234 0 235 168 0 c194 0 246 11 309 65 80 68 78 52 78 710 0 577 0 580 -22 621 -29 54 -93 110 -140 123 -25 7 -278 11 -749 11 l-710 0 -52 -26z m1298 -2728 c859 -113 1611 -451 2243 -1006 709 -623 1181 -1454 1351 -2380 50 -274 61 -401 61 -750 0 -349 -11 -476 -61 -750 -308 -1675 -1608 -2998 -3276 -3334 -602 -121 -1241 -106 -1839 44 -1173 295 -2164 1092 -2708 2178 -228 455 -370 949 -423 1472 -16 161 -16 619 0 780 92 909 453 1729 1058 2401 688 766 1641 1247 2678 1354 184 19 746 14 916 -9z"></path> <path d="M4850 8620 l0 -220 45 0 45 0 0 220 0 220 -45 0 -45 0 0 -220z"></path> <path d="M2930 8319 c-19 -12 -37 -23 -38 -24 -3 -3 161 -293 205 -363 l14 -23 37 22 c20 12 38 23 40 25 4 2 -205 365 -216 376 -4 4 -23 -2 -42 -13z"></path> <path d="M6714 8144 c-114 -198 -111 -187 -54 -218 l26 -14 104 180 c58 100 106 187 108 194 3 12 -45 44 -67 44 -5 0 -58 -84 -117 -186z"></path> <path d="M4827 8093 l-65 -128 -31 -1130 c-17 -621 -33 -1198 -37 -1282 l-6 -152 -65 -38 c-289 -168 -351 -573 -126 -823 237 -264 660 -227 855 74 109 168 109 404 0 572 -46 71 -137 156 -202 189 l-47 24 -6 188 c-3 103 -18 681 -33 1283 l-27 1095 -64 128 c-36 70 -68 127 -72 127 -5 0 -38 -57 -74 -127z m159 -3048 c48 -28 84 -91 84 -145 0 -54 -36 -117 -84 -145 -49 -29 -121 -33 -165 -10 -70 35 -110 123 -91 195 30 113 155 164 256 105z"><animateTransform attributeType="xml" attributeName="transform" type="rotate" from="360 4850 4905" to="0 4850 4905" dur="4s" repeatCount="indefinite"></animateTransform></path> <path d="M1490 6878 c-11 -18 -20 -36 -20 -39 0 -6 363 -219 373 -219 3 0 15 16 28 36 18 30 19 38 8 45 -38 25 -359 209 -364 209 -3 0 -14 -15 -25 -32z"></path> <path d="M8100 6809 c-96 -56 -179 -105 -184 -110 -12 -11 25 -81 40 -77 6 2 92 50 191 108 l181 104 -19 38 c-11 21 -23 38 -27 38 -4 0 -86 -45 -182 -101z"></path> <path d="M960 4905 l0 -45 220 0 220 0 0 45 0 45 -220 0 -220 0 0 -45z"></path> <path d="M8390 4905 l0 -45 220 0 220 0 0 45 0 45 -220 0 -220 0 0 -45z"></path> <path d="M1666 3094 c-99 -57 -184 -108 -189 -113 -6 -6 -1 -23 13 -45 l22 -36 57 32 c254 145 321 184 321 188 0 3 -10 21 -23 41 l-22 37 -179 -104z"></path> <path d="M7925 3155 c-11 -19 -17 -36 -15 -39 3 -2 88 -52 189 -111 140 -81 186 -103 193 -93 5 7 15 25 23 40 l15 26 -184 106 c-101 58 -188 106 -193 106 -5 0 -18 -16 -28 -35z"></path> <path d="M3097 1878 c-8 -13 -49 -84 -92 -158 -42 -74 -86 -150 -97 -167 l-19 -32 33 -21 c18 -11 38 -20 43 -20 6 0 44 60 85 133 41 72 89 157 107 188 l32 56 -39 22 -39 22 -14 -23z"></path> <path d="M6647 1881 c-15 -10 -30 -20 -33 -24 -5 -5 186 -347 207 -371 9 -10 79 23 79 37 0 12 -213 377 -220 376 -3 0 -17 -8 -33 -18z"></path> <path d="M4850 1190 l0 -220 45 0 45 0 0 220 0 220 -45 0 -45 0 0 -220z"></path> </g> </svg>';
      }).property()
    });

    __exports__["default"] = RecipeIngredientAttributeSmallView;
  });
define("v2/views/style/attribute", 
  ["v2/mixins/template-switcher","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var TemplateSwitchMxn = __dependency1__["default"];

    var Ingred;

    Ingred = Ember.View.extend(TemplateSwitchMxn, {
      templateId: "attribute",
      tagName: "div",
      classNames: ['rAttr_el'],
      postfix: (function() {
        var m, r;
        r = /temperature (.*)/;
        m = this.get("title").match(r);
        if (m) {
          return m[1];
        }
      }).property("title"),
      calcPrm: (function(k, v, ov) {
        var tmpCelsius;
        if (arguments.length !== 1) {
          this.set("attribute.prm", v);
        }
        if (+this.attribute.attr === 290) {
          if (this.get("controller.content.conversion.currConv.id") === "us") {
            tmpCelsius = this.attribute.prm.match(/(\d+)/)[0];
            return (tmpCelsius * 1.8 + 32) + "°F";
          } else {
            return tmpCelsius + "°C";
          }
        }
        if (+this.attribute.attr === 446) {
          if (this.get("controller.content.conversion.currConv.id") === "metric") {
            tmpCelsius = this.attribute.prm.match(/(\d+)/)[0];
            return (tmpCelsius - 32) / 1.8 + "°C";
          } else {
            return tmpCelsius + "°F";
          }
        }
        return this.attribute.prm;
      }).property("controller.content.conversion.currConv", "attribute.prm"),
      click: function() {
        if (this.get("controller.currAttrib.item")) {
          return this.set("controller.currAttrib.item", null);
        } else {
          return this.get("controller").send("showAttrib", this.get("item"), this.$());
        }
      }
    });

    __exports__["default"] = Ingred;;
  });
define("v2/views/style/ingred", 
  ["v2/models/svg-element","v2/config/environment","v2/models/conversion","v2/mixins/template-switcher","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var svgElement = __dependency1__["default"];

    var ENV = __dependency2__["default"];

    var conversion = __dependency3__["default"];

    var TemplateSwitchMxn = __dependency4__["default"];

    var RecipeStepView;

    RecipeStepView = Ember.View.extend(TemplateSwitchMxn, {
      templateId: "ingred",
      tagName: "div",
      classNames: ['ingred_container'],
      doNotScale: true,
      classNameBindings: ['isResolved', 'isError', 'isOverview:isOverview'],
      itemHasChangedObserver: (function() {
        return this.set("controller.itemHasChanged", (new Date()).getTime());
      }).observes("item.svgObj.el_type"),
      isResolved: (function() {
        return this.get("query.status") === "ok";
      }).property("query.status"),
      isError: (function() {
        return this.get("query.status") === "error";
      }).property("query.status"),
      isOverview: (function() {
        return this.get("controller.recipeController");
      }).property("controller.recipeController"),
      isCutAllowed: (function() {
        var correctLevel;
        correctLevel = this.get("item._lvl") === 1 || this.get("item._lvl") === 2 && this.get("item._sideCnt");
        return correctLevel && !this.get("controller.viewOnly");
      }).property("item._lvl", "item._sideCnt"),
      useResultStyle: (function() {
        if (!(this.get("item") && "isResult" in this.get("item"))) {
          return "";
        }
        if (this.get("itemResult._first._sideCnt") === 0) {
          return "rIconResult";
        }
        if (this.get("itemResult._first._sideCnt") !== 0) {
          return "rIconResultStyle" + 1;
        }
        return "";
      }).property("item", "itemResult", "itemResult._first._sideCnt"),
      useResultStyleArrow: (function() {
        var tmpSideCnt;
        if (!(this.get("item") && "isResult" in this.get("item"))) {
          return "";
        }
        if (this.get("itemResult._first._sideCnt") === 0) {
          return "result-main";
        }
        if ((tmpSideCnt = this.get("itemResult._first._sideCnt")) !== 0) {
          return "result-side-" + ((tmpSideCnt - 1) % ENV.nrOfSideColors);
        }
      }).property("item", "itemResult", "itemResult._first._sideCnt"),
      itemResult: (function() {
        if (this.get("item.isResult")) {
          return this.get("item._root.resultsList")[this.get("item.uid")];
        } else {
          return this.get("item");
        }
      }).property("query.item", "item.amount", "item.id", "item"),
      initSVG: (function() {
        var tmpSvgEl;
        tmpSvgEl = svgElement.create();
        tmpSvgEl.set("rawSvg", this.get("item.svg"));
        tmpSvgEl.set("id", this.get("item.id"));
        return this.set("query.item.svgObj", tmpSvgEl);
      }).observes("query.item"),
      calcAmount: (function() {
        if (this.get("query") != null) {
          if (!this.get("query.item")) {
            return this.get("query.status");
          }
          this.set("item", this.get("query.item"));
        }
        return this._super();
      }).property("query.item", "item.amount", "item.unitDescr"),
      calcDisplayAmount: (function() {
        var calcAmount, convRef, convertedUnit, currCtrl, currServ, currType, niceCalc, stdReturn, tmpAmount, tmpUnit, _ref;
        niceCalc = function(nr) {
          if (nr.between(0.47, 0.53)) {
            return "1/2";
          }
          if (nr.between(0.23, 0.27)) {
            return "1/4";
          }
          if (nr.between(0.73, 0.77)) {
            return "3/4";
          }
          if (nr.between(0.31, 0.35)) {
            return "1/3";
          }
          if (nr.between(0.65, 0.68)) {
            return "2/3";
          }
          if (nr > 100) {
            return nr.toFixed(0);
          }
          if ((nr * 10) % 10 === 0) {
            return nr.toFixed(0);
          }
          return nr.toFixed(1);
        };
        if (!this.get("item").unitDescr) {
          this.set("item.unitDescr", "");
        }
        currCtrl = this.get("controller");
        if (!currCtrl.get("recipe.conversion.short")) {
          currCtrl = this.get("controller.parentView.controller");
        }
        currType = (_ref = currCtrl.get("recipe.conversion.currConv.id")) != null ? _ref : "author";
        currServ = currCtrl.get("recipe.servings");
        tmpAmount = +this.get("item").amount;
        if (this.isOverview && this.get("item").accuAmount) {
          tmpAmount = +this.get("item").accuAmount;
        }
        calcAmount = (tmpAmount / currCtrl.get("recipe.initialServing")) * currServ;
        if (this.get("doNotScale") && !this.get("controller.viewOnly")) {
          calcAmount = tmpAmount;
        }
        tmpUnit = this.get("item").unitDescr;
        stdReturn = niceCalc(calcAmount) + " " + Ember.I18n.t("" + tmpUnit, {
          count: Math.ceil(calcAmount)
        });
        if (!calcAmount) {
          return "";
        }
        if (tmpAmount === 0) {
          return void 0;
        }
        if (currCtrl.get("viewOnly") === false) {
          return stdReturn;
        }
        if (currType === "author") {
          return stdReturn;
        }
        tmpUnit = this.get("item").unitDescr;
        tmpUnit = tmpUnit.replace(/s$/, '');
        if (tmpUnit in conversion[currType]) {
          convRef = conversion[currType][tmpUnit];
          convertedUnit = convRef["new"];
          return niceCalc(calcAmount * convRef.r) + " " + Ember.I18n.t("" + convertedUnit, {
            count: Math.ceil(calcAmount * convRef.r)
          });
        } else {
          console.log("could not convert " + tmpUnit);
        }
        return stdReturn;
      }).property("item.amount", "item.unitDescr", "controller.recipe.servings", "controller.recipe.conversion.currConv"),
      click: function(e) {
        var tmpItem;
        console.log(e);
        if ($(e.target).is("textarea")) {
          return;
        }
        if ($(e.target).is(".ingredDelete")) {
          return;
        }
        if (this.get("actionDelegate")) {
          this.get("actionDelegate").send("clickHandler", this.get("item"));
          console.log("AHHHHHAAAA");
        }
        if (this.get("isOverview") && this.get("controller.recipeController.currAttrib.modalSearchElementRef")) {
          return this.set("controller.recipeController.currAttrib.modalSearchElementRef", this.get("item"));
        } else if (this.get("controller.currAttrib.item")) {
          return this.set("controller.currAttrib.item", null);
        } else {
          tmpItem = this.get("item");
          delete tmpItem.svgRefOverwrite;
          if (this.get("item.isResult")) {
            tmpItem.svgRefOverwrite = this.get("itemResult");
          }
          return this.get("controller").send("showAttrib", tmpItem, this.$(), this);
        }
      },
      doubleClick: function() {
        if (this.get("controller.controllers.application.user.email") === "marri@marri.is") {
          return open(ENV.tgtURL + "/admin/recipes/element/" + this.get("item").svgObj.id, "_blank");
        }
      },
      actions: {
        cutIngred: function() {
          var itemCpy, knifeSymbol, o;
          knifeSymbol = {
            id: 23,
            name: "Knife",
            svg: "<svg version=\"1.0\" xmlns=\"http://www.w3.org/2000/svg\" width=\"204.000000pt\" height=\"204.000000pt\" viewBox=\"0 0 204.000000 204.000000\" preserveAspectRatio=\"xMidYMid meet\">\n<metadata>\nCreated by potrace 1.10, written by Peter Selinger 2001-2011\n</metadata>\n<g transform=\"translate(0.000000,204.000000) scale(0.100000,-0.100000)\" fill=\"#000000\" stroke=\"none\">\n<path d=\"M385 1179 c-191 -20 -291 -65 -349 -158 -34 -54 -40 -77 -20 -85 28\n-11 269 -44 399 -55 276 -24 471 -32 660 -29 l200 3 11 68 c16 100 23 103 131\n82 115 -24 272 -24 396 0 l97 18 24 -22 c21 -20 27 -21 48 -11 44 23 58 42 58\n79 0 34 -30 98 -54 113 -15 10 -1502 7 -1601 -3z m818 -46 c3 -10 8 -36 12\n-58 3 -22 14 -53 24 -70 12 -20 16 -44 13 -74 l-4 -44 -327 6 c-179 4 -397 13\n-483 21 -203 19 -388 47 -388 59 0 20 46 71 88 98 109 69 169 76 664 78 370 1\n397 0 401 -16z m184 -19 c3 -8 -2 -23 -11 -32 -15 -15 -17 -15 -33 0 -13 14\n-14 20 -3 32 16 20 39 20 47 0z m253 0 c11 -12 11 -18 -2 -30 -18 -18 -48 -8\n-48 15 0 33 28 42 50 15z m258 -8 c4 -21 -14 -37 -35 -29 -14 5 -18 35 -6 47\n12 12 38 1 41 -18z\"></path>\n</g>\n</svg>"
          };
          o = this.get("item._root").createStep(knifeSymbol);
          itemCpy = this.get("item._root").createStep(this.get("item"));
          Ember.run.scheduleOnce('afterRender', this, (function(_this) {
            return function() {
              return _this.set("controller.currAttrib.item", itemCpy);
            };
          })(this));
          o.get("subs").pushObject(itemCpy);
          this.get("item._parent.subs").pushObject(o);
          this.get("item").destroy();
          return this.set("controller.redraw", (new Date()).getTime());
        },
        removeMe: function() {
          var root;
          if (this.get("specialResults")) {
            this.get("item._root").deleteResults(this.get("item.uid"));
            this.get("specialResults").removeObject(this.get("ref"));
          } else if (this.get("isSearchresult")) {
            this.get("isSearchresult").removeObject(this.get("ref"));
          } else {
            root = this.get("item._root");
            this.get("item").destroy();
            root.reindexMeta();
          }
          return this.set("controller.redraw", (new Date()).getTime());
        }
      }
    });

    __exports__["default"] = RecipeStepView;;
  });
define("v2/views/style/multi", 
  ["ember","v2/mixins/template-switcher","v2/utils/symbol-type-lut","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var TemplateSwitchMxn = __dependency2__["default"];

    var SymbolTypeLUT = __dependency3__["default"];

    var EditMultiView;

    EditMultiView = Ember.View.extend(TemplateSwitchMxn, {
      templateId: "multi",
      classNames: ["sortme"],
      letterToBeShown: (function() {
        var alphabet, multiWithinMulti;
        alphabet = ["A", "B", "C", "D", "E", "F", "G", "H", "I", "J", "K", "L", "M", "N", "O", "P"];
        multiWithinMulti = this.get("item._parent.subs").filter(function(i) {
          return i.get("subs").length > 0;
        });
        return alphabet[multiWithinMulti.indexOf(this.get('item'))];
      }).property("item._parent.subs.@each"),
      initItem: (function() {
        return this.set('item.isTool', true);
      }).on('init'),
      isToolAction: Ember.computed("item.svgObj.type", function() {
        if (+this.get("item.svgObj.type") === SymbolTypeLUT["tool-action"]) {
          return true;
        }
      }),
      isInfo: Ember.computed("item.svgObj.type", function() {
        if (!this.get("controller.viewOnly")) {
          return false;
        }
        if (+this.get("item.svgObj.type") === SymbolTypeLUT["info"]) {
          return true;
        }
      }),
      resetItem: (function() {
        return this.set('item.isTool', false);
      }).on('willDestroyElement'),
      actions: {
        removeItem: function() {
          return this.get("item").destroy();
        },
        showAttribute: function(item) {
          var currItem;
          currItem = item != null ? item : this.get("item");
          if (this.get("controller.currAttrib.item")) {
            return this.set("controller.currAttrib.item", null);
          } else {
            return this.get("controller").send("showAttrib", currItem, this.$());
          }
        }
      }
    });

    __exports__["default"] = EditMultiView;
  });
define("v2/views/style/side", 
  ["v2/views/style/step","v2/config/environment","v2/mixins/template-switcher","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var step = __dependency1__["default"];

    var ENV = __dependency2__["default"];

    var TemplateSwitchMxn = __dependency3__["default"];

    var RecipeStepView;

    RecipeStepView = step.extend(TemplateSwitchMxn, {
      templateId: "side",
      tagName: "div",
      getSideClass: (function() {
        var tmpSideCnt;
        if ((tmpSideCnt = this.get("item.subs.0._sideCnt")) !== 0) {
          return "task-title-" + ((tmpSideCnt - 1) % ENV.nrOfSideColors);
        } else {
          return "";
        }
      }).property("item", "item.subs.@each._sideCnt"),
      needsLine: Ember.computed.gte('item.parentIndex', 1),
      getSideClassHover: (function() {
        return this.get('getSideClass') + ' hoverTransparent';
      }).property('getSideClass'),
      deleteSideWhenEmpty: (function() {
        if (this.get('item.subs.length') === 0) {
          return this.get('item').destroy();
        }
      }).observes('item.subs.@each'),
      actions: {
        toggleElement: function() {
          var i, index, stuff, _i, _len, _ref;
          window.ga && window.ga('send', 'event', 'Editor', 'sideProcess', "resolveSideprocess");
