define("v2/tests/unit/components/edit-ingredient-test", 
  ["ember-qunit"],
  function(__dependency1__) {
    "use strict";
    var test = __dependency1__.test;
    var moduleForComponent = __dependency1__.moduleForComponent;
    moduleForComponent('edit-ingredient', 'EditIngredientComponent', {});

    test('it renders', function() {
      var component;
