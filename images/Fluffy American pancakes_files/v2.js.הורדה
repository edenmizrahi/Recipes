define("v2/controllers/index", 
  ["v2/utils/recipe-utils","v2/models/recipe-blogger-group","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var recipeModel = __dependency1__["default"];

    var recipesBloggerGroup = __dependency2__["default"];

    var ENV = __dependency3__["default"];

    var index;

    index = Ember.ArrayController.extend({
      needs: ["application", "modal-login"],
      loadingMore: false,
      ENV: ENV,
      recipeOfToday: "test",
      viewOnly: true,
      activeStyle: "arrow",
      testFooActive: (function() {
        return +sessionStorage["nlD7PoXUQFKlSO-cI3zITw-varID"] === 1;
      }).property(""),
      isLoggedIn: Ember.computed("controllers.application.loggedIn", function() {
        var tmpResult, _ref;
        tmpResult = this.get("controllers.application.loggedIn");
        if (tmpResult === true) {
          this.set("rrcIndex", void 0);
          if ((_ref = this.get("routeRef")) != null) {
            _ref.refresh();
          }
          console.log("triggering something...");
        }
        return tmpResult;
      }),
      hideRegisterBanner: (function() {
        if (this.get("controllers.application.loggedIn")) {
          return false;
        }
        return !sessionStorage["hideRegisterBanner"];
      }).property("hideRegister_trigger", "controllers.application.loggedIn"),
      loadMoreRecipes: (function() {
        if (this.get("rrc.status.isLoading")) {
          return;
        }
        return window.ga && window.ga('send', 'event', 'Index', 'loadMoreRecipes', this.currentPage);
      }).observes("rrc.status.isLoading"),
      actions: {
        addRandomProfiles: function() {
          var a;
          if (this.get("model").length > 6) {
            a = recipesBloggerGroup.create();
            a.getRandomBlogger();
            return this.get("model").insertAt(this.get("model").length - 6, a);
          }
        },
        triggerHome: function() {
          return this.transitionToRoute('index');
        },
        tryFBLogin: function() {
          return this.get("controllers.modal-login").send("tryFBLogin");
        },
        closeRegisterBanner: function() {
          sessionStorage["hideRegisterBanner"] = 1;
          return this.set("hideRegister_trigger", 1);
        },
        createrWithContent: function(exId) {
          var tmpTxt;
          tmpTxt = $("#" + exId).text();
          if (!tmpTxt) {
            tmpTxt = $("#" + exId).val();
          }
          console.log(tmpTxt);
          return this.transitionToRoute('recipe_edit4', 'new').then(function(newRoute) {
            console.log(newRoute);
            newRoute.controller.set('graciphyInputTxt', tmpTxt);
            newRoute.controller.set('graciphyAutoConvert', true);
          });
        }
      }
    });

    __exports__["default"] = index;;
  });
define("v2/controllers/modal-confirm", 
  ["exports"],
  function(__exports__) {
    "use strict";
    var c;

    c = Ember.ObjectController.extend({
      needs: ["application"],
      callback: false,
      text: "Are you sure?",
      actions: {
        onYes: function() {
          if (this.callback) {
            return this.callback(true);
          } else {
            return this.send("hideModal");
          }
        },
        onNo: function() {
          if (this.callback) {
            return this.callback(false);
          } else {
            return this.send("hideModal");
          }
        },
        close: function() {
          var e;
          this.callback(null);
          try {
            return this.send("hideModal");
          } catch (_error) {
            e = _error;
            return window.location.replace("/");
          }
        }
      }
    });

    __exports__["default"] = c;;
  });
define("v2/controllers/modal-login", 
  ["v2/utils/ajax-resolver","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var ajax = __dependency1__["default"];

    var FB_workaround, c;

    FB_workaround = function(cb) {
      var funky, login, statusChangeCallback;
      funky = function(argument) {
        FB.api("/me", function(response) {
          cb(FB.getAuthResponse().accessToken, response);
        });
      };
      statusChangeCallback = function(response) {
        if (response.status === "connected") {
          funky();
        } else {
          login();
        }
      };
      login = function() {
        FB.login((function(response) {
          if (response.authResponse) {
            funky();
          } else {
            console.log("User cancelled login or did not fully authorize.");
          }
        }), {
          scope: 'email'
        });
      };
      statusChangeCallback(window.storedFbResponse);
    };

    c = Ember.ObjectController.extend({
      needs: ["application"],
      state: "login",
      txt_user: "",
      txt_pwd: "",
      txt_pwdConfirm: "",
      errorOccured: 0,
      isRegister: (function() {
        return this.get("state") === "register";
      }).property("state"),
      islogin: (function() {
        return this.get("state") === "login";
      }).property("state"),
      pwNotMatch: (function() {
        return this.txt_pwd !== this.txt_pwdConfirm;
      }).property("txt_pwd", "txt_pwdConfirm"),
      actions: {
        changeState: function(state) {
          return this.set("state", state);
        },
        close: function() {
          return this.send("hideModal");
        },
        tryLogin: function() {
          var pwd, usr;
          window.ga && window.ga('send', 'event', 'Login', 'login', "email");
          this.set("errorOccured", 0);
          usr = this.get("txt_user");
          pwd = this.get("txt_pwd");
          console.log("usr: " + this.get("txt_user") + " pwd: " + this.get("txt_pwd"));
          return ajax.getLogin(usr, pwd, (function(_this) {
            return function(data) {
              if (!data) {
                _this.set("errorOccured", 1);
                return;
              }
              _this.set("controllers.application.user.email", _this.get("txt_user"));
              _this.set("controllers.application.user.secretAT", data.access_token);
              return _this.send("close");
            };
          })(this));
        },
        tryFBLogin: function() {
          var test;
          window.ga && window.ga('send', 'event', 'Login', 'login', "facebook");
          console.log('bliiiiii');
          test = (function(_this) {
            return function(token, me) {
              var opt;
              opt = {
                fb_token: token
              };
              return ajax.getUser(opt, function(data) {
                if (!data) {
                  alert("error!");
                  return;
                }
                if (data.fbJustRegistered) {
                  window.ga && window.ga('send', 'event', 'Login', 'register', "facebook");
                }
                console.log("Hello: " + data.first_name + " and your new token :) " + data.access_token);
                _this.set("controllers.application.user.fbUser", me);
                _this.set("controllers.application.user.email", data.email);
                _this.set("controllers.application.user.secretAT", data.access_token);
                return _this.send("close");
              });
            };
          })(this);
          return FB_workaround(test);
        },
        tryRegister: function() {
          var opt, pwd, usr;
          window.ga && window.ga('send', 'event', 'Login', 'register', "email");
          if (this.get("pwNotMatch")) {
            return;
          }
          usr = this.get("txt_user");
          pwd = this.get("txt_pwd");
          opt = {
            userName: usr,
            pw: pwd,
            register: 1
          };
          return ajax.getUser(opt, (function(_this) {
            return function(data) {
              if (!data) {
                alert("error!");
                return;
              }
              return _this.send("tryLogin");
            };
          })(this));
        }
      }
    });

    __exports__["default"] = c;;
  });
