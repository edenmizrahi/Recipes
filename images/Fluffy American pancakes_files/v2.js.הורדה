    var ToolsOverview;

    ToolsOverview = Ember.Component.extend({
      layoutName: "components/editor-searchresults",
      activeStyle: "arrow",
      _registerWithTarget: Ember.on('init', function() {
        return this.set('register-as', this);
      }),
      filterByStatus: function(isResolved, isIngredient, doneArray) {
        var i, retArr, _i, _len;
        retArr = [];
        for (_i = 0, _len = doneArray.length; _i < _len; _i++) {
          i = doneArray[_i];
          i.set("item", this.get("currCtrl.model").createStep(i.item));
          if (isResolved && i.get("status") === "ok") {
            if (isIngredient && i.get("item").type !== 2) {
              retArr.push(i);
            }
            if (!isIngredient && i.get("item").type === 2) {
              retArr.push(i);
            }
          } else if (!isResolved && i.get("status") !== "ok") {
            retArr.push(i);
          }
        }
        return retArr;
      },
      history: nodeDis.queueList,
      doneArray: nodeDis.doneList,
      resolvedElements: (function() {
        return this.filterByStatus(true, true, this.get("doneArray"));
      }).property("doneArray.@each"),
