define("v2/components/graciphy-input", 
  ["v2/utils/ajax-resolver","v2/models/svg-element","ember","v2/utils/utils","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var ajax = __dependency1__["default"];

    var svgElement = __dependency2__["default"];

    var Ember = __dependency3__["default"];
    var UTILS = __dependency4__["default"];

    var ajax = __dependency1__["default"];

    var GraciphyInputComponent,
      __indexOf = [].indexOf || function(item) { for (var i = 0, l = this.length; i < l; i++) { if (i in this && this[i] === item) return i; } return -1; };

    GraciphyInputComponent = Ember.Component.extend({
      isConverting: false,
      inputIngredTxt: "",
      inputTxt: "",
      defaultInputIngredTxt: "1/2 large onion\n2 carrots\n100 g cream cheese",
      defaultInputTxt: "Add the cut onion, the carrots and cream cheese and cook in a pan on medium heat for 10 minutes.",
      _registerWithTarget: Ember.on('init', function() {
        var qs;
        this.set('register-as', this);
        qs = UTILS.getQueryParams(document.location.search);
        if ("convertText" in qs && "convertIngredText" in qs) {
          this.set("defaultInputTxt", qs["convertText"]);
          this.set("defaultInputIngredTxt", qs["convertIngredText"]);
          return this.set("autoConvert", true);
        }
      }),
      didInsertElement: function() {
        if (!this.get("inputIngredTxt")) {
          this.set("inputIngredTxt", this.get("defaultInputIngredTxt"));
        }
        if (!this.get("inputTxt")) {
          this.set("inputTxt", this.get("defaultInputTxt"));
        }
        if (this.get("autoConvert")) {
          this.send("convert");
          return this.set("autoConvert", false);
        }
      },
      keyDown: function(e) {
        if (+e.keyCode === 13) {
          return console.log("nothing happends");
        }
      },
      actions: {
        loadFromURL: function(recipeURL) {
          if (!recipeURL) {
            recipeURL = prompt('Enter URL to scrape', 'http://allrecipes.com/');
          }
          if (recipeURL !== null) {
            this.set("isConverting", true);
            ajax.scrapeWebsite(recipeURL, (function(_this) {
              return function(d) {
                _this.set("inputIngredTxt", "" + d.ingredient_text);
                _this.set("inputTxt", "" + d.instruction_text);
                return _this.send("convert");
              };
            })(this));
          }
          console.log('it must be svg-element');
          return window.ga && window.ga('send', 'event', 'Editor', 'loadFromURL', recipeURL);
        },
        convert: function() {
          var a, b;
          window.ga && window.ga('send', 'event', 'Editor', 'converting');
          this.set("isConverting", true);
          a = this.get("inputIngredTxt").replace(/\n(\s*\n)*/, "\n");
          b = this.get("inputTxt").replace(/\n(\s*\n)*/, "\n");
          return ajax.gracipyText("" + a + "\n\n" + b, (function(_this) {
            return function(retVal) {
              if (retVal === null) {
                alert("An error occured during conversion, maybe try another recipe?");
                _this.set("currCtrl.redraw", (new Date()).getTime());
                _this.set("isConverting", false);
                return;
              }
              return Ember.run(function() {
                var flameLut, heat, i, marshall, o, temp, tempId, tempPrm, tempStates, time, timeId, tmpContainer, tmpO, toolLUT, val, x, _i, _j, _k, _len, _len1, _len2, _ref, _ref1, _ref2, _ref3, _results;
                _this.set("isConverting", false);
                _ref = retVal.unusedIngredients;
                for (_i = 0, _len = _ref.length; _i < _len; _i++) {
                  val = _ref[_i];
                  marshall = Ember.Object.create({
                    item: val,
                    status: "missing"
                  });
                  _this.get("currCtrl.searchResultsComponent.doneArray").pushObject(marshall);
                }
                _ref1 = retVal.gracipeSteps;
                _results = [];
                for (_j = 0, _len1 = _ref1.length; _j < _len1; _j++) {
                  val = _ref1[_j];
                  console.log(val);
                  o = _this.get("model").createStep(Ember.Object.create());
                  o.svgObj = svgElement.create().set("id", val.tool.id);
                  o.set("rawText", val.instructions);
                  o.set("rawJSON", JSON.stringify(val));
                  o.set("hasRawText", true);
                  o.set("id", val.tool.id);
                  o.set("name", val.tool.name);
                  o.set("instructions", val.instructions);
                  o.set("subs", Ember.A([]));
                  o.set("attributes", Ember.A([]));
                  if (val.temperature) {
                    tempStates = {
                      "degrees-c": 290,
                      "degrees-f": 446
                    };
                    tempId = tempStates[val.temperature.unit];
                    tempPrm = +val.temperature.amount;
                    temp = _this.get("model").createStep({
                      id: tempId,
                      attr: tempId,
                      prm: tempPrm
                    });
                    o.get("attributes").pushObject(temp);
                  }
                  if (val.time) {
                    timeId = 26;
                    time = _this.get("model").createStep({
                      id: timeId,
                      attr: timeId,
                      prm: val.time
                    });
                    o.get("attributes").pushObject(time);
                  }
                  flameLut = {
                    "low": 269,
                    "medium": 270,
                    "high": 271
                  };
                  if (val.heat && flameLut[val.heat]) {
                    heat = _this.get("model").createStep({
                      id: flameLut[val.heat],
                      attr: flameLut[val.heat],
                      prm: val.heat + " heat"
                    });
                    o.get("attributes").pushObject(heat);
                  }
                  toolLUT = {};
                  _ref2 = val.ingredients;
                  for (_k = 0, _len2 = _ref2.length; _k < _len2; _k++) {
                    i = _ref2[_k];
                    if (i.tool_id && i.tool_name) {
                      if (_ref3 = i.tool_id, __indexOf.call(toolLUT, _ref3) < 0) {
                        tmpO = _this.get("model").createStep({
                          id: i.tool_id,
                          name: i.tool_name
                        });
                        if ("tool_instruction" in i) {
                          tmpO.set("instructions", i.tool_instruction);
                        }
                        o.get("subs").pushObject(tmpO);
                        toolLUT[i.tool_id] = tmpO;
                      }
                      tmpContainer = toolLUT[i.tool_id].get("subs");
                    } else {
                      tmpContainer = o.get("subs");
                    }
                    x = _this.get("model").createStep(Ember.Object.create());
                    x.set("id", i.id);
                    x.set("name", i.name);
                    if (i.amount != null) {
                      x.set("amount", i.amount);
                    }
                    if (i.unitDescr != null) {
                      x.set("unitDescr", i.unitDescr);
                    }
                    x.svgObj = svgElement.create().set("id", i.id);
                    tmpContainer.pushObject(x);
                  }
                  _this.get("model.stepsArr").pushObject(o);
                  _results.push(_this.set("currCtrl.redraw", (new Date()).getTime()));
                }
                return _results;
              });
            };
          })(this));
        }
      }
    });

    __exports__["default"] = GraciphyInputComponent;
  });
define("v2/components/image-hover-or-modal", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ImageHoverOrModalComponent;

    ImageHoverOrModalComponent = Ember.Component.extend({
      presentMobile: (function() {
        return true;
        return window.mobilecheck();
      }).property(),
      isVideo: (function() {
        return this.get("webm") && this.get("webm") !== "";
      }).property("webm"),
      isVideoAndMobile: (function() {
        return false;
        return this.get("presentMobile") && this.get("isVideo");
      }).property("presentMobile", "isVideo"),
      getTitle: (function() {
        var actionString, eventString;
        actionString = this.get("presentMobile") ? "Click" : "Hover";
        eventString = this.get("isVideo") ? "play video" : "zoom";
        return "" + actionString + " to " + eventString;
      }).property("hoverSrc"),
      getVideoExtension: (function() {
        var m, tmpSrcString;
        tmpSrcString = "" + (this.get("webm"));
        m = tmpSrcString.match(/\.(webm|mp4|ogg)($|\?)/);
        if (!m) {
          return void 0;
        }
        return m[1];
      }).property("webm"),
      hoverSrc: (function() {
        if (this.get("isVideo")) {
          return this.get("webm");
        } else {
          return this.get("src");
        }
      }).property("isVideo", "src"),
      actions: {
        showModal: function() {
          if (this.get("presentMobile")) {
            if (this.get("isVideo")) {
              return this.get("delegate").send("showModalImage", this.get("webm"));
            } else {
              return this.get("delegate").send("showModalImage", this.get("src"));
            }
          }
        }
      }
    });

    __exports__["default"] = ImageHoverOrModalComponent;
  });
