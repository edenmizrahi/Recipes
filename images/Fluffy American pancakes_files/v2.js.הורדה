    oAuthKey = '';

    client_id = 'f4f4715f2ef02152a418';

    client_secret = '4883947ef46c49cd44596c0e3b1a15dc1a212098';

    getOAuthKey = function() {
      if (!oAuthKey) {
        return oAuthTestKey;
      }
      return oAuthKey;
    };

    resolver = {
      setOAuthAccessToken: (function(_this) {
        return function(newToken) {
          return oAuthKey = newToken;
        };
      })(this),
