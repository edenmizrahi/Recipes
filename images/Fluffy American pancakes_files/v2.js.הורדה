    RecipeWatchdog = (function() {
      function RecipeWatchdog(modelRef) {
        var checkWatch, self;
        this.modelRef = modelRef;
        delete localStorage["recipeWatch"];
        delete localStorage["recipeWatchId"];
        self = this;
        checkWatch = (function(_this) {
          return function() {
            if (self.modelRef && self.enabled) {
              self.modelRef.reindexMeta();
              localStorage["recipeWatch"] = RUtils.recipeToStorage(self.modelRef);
              localStorage["recipeWatchId"] = self.modelRef.id;
              localStorage["recipeWatchTitle"] = self.modelRef.title;
              localStorage["recipeWatchInitialServing"] = self.modelRef.initialServing;
              localStorage["recipeWatchDescription"] = self.modelRef.description;
              localStorage["recipeWatchSrcImgId"] = self.modelRef.srcImgId;
            }
            return setTimeout(function() {
              return checkWatch();
            }, self.watchIntervall);
          };
        })(this);
        checkWatch();
      }

      RecipeWatchdog.prototype.stopWatchdog = function() {
        this.enabled = false;
        return delete localStorage["recipeWatch"];
      };

      RecipeWatchdog.prototype.enabled = true;

      RecipeWatchdog.prototype.watchIntervall = 2000;

      return RecipeWatchdog;

    })();

    RecipeController = Ember.ObjectController.extend({
      needs: ["application", "index"],
      activeStyle: "arrow",
      redraw: 0,
      isEditor4: 1,
      viewOnly: false,
      showRecipeDetails: false,
      currAttrib: {},
      elPicker: {},
      quickAdd: GQuickAdd,
      wd: null,
      showConverter: true,
      showIngredints: false,
      recipe: Ember.computed.alias("content"),
      pubSteps: (function() {
        this.get("model");
        return this.get("model.stepsArr");
      }).property("model"),
      isEmpty: (function() {
        return this.get("pubSteps").length === 0;
      }).property("pubSteps.@each"),
      isEmptyOrEndParallel: (function() {
        return this.get("isEmpty") || this.get("stopInParallel");
      }).property("isEmpty", "stopInParallel"),
      isStopInParallelPossible: (function() {
        return this.get("lastEl._parent.subproc");
      }).property("lastEl._parent.subproc"),
      lastEl: (function() {
        var lastEl;
        lastEl = this.get("pubSteps")[this.get("pubSteps").length - 1];
        if (lastEl != null ? lastEl.subproc : void 0) {
          lastEl = lastEl.subs[lastEl.subs.length - 1];
        }
        return lastEl;
      }).property("pubSteps.@each", "redraw"),
      initWatchdog: function(m) {
        var wd;
        wd = new RecipeWatchdog(m);
        this.set("wd", wd);
        return wd;
      },
      autoConvertURL: "http://allrecipes.com/recipe/158703/sweet-bacon-wrapped-pork-loin/?prop24=RD_RelatedRecipes",
      autoConvertURLStart: false,
      actions: {
        blurInput: function() {
          console.log('Blurring all inputs');
          return $("input").blur();
        },
        autoConvertFromURL: function(url) {
          if (!url) {
            url = this.get("autoConvertURL");
          }
          console.log(url);
          return this.get("graciphyer").send("loadFromURL", url);
        },
        removeAllSteps: function() {
          var proceed;
          proceed = confirm('Are you sure you want to delete all steps?');
          if (proceed) {
            while (this.get('model.stepsArr')[0]) {
              this.get('model.stepsArr')[0].destroy();
            }
            this.set('model.results', {});
            return this.set("redraw", (new Date()).getTime());
          }
        },
        stopInParallel: function() {
          return this.set("stopInParallel", 1);
        },
        addInParallel: function() {
          var tmpO;
          this.set("stopInParallel", 0);
          tmpO = this.get("model").createStep({
            el: "side_template",
            side: "side",
            subproc: true,
            subs: [],
            _noDelete: true
          });
          this.get("pubSteps").pushObject(tmpO);
          return this.send("showElement", tmpO.subs, null, true);
        },
        addSameTool: function(itemToCopy) {
          var elementToCopy, insertAtIdx, newO, o;
          console.log("ADD TOOL");
          if (itemToCopy) {
            elementToCopy = itemToCopy;
            insertAtIdx = itemToCopy.get("parentIndex") + 1;
          } else {
            elementToCopy = this.get("lastEl");
            insertAtIdx = this.get("lastEl.parentIndex") + 1;
          }
          o = {
            id: elementToCopy.id,
            name: elementToCopy.name
          };
          newO = this.get("model").createStep(o);
          elementToCopy.get("_parent.subs").insertAt(insertAtIdx, newO);
          return this.set("redraw", (new Date()).getTime());
        },
        doneSave: function() {
          var data, tmpId;
          if (!this.get("controllers.application.loggedIn")) {
            this.get("controllers.application").send("doLogin");
            return;
          }
          if (!this.get("model.title")) {
            this.set("showRecipeDetails", 1);
            this.set("errorTitleMissing", "To save the recipe, please give it a title");
            return;
          }
          if (!this.get("model.initialServing")) {
            this.set("model.initialServing", 4);
          }
          tmpId = this.get("model.id");
          if (tmpId == null) {
            tmpId = 0;
          }
          data = {
            id: tmpId,
            title: this.get("model.title"),
            servings: this.get("model.initialServing"),
            preptime: this.get("model.preptime"),
            description: this.get("model.description"),
            srcImgId: this.get("model.srcImgId"),
            cat: JSON.stringify(this.get("model.cats")),
            json: RUtils.recipeToStorage(this.get("model"))
          };
          this.get("wd").stopWatchdog();
          return ajax.storeGracipe(data, (function(_this) {
            return function(res) {
              if (res) {
                console.log("doneSave - YEHA");
                window.ga && window.ga('send', 'event', 'Editor', 'gracipeSaved', _this.get("model.title"));
                return _this.transitionToRoute('recipe', res);
              } else {
                return console.log("doneSave - NOP");
              }
            };
          })(this));
        },
        setShowDetails: function(s) {
          this.set("showRecipeDetails", s);
          if (s === 0) {
            return $('.editorSidebar').css("min-height", "800px");
          } else {
            return $('.editorSidebar').css("min-height", "" + (800 + 565) + "px");
          }
        },
        setShowConverter: function(s) {
          return this.set("showConverter", s);
        },
        setShowIngredients: function(s) {
          return this.set("showIngredints", s);
        },
        removeMe: function(ref) {
          ref.destroy();
          return this.set("redraw", (new Date()).getTime());
        },
        discardChanges: function() {
          this.get("wd").stopWatchdog();
          if (this.get("model.slug")) {
            return this.transitionToRoute('recipe', this.get("model.slug"));
          } else {
            return this.transitionToRoute('index');
          }
        },
        clickEvent: function() {
          if (!this.get("currAttrib.ignoreClick")) {
            this.set("currAttrib.item", null);
          }
          return this.set("currAttrib.ignoreClick", false);
        },
        showAttrib: function(prm, jq, viewRef) {
          console.log('showattrib stuff');
          console.log(prm);
          console.log(jq);
          console.log(viewRef);
          console.log('showattrib stuff end');
          this.set("currAttrib.item", prm);
          this.set("currAttrib.jq", jq);
          this.set("currAttrib.viewRef", viewRef);
          return this.set("currAttrib.ignoreClick", true);
        },
        showElement: function(prm, viewRef, isTools, index) {
          console.log("SHOW_ELEMENT");
          this.set("elPicker.container", prm);
          this.set("elPicker.viewRef", viewRef);
          this.set("elPicker.isTools", isTools);
          return this.set("elPicker.index", index);
        },
        clearList: function() {
          return nodeDis.doneList.clear();
        },
        quickAddElement: function(oRef) {
          var o;
          this.set("stopInParallel", 0);
          window.ga && window.ga('send', 'event', 'Editor', 'quickAdd', oRef.name);
          o = JSON.parse(JSON.stringify(oRef));
          o = this.get("model").createStep(o);
          this.get("model.stepsArr").addObject(o);
          return this.set("redraw", (new Date()).getTime());
        },
        addResultAfterItem: function(item) {
          var pArr, tmpObj;
          if (!item) {
            item = this.get("pubSteps")[this.get("pubSteps").length - 1];
            if (item.subproc) {
              item = item.subs[item.subs.length - 1];
            }
          }
          window.ga && window.ga('send', 'event', 'Editor', 'addResult', "true");
          tmpObj = this.get("model").createResults(item);
          pArr = item.get("_parent.subs");
          pArr.insertAt(pArr.indexOf(item) + 1, tmpObj);
          this.get("model").reindexMeta();
          return Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              _this.get("model").reindexMeta();
              return _this.set("redraw", (new Date()).getTime());
            };
          })(this));
        },
        uploadPicture: function() {
          var $uploadField;
          window.ga && window.ga('send', 'event', 'Editor', 'uploadPicture', "true");
          $uploadField = $(".fileClick");
          $uploadField.unbind("change");
          $uploadField.bind("change", (function(_this) {
            return function(event) {
              var files;
              files = event.target.files;
              return RUtils.uploadFiles(files, function(newURL) {
                if (newURL) {
                  _this.set("model.srcImg", newURL.filePath);
                }
                if (newURL) {
                  return _this.set("model.srcImgId", newURL.id);
                }
              });
            };
          })(this));
          return $uploadField.click();
        },
        focusTextbox: function() {
          window.ga && window.ga('send', 'event', 'Editor', 'quickAddMore', "MORE");
          switch (Math.floor(Math.random() * 4)) {
            case 0:
              $("#theSearchPlaceholder").val("blender");
              break;
            case 1:
              $("#theSearchPlaceholder").val("100g ice-cream");
              break;
            case 2:
              $("#theSearchPlaceholder").val("1/2 pineapple");
              break;
            case 3:
              $("#theSearchPlaceholder").val("oven");
          }
          return $("#theSearchPlaceholder").focus();
        },
        trackTutorialClick: function() {
          return window.ga && window.ga('send', 'event', 'Editor', 'openTutorial', "true");
        }
      }
    });

    __exports__["default"] = RecipeController;;
  });
define("v2/controllers/search", 
  ["ember","v2/utils/recipe-request-resolver","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var RecipeRequestController = __dependency2__["default"];

    var SearchController;

    SearchController = Ember.ArrayController.extend({
      queryParams: ['category', 'q'],
      category: null,
      tagList: [],
      oldQ: null,
      tagListGen: (function() {
        var boolVal, res, tag, tagLis, _i, _len;
        if ((this.q && this.tagList.length === 0) || (this.q !== this.oldQ)) {
          this.set('tagList', this.rrc.tags);
          this.set('oldQ', this.q);
        }
        tagLis = this.get('tagList');
        res = [];
        if (tagLis.length) {
          for (_i = 0, _len = tagLis.length; _i < _len; _i++) {
            tag = tagLis[_i];
            boolVal = ("" + tag).toLowerCase() === ("" + this.category).toLowerCase();
            res.push({
              name: tag,
              selected: boolVal
            });
          }
        }
        return res;
      }).property('model.@each')
    });

    __exports__["default"] = SearchController;
  });
define("v2/helpers/title-case", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var TitleCaseHelper, titleCase;

    titleCase = function(value, option) {
      return value.replace(/([^\x00-\x7F]|\w)\S*/g, function(txt) {
        return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
      });
    };

    TitleCaseHelper = Ember.Handlebars.makeBoundHelper(titleCase);

    __exports__.titleCase = titleCase;

    __exports__["default"] = TitleCaseHelper;
  });
define("v2/initializers/extend-basic-types", 
  ["exports"],
  function(__exports__) {
    "use strict";
    var ExtendBasicTypesInitializer, initialize;

    String.prototype.toProperCase = function() {
      return this.replace(/\w\S*/g, function(txt) {
        return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
      });
    };

    Number.prototype.between = function(min, max) {
      return min <= this && this <= max;
    };

    Number.prototype.displayTime = function() {
      var hours, min, ret, sec, time;
      time = this;
      sec = Math.floor(time % 60);
      min = Math.floor((time % 3600) / 60);
      hours = Math.floor(time / 3600);
      ret = "";
      if (hours > 0) {
        ret += "" + hours + ":" + (min < 10 ? "0" : "");
      }
      ret += "" + min + ":" + (sec < 10 ? "0" : "");
      ret += "" + sec;
      return ret;
    };

    Array.prototype.unique = function() {
      var key, output, value, _i, _ref, _results;
      output = {};
      for (key = _i = 0, _ref = this.length; 0 <= _ref ? _i < _ref : _i > _ref; key = 0 <= _ref ? ++_i : --_i) {
        output[this[key]] = this[key];
      }
      _results = [];
      for (key in output) {
        value = output[key];
        _results.push(value);
      }
      return _results;
    };

    initialize = function() {};

    ExtendBasicTypesInitializer = {
      name: 'extend-basic-types',
      initialize: initialize
    };

    __exports__.initialize = initialize;

    __exports__["default"] = ExtendBasicTypesInitializer;
  });
