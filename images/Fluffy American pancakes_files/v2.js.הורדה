define("v2/models/timer", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];

    var Timer;

    Timer = Ember.Object.extend({
      recipe: null,
      stepNumber: 0,
      stepTitle: 'Step title',
      inpTime: null,
      totalTime: null,
      passedTime: 0,
      initTime: null,
      intervalSet: false,
      interval: null,
      blinkInterval: null,
      blink: false,
      blinkRunning: false,
      getTime: function() {
        return Date.now() / 1000;
      },
      initTimer: (function(iniTime) {
        this.set('totalTime', iniTime);
        return this.set('inpTime', iniTime);
      }),
      resetTime: function() {
        clearInterval(this.get('blinkInterval'));
        this.set('recipe._blinking', false);
        this.set('blink', false);
        return this.set('blinkRunning', false);
      },
      runFirst: function() {
        window.ga && window.ga('send', 'event', 'Timer', 'starting');
        this.set('recipe._timerRunning', true);
        this.set('recipe._timerSet', true);
        this.set('recipe._blinking', false);
        this.set('recipe._minLeft', Math.ceil(this.get('totalTime') / 60));
        return setTimeout(((function(_this) {
          return function() {
            if (!_this.get('intervalSet')) {
              return _this.startToggle();
            }
          };
        })(this)), 1000);
      },
      startToggle: function() {
        this.resetTime();
        if (this.get('ratio') === 100) {
          return;
        }
        if (!this.intervalSet) {
          this.set('recipe._timerRunning', true);
          this.set('recipe._timerSet', true);
          this.set('initTime', this.getTime() + this.get('totalTime') - this.get('passedTime'));
          this.set('interval', setInterval(((function(_this) {
            return function() {
              return _this.set('passedTime', _this.get('totalTime') - (_this.get('initTime') - _this.getTime()));
            };
          })(this)), 200));
          return this.set('intervalSet', true);
        } else {
          this.set('recipe._timerRunning', false);
          clearInterval(this.get('interval'));
          return this.set('intervalSet', false);
        }
      },
      reset: function() {
        window.ga && window.ga('send', 'event', 'Timer', 'reset');
        this.resetTime();
        this.set('recipe._timerRunning', false);
        this.set('intervalSet', false);
        this.set('totalTime', this.get('inpTime'));
        this.set('passedTime', 0);
        this.set('initTime', null);
        return clearInterval(this.get('interval'));
      },
      add: function() {
        this.resetTime();
        this.set('totalTime', this.get('totalTime') + 60);
        return this.set('initTime', this.get('initTime') + 60);
      },
      sub: function() {
        if (this.get('ratio') !== 100) {
          this.resetTime();
          this.set('totalTime', this.get('totalTime') - 60);
          return this.set('initTime', this.get('initTime') - 60);
        }
      },
      remove: function() {
        window.ga && window.ga('send', 'event', 'Timer', 'remove');
        this.reset();
        return this.set('recipe._timerSet', false);
      },
      timeLeft: (function() {
        var tmp;
        tmp = this.get('totalTime') - this.get('passedTime');
        if (tmp < 0) {
          tmp = 0;
        }
        return tmp.displayTime();
      }).property('passedTime', 'initTime'),
      minLeft: (function() {
        var tmp;
        tmp = this.get('totalTime') - this.get('passedTime');
        if (tmp < 0) {
          tmp = 0;
        }
        tmp = Math.ceil(tmp / 60);
        return this.set('recipe._minLeft', tmp);
      }).observes('passedTime', 'initTime'),

      /*
      totalTimeLeft: (->
        tmp = Math.ceil(@get('totalTime')/60)
        if tmp < 0
          tmp = 0
        @set('recipe._totalTimeLeft',tmp)
      ).observes('totalTime')
       */
      ratio: (function() {
        var pass, tot;
        tot = this.get('totalTime');
        pass = this.get('passedTime');
        if (pass >= tot || tot === 0) {
          return 100;
        }
        if (pass === 0) {
          return 0.00001;
        }
        return (pass / tot) * 100;
      }).property('passedTime', 'initTime'),
      isTimerDone: (function() {
        var pass, tot;
        if (this.get('intervalSet')) {
          tot = this.get('totalTime');
          pass = this.get('passedTime');
          if (pass >= tot || tot === 0) {
            clearInterval(this.get('interval'));
            this.set('intervalSet', false);
            this.set('passedTime', this.get('totalTime'));
            this.timerFinished();
            return 100;
          }
        }
      }).observes('passedTime', 'initTime'),
      timerFinished: function() {
        this.set('recipe._timerRunning', false);
        window.ga && window.ga('send', 'event', 'Timer', 'finished');
        this.set('blinkInterval', setInterval(((function(_this) {
          return function() {
            return _this.set('blink', !_this.get('blink'));
          };
        })(this)), 400));
        this.set('blinkRunning', true);
        this.set('recipe._blinking', true);
        return setTimeout(((function(_this) {
          return function() {
            if (_this.get('blinkRunning')) {
              clearInterval(_this.get('blinkInterval'));
              _this.set('recipe._blinking', false);
              return _this.set('blink', true);
            }
          };
        })(this)), 6000);
      }
    });

    __exports__["default"] = Timer;
  });
define("v2/models/user", 
  ["v2/mixins/json-serializer","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var jsonmixin = __dependency1__["default"];


    function get_gravatar(md5, size) {

        // MD5 (Message-Digest Algorithm) by WebToolkit
        //

        var MD5=function(s){function L(k,d){return(k<<d)|(k>>>(32-d))}function K(G,k){var I,d,F,H,x;F=(G&2147483648);H=(k&2147483648);I=(G&1073741824);d=(k&1073741824);x=(G&1073741823)+(k&1073741823);if(I&d){return(x^2147483648^F^H)}if(I|d){if(x&1073741824){return(x^3221225472^F^H)}else{return(x^1073741824^F^H)}}else{return(x^F^H)}}function r(d,F,k){return(d&F)|((~d)&k)}function q(d,F,k){return(d&k)|(F&(~k))}function p(d,F,k){return(d^F^k)}function n(d,F,k){return(F^(d|(~k)))}function u(G,F,aa,Z,k,H,I){G=K(G,K(K(r(F,aa,Z),k),I));return K(L(G,H),F)}function f(G,F,aa,Z,k,H,I){G=K(G,K(K(q(F,aa,Z),k),I));return K(L(G,H),F)}function D(G,F,aa,Z,k,H,I){G=K(G,K(K(p(F,aa,Z),k),I));return K(L(G,H),F)}function t(G,F,aa,Z,k,H,I){G=K(G,K(K(n(F,aa,Z),k),I));return K(L(G,H),F)}function e(G){var Z;var F=G.length;var x=F+8;var k=(x-(x%64))/64;var I=(k+1)*16;var aa=Array(I-1);var d=0;var H=0;while(H<F){Z=(H-(H%4))/4;d=(H%4)*8;aa[Z]=(aa[Z]|(G.charCodeAt(H)<<d));H++}Z=(H-(H%4))/4;d=(H%4)*8;aa[Z]=aa[Z]|(128<<d);aa[I-2]=F<<3;aa[I-1]=F>>>29;return aa}function B(x){var k="",F="",G,d;for(d=0;d<=3;d++){G=(x>>>(d*8))&255;F="0"+G.toString(16);k=k+F.substr(F.length-2,2)}return k}function J(k){k=k.replace(/rn/g,"n");var d="";for(var F=0;F<k.length;F++){var x=k.charCodeAt(F);if(x<128){d+=String.fromCharCode(x)}else{if((x>127)&&(x<2048)){d+=String.fromCharCode((x>>6)|192);d+=String.fromCharCode((x&63)|128)}else{d+=String.fromCharCode((x>>12)|224);d+=String.fromCharCode(((x>>6)&63)|128);d+=String.fromCharCode((x&63)|128)}}}return d}var C=Array();var P,h,E,v,g,Y,X,W,V;var S=7,Q=12,N=17,M=22;var A=5,z=9,y=14,w=20;var o=4,m=11,l=16,j=23;var U=6,T=10,R=15,O=21;s=J(s);C=e(s);Y=1732584193;X=4023233417;W=2562383102;V=271733878;for(P=0;P<C.length;P+=16){h=Y;E=X;v=W;g=V;Y=u(Y,X,W,V,C[P+0],S,3614090360);V=u(V,Y,X,W,C[P+1],Q,3905402710);W=u(W,V,Y,X,C[P+2],N,606105819);X=u(X,W,V,Y,C[P+3],M,3250441966);Y=u(Y,X,W,V,C[P+4],S,4118548399);V=u(V,Y,X,W,C[P+5],Q,1200080426);W=u(W,V,Y,X,C[P+6],N,2821735955);X=u(X,W,V,Y