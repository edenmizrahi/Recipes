                _ref11.pushObject({});
              }
            }
          }
          if (typeof tmpA_parent !== "undefined" && tmpA_parent !== null) {
            if ((_ref12 = tmpA_parent.get("item")) != null) {
              if ((_ref13 = _ref12.subs) != null) {
                _ref13.popObject({});
              }
            }
          }
          if (tmpB != null) {
            if ((_ref14 = tmpB.get("item")) != null) {
              if ((_ref15 = _ref14.subs) != null) {
                _ref15.pushObject({});
              }
            }
          }
          if (tmpB != null) {
            if ((_ref16 = tmpB.get("item")) != null) {
              if ((_ref17 = _ref16.subs) != null) {
                _ref17.popObject({});
              }
            }
          }
          if ((_ref18 = tmpA.get("controller.model.stepsArr")) != null) {
            _ref18.pushObject({});
          }
          if ((_ref19 = tmpA.get("controller.model.stepsArr")) != null) {
            _ref19.popObject({});
          }
          if (typeof tmpA_parent !== "undefined" && tmpA_parent !== null) {
            if ((_ref20 = tmpA_parent.get("item._parent")) != null) {
              if ((_ref21 = _ref20.subs) != null) {
                _ref21.pushObject({});
              }
            }
          }
          if (typeof tmpA_parent !== "undefined" && tmpA_parent !== null) {
            if ((_ref22 = tmpA_parent.get("item._parent")) != null) {
