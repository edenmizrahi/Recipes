define("v2/views/index", 
  ["v2/mixins/bottom-reload","v2/mixins/freewall","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var BottomReloadMixin = __dependency1__["default"];
    var FreewallMixin = __dependency2__["default"];
    var ScrollableMixin, index;

    ScrollableMixin = Ember.Mixin.create({
      preservePos: (function() {
        var reallyLast;
        reallyLast = window.lastIndexPos;
        setTimeout(function() {
          return Ember.$(window).scrollTop(reallyLast != null ? reallyLast : 0);
        }, 5);
        if (!window.lastIndexPos) {
          this.$().css("min-height", document.documentElement.clientHeight - 10);
        }
        return $(window).bind('scroll', function() {
          return window.lastIndexPos = Ember.$(window).scrollTop();
        });
      }).on('didInsertElement'),
      stopPreservingPos: (function() {
        return $(window).unbind('scroll');
      }).on('willDestroyElement')
    });

    index = Ember.View.extend(ScrollableMixin, BottomReloadMixin, FreewallMixin);

    __exports__["default"] = index;;
  });
define("v2/views/preview", 
  ["v2/config/environment","v2/mixins/style-switch","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var V2ENV = __dependency1__["default"];

    var StyleSwitchMixin = __dependency2__["default"];
    var RecipePreview, copyToClipboard;

    copyToClipboard = function(text) {
      return window.prompt("Send this link to your friends (copy with ctrl + c)", text);
    };

    RecipePreview = Ember.View.extend(StyleSwitchMixin, {
      tagName: "div",
      style: (function() {
        return "background-image:url('" + this.get("controller.model.srcImgFull") + "')";
      }).property("controller.model.srcImg"),
      rerenderWatch: (function() {
        return this.rerender();
      }).observes("controller.rerenderSwitch"),
      didInsertElement: (function() {
        return this.set("controller.redraw", (new Date()).getTime());
      }),
      autoPrintActive: (function() {
        if (!this.get("controller.autoPrint")) {
          return;
        }
        if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
          alert("Firefox detected: To get the best print results turn on 'Print background colors'");
        }
        return setTimeout(function() {
          return print();
        }, 4000);
      }).on("didInsertElement"),
