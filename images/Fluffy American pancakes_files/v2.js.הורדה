        return self.set(preFix + "Result", 1 + self.get(preFix + "States").indexOf(currHeatState[0]));
      } else {
        return self.set(preFix + "Result", "-");
      }
    };

    handleSpinnerAction = function(self, p, preFix) {
      var o, preCheck, tmpEl, tmpIdx, tmpSvgEl;
      preCheck = p === self.get(preFix + "Result");
      removeStatesFromLst(self.get(preFix + "States"), self.get("item.attributes"));
