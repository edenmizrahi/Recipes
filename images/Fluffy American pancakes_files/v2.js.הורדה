        var size = size || 160;

        return 'http://www.gravatar.com/avatar/' + md5 + '.jpg?d=identicon&s=' + size;
        return 'http://robohash.org/' + md5
    }
    ;
    var User;

    window.get_gravatar = get_gravatar;

    User = Ember.Object.extend(jsonmixin, {
      email: "",
      id: 0,
      first_name: "",
      secretAT: "",
      last_name: "",
      gravatar: (function() {
        if (this.get("avatar")) {
          return this.get("avatar");
        }
        if (this.get("fbUser")) {
          return "http://graph.facebook.com/" + this.get("fbUser").id + "/picture";
        }
        return get_gravatar(this.get("md5"));
      }).property("email", "avatar"),
      name: (function() {
        if (this.get("first_name")) {
          return this.get("first_name");
        }
        if (this.get("last_name")) {
          return this.get("last_name");
        }
        if (this.get("email")) {
          return this.get("email");
        }
        return "Loading...";
      }).property("first_name", "last_name", "email"),
      isAdmin: (function() {
        if (this.get("email") === "marri@marri.is") {
          return true;
        }
        if (this.get("email") === "kai.koehn@gmx.de") {
          return true;
        }
        if (this.get("email") === "marinopv@gmail.com") {
          return true;
        }
        if (this.get("email") === "herdis.arnalds@gmail.com") {
          return true;
        }
        if (this.get("email") === "janusbirgisson@gmail.com") {
          return true;
        }
        if (this.get("email") === "arnalds3@gmail.com") {
          return true;
        }
        return false;
      }).property("email"),
      saveLocalStorage: function() {
        return localStorage["user"] = JSON.stringify(this.serialize());
      }
    });

    __exports__["default"] = User;
  });
define("v2/router", 
  ["v2/config/environment","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var V2ENV = __dependency1__["default"];

    var Router;

    Router = Ember.Router.extend({
      location: V2ENV.locationType
    });

    Router.map(function() {
      this.route('charl');
      this.resource('quick_view', {
        path: 'recipe/:recipe_id/quick_view'
      });
      this.route('search', {
        path: 'search/'
      });
      this.resource('cats', {
        path: 'cats/:cat_name'
      });
      this.resource("recipe", {
        path: "r/:gracipe_slug"
      });
      this.resource("preview", {
        path: "r/:gracipe_slug/preview"
      });
      this.resource("preview", {
        path: "hellofresh/:gracipe_slug"
      });
      this.resource("embed", {
        path: "r/:gracipe_slug/embed"
      });
      this.resource("recipe-url-forward", {
        path: "recipe/:recipe_id"
      });
      this.resource("recipe_edit4", {
        path: "recipe/:recipe_id/edit"
      });
      this.resource("recipe_edit", {
        path: "recipe/:recipe_id/edit4"
      });
      return this.resource('profile', {
        path: "profile/:user_id"
      }, function() {
        this.route("profile_me", {
          path: "gracipes"
        });
        this.route('profile_follow', {
          path: "social"
        });
        this.route("profile_bookmark", {
          path: "bookmark"
        });
        return this.route("profile_settings", {
          path: "settings"
        });
      });
    });

    Router.reopen({
      rootURL: V2ENV.rootURL
    });

    Router.reopen({
      notifyGoogleAnalytics: (function() {
        if (!window.ga) {
          return;
        }
        if (window.gaFirstTransmission) {
          window.gaFirstTransmission = false;
          return;
        }
        return window.ga("send", "pageview", {
          page: this.get("url"),
          title: this.get("url")
        });
      }).on("didTransition")
    });

    __exports__["default"] = Router;;
  });
define("v2/routes/application", 
  ["exports"],
  function(__exports__) {
    "use strict";
    var r;

    r = Ember.Route.extend({
      actions: {
        showModal: function(modalName) {
          var error;
          try {
            window.showModalWorkaround = false;
            this.controllerFor("application").set("modalActive", true);
            return this.render(modalName, {
              into: "application",
              outlet: "modal"
            });
          } catch (_error) {
            error = _error;
            window.showModalWorkaround = true;
            return this.render(modalName);
          }
        },
        doLogin: function() {
          var modalName;
          modalName = "modal-login";
          this.controllerFor(modalName).set("state", "login");
          return this.send("showModal", modalName);
        },
        doRegister: function() {
          var modalName;
          modalName = "modal-login";
          this.controllerFor(modalName).set("state", "register");
          return this.send("showModal", modalName);
        },
        hideModal: function() {
          this.controllerFor("application").set("modalActive", false);
          return this.disconnectOutlet({
            outlet: "modal",
            parentView: "application"
          });
        }
      }
    });

    __exports__["default"] = r;;
  });
define("v2/routes/cats", 
  ["ember","v2/utils/recipe-utils","v2/utils/recipe-request-resolver","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var recipeModel = __dependency2__["default"];

    var RecipeRequestController = __dependency3__["default"];

    var CatsRoute;

    CatsRoute = Ember.Route.extend({
      model: function(p) {
        var rndCtrl;
        rndCtrl = new RecipeRequestController();
        rndCtrl.opt = {
          size: 6,
          cat: [p.cat_name]
        };
        rndCtrl.getNext();
        rndCtrl.displayName = p.cat_name.charAt(0).toUpperCase() + p.cat_name.slice(1);
        return rndCtrl;
      }
    });

    __exports__["default"] = CatsRoute;
  });
