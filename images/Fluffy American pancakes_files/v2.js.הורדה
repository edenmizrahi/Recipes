            v.prm = setVal;
          }
          if (v.name === "timer") {
            retVal = v.prm;
          }
        }
        return {
          "hasTimer": retVal != null,
          "val": retVal
        };
      }).property("item.attributes.@each.prm"),
      calcOptimalTableNum: (function() {
        var hasSubSubs, nrOfSubs, retVal;
        return 2;
        if (!("subs" in this.item)) {
          return 0;
        }
        retVal = 0;
        hasSubSubs = 0;
        nrOfSubs = 0;
        $.each(this.item.subs, function(index, value) {
          if ((value.subs != null) && value.subs.length > 0) {
            hasSubSubs = 2;
            nrOfSubs = value.subs.length;
          } else {
            retVal += 2;
          }
        });
        if (hasSubSubs && nrOfSubs > 2) {
          return 4;
        } else {
          return Math.min(retVal, 2);
        }
        if (this.isLvl2 != null) {
          return Math.min(retVal, 2);
        }
        if (retVal > 4) {
          if ((retVal % 3) === 0) {
            return 3;
          }
          if ((retVal % 4) === 0) {
            return 4;
          }
          if ((retVal % 3) < (retVal % 4)) {
            return 4;
          }
          return 3;
        }
        return Math.min(retVal, 4);
      }).property("item.subs.@each"),
      textOnlyBG: (function() {
        if (!this.get("item")) {
          return false;
        }
        if ((!this.get("item").subs || this.get("item").subs.length === 0) && !!this.get("item.instructions")) {
          return true;
        }
        return false;
      }).property("item"),
      showBG: (function() {
        if (!this.get("item")) {
          return false;
        }
        if (!this.get("controller.viewOnly")) {
          return true;
        }
        if ((!this.get("item").subs || this.get("item").subs.length === 0) && !this.get("item.instructions")) {
          return false;
        }
        return true;
      }).property("item"),
      actions: {
        removeItem: function() {
          return this.get("item").destroy();
        },
        showAttribute: function(item, jqSelector) {
          var $jqElement, currItem;
          if (this.get("controller.viewOnly")) {
            return;
          }
          currItem = item != null ? item : this.get("item");
          if (jqSelector) {
            $jqElement = this.$().find(jqSelector);
          } else {
            $jqElement = this.$();
          }
          if (this.get("controller.currAttrib.item")) {
            return this.set("controller.currAttrib.item", null);
          } else {
            return this.get("controller").send("showAttrib", currItem, $jqElement, this);
          }
        },
        removeResult: function() {
          var idx;
          if ((idx = this.get("item.isFollowedByResult")) !== -1) {
            this.get("item._parent.subs." + idx).destroy();
            return this.set("controller.redraw", this.get("controller.redraw") + 1);
          }
        },
        toggleElement: function() {
          var index, stuff, tmpO;
          window.ga && window.ga('send', 'event', 'Editor', 'sideProcess', "intoSideprocess");
          index = 0;
          stuff = this.get("controller.model.stepsArr");
          if (!stuff) {
            return;
          }
          index = stuff.indexOf(this.get("item"));
          tmpO = this.get("controller.model").createStep({
            el: "side_template",
            side: "side",
            subproc: true,
            subs: [this.item]
          });
          stuff.insertAt(index, tmpO);
          stuff.removeObject(this.item);
          return this.set("controller.redraw", this.get("controller.redraw") + 1);
        }
      }
    });

    __exports__["default"] = Step;;
  });
define("v2/views/tfview", 
  ["v2/utils/recipe-editor-search-resolver","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var nodeDis = __dependency1__["default"];

    var V;

    V = Ember.TextArea.extend({
      classNames: ['ingredInput'],
      elementId: "theSearchPlaceholder",
      didInsertElement: function() {
        this.$().on('paste', (function(_this) {
          return function() {
            _this.pasted = 1;
            return window.ga && window.ga('send', 'event', 'Editor', 'search', 'paste');
          };
        })(this));
        return this.$().on("keydown", (function(_this) {
          return function(e) {
            if (e.keyCode === 13) {
              window.ga && window.ga('send', 'event', 'Editor', 'search', 'enter');
              return _this.$().val(_this.$().val() + "\n");
            }
          };
        })(this));
      },
      willDestroyElement: function() {
        return this.$().off();
      },
      valueDidChange: (function() {
        var i, lastLine, linesArr, _i, _len;
        linesArr = this.get("value").split(/\n/);
        lastLine = linesArr.pop();
        for (_i = 0, _len = linesArr.length; _i < _len; _i++) {
          i = linesArr[_i];
          if (i.match(/^\s*$/)) {
            continue;
          }
          if (!this.get("retVal")) {
            this.set("retVal", []);
          }
          nodeDis.createNode(i.toLowerCase());
        }
        if (this.pasted && !lastLine.match(/^\s*$/)) {
          nodeDis.createNode(lastLine.toLowerCase());
          this.pasted = 0;
          lastLine = "";
        }
        return this.set("value", lastLine);
      }).observes('value')
    });

    __exports__["default"] = V;;
  });
