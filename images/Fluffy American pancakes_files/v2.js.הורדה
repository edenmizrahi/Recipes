    window.GUnitLst = [" ", "bag", "balls", "block", "bunch", "bundle", "can", "centimeter", "clove", "cup", "deciliter", "gram", "handful", "jar", "kilogram", "liter", "milliliter", "ounce", "pack", "piece", "pinch", "pint", "pot", "pound", "punnet", "sheet", "slice", "sprig", "stalk", "tablespoon", "teaspoon"];

    ModalAttributeComponent = Ember.Component.extend({
      needs: ["index", "gracipe"],
      classNames: ["modal-attribute"],
      attributeBindings: ['style'],
      lstAttributes: window.GLstAttributes,
      elLstAttributes: window.GLstElAttr,
      unitLst: window.GUnitLst,
      unitLstPlural: window.GUnitLstPlural,
      _usePlural: true,
      showAdvanced: false,
      unitPlural: (function() {
        if (this.get('item.amount') === '1') {
          return false;
        }
        return true;
      }).property('item.amount'),
      itemWatch: (function() {
        if (!this.get("item")) {
          return;
        }
        if (this.get("item.unitDescr") === "") {
          if (this.get("item.svgObj.defaultUnit")) {
            return this.set("item.unitDescr", this.get("item.svgObj.defaultUnit"));
          }
        }
      }).observes("item", "item.svgObj.defaultUnit"),
      gfycatWatch: (function() {
        var gfyId;
        gfyId = this.get("item.gfycat");
        if (gfyId === "") {
          this.set("uploadInProgress", false);
          return;
        }
        this.set("uploadInProgress", true);
        return Ember.$.get('https://gfycat.com/cajax/get/' + gfyId, (function(_this) {
          return function(data) {
            _this.set("uploadInProgress", false);
            if (data && data.gfyItem) {
              _this.set("item.webmReference", data.gfyItem.mp4Url);
              _this.set("item.picReference", 'http://thumbs.gfycat.com/' + _this.get("item.gfycat") + '-poster.jpg');
            }
          };
        })(this));
      }).observes("item.gfycat"),
      style: (function() {
        var $jqElement, e, preHeight, preScrollX, preScrollY, preWidth, tLeft, tTop;
        if (!this.get("item")) {
          return "display:none";
        }
        $jqElement = this.get("jq");
        try {
          if ($jqElement.parent().find('.rIconEdit').length !== 0) {
            tLeft = Math.floor($jqElement.parent().find('.rIconEdit').offset().left - this.$().width() - 40);
            tTop = Math.floor($jqElement.parent().find('.rIconEdit').offset().top - 40);
          } else {
            tLeft = Math.floor($jqElement.offset().left - this.$().width() - 40);
            tTop = Math.floor($jqElement.offset().top - 40);
          }
          preWidth = document.body.clientWidth;
          preHeight = document.body.clientHeight;
          preScrollX = window.scrollX;
          preScrollY = window.scrollY;
          Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              _this.$().find("input").first().select();
              _this.$().find("input").first().focus();
              tLeft = Math.max(preScrollX, tLeft);
              tLeft = Math.min(preScrollX + window.innerWidth - _this.$().width(), tLeft);
              tTop = Math.max(preScrollY, tTop);
              tTop = Math.min(preScrollY + window.innerHeight - _this.$().height(), tTop);
              _this.$().css({
                left: tLeft,
                top: tTop
              });
              return "left:" + tLeft + "px;top:" + tTop + "px";
            };
          })(this));
          return "left:" + tLeft + "px;top:" + tTop + "px";
        } catch (_error) {
          e = _error;
          console.warn(e);
          return "display:none";
        }
      }).property("item"),
      isSingle: (function() {
        if (this.get('item.isTool')) {
          return false;
        }
        return !this.get("item.isMainStep");
      }).property("item.isMainStep", 'item.isTool'),
      click: function() {
        var pc;
        pc = this.get("parentView.controller");
        return pc.set("currAttrib.ignoreClick", true);
      },
      keyDown: function(e) {
        if (e.keyCode === 27 || e.keyCode === 13) {
          return this.set("item", null);
        }
      },
      nextItemIsResult: (function() {
        var ind, item, pArr;
        item = this.get('item');
        if (item) {
          if (item.isResult) {
            return false;
          }
          pArr = item.get('_parent.subs');
          ind = pArr[pArr.indexOf(item) + 1];
          if (ind) {
            if (ind.isResult) {
              return true;
            }
          }
        }
        return false;
      }).property('item'),
      resetAdvanced: (function() {
        if (this.get('item') === null) {
          return this.set('showAdvanced', false);
        }
      }).observes('item'),
      showAddPicture: (function() {
        if (this.get('item.isResult') || this.get('showAdvanced')) {
          return true;
        }
        return false;
      }).property('item', 'showAdvanced'),
      actions: {
        removeItem: function() {
          return this.get("item").destroy();
        },
        openElementSearch: function() {
          if (this.get("item.svgRefOverwrite")) {
            return this.set("exchangeObject.modalSearchElementRef", this.get("item.svgRefOverwrite"));
          } else {
            return this.set("exchangeObject.modalSearchElementRef", this.get("item"));
          }
        },
        deleteAttr: function(attr) {
          return this.get("item.attributes").removeObject(attr);
        },
        addResult: function() {
          this.get("parentView.controller").send("addResultAfterItem", this.get("item"));
          return Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              var resultItem;
              resultItem = _this.get("item.getNextSiblingResult");
              return _this.get('parentView.controller').send('showAttrib', resultItem, _this.get("jq").closest(".step").find(".result"));
            };
          })(this));
        },
        showProperties: function() {
          console.log(this.get('showAdvanced'));
          return this.set('showAdvanced', !this.get('showAdvanced'));
        },
        removePicture: function() {
          this.set("item.gfycat", void 0);
          this.set("item.picReference", void 0);
          return this.set("item.picReferenceId", void 0);
        },
        addPicture: function() {
          var $modalFileUploader;
          $modalFileUploader = this.$().find('#modal-attrib-fileupload');
          $modalFileUploader.unbind("change");
          $modalFileUploader.bind("change", (function(_this) {
            return function(event) {
              var files;
              files = event.target.files;
              _this.set("uploadInProgress", true);
              return RUtils.uploadFiles(files, function(newURL) {
                _this.set("uploadInProgress", false);
                console.log(newURL);
                _this.set("item.picReference", newURL.filePath);
                return _this.set("item.picReferenceId", newURL.id);
              });
            };
          })(this));
          return $modalFileUploader.click();
        },
        close: function() {
          return this.set("item", null);
        }
      }
    });

    __exports__["default"] = ModalAttributeComponent;;
  });
define("v2/components/modal-elementpicker", 
  ["ember","v2/utils/ajax-resolver","v2/utils/utils","v2/utils/recipe-editor-search-resolver","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ajax = __dependency2__["default"];

    var UTILS = __dependency3__["default"];

    var nodeDis = __dependency4__["default"];

    var ModalElementpickerComponent, arrowkeysKeyCodeLUT, defaultIngred, defaultTools;

    arrowkeysKeyCodeLUT = {
      left: 37,
      up: 38,
      right: 39,
      down: 40,
      esc: 27,
      enter: 13
    };

    defaultTools = [];

    defaultIngred = [];

    ModalElementpickerComponent = Ember.Component.extend({
      container: null,
      index: null,
      isTools: 1,
      cursor: null,
      q: "",
      getSvgsById: function(ids) {
        var resArr;
        resArr = [];
        ajax.getSymbols({
          svgs: JSON.stringify(ids)
        }, (function(_this) {
          return function(data) {
            var k, tmpO, v, _results;
            if (data) {
              _results = [];
              for (k in data) {
                v = data[k];
                tmpO = {
                  id: k,
                  svg: v.svg,
                  name: v.name
                };
                tmpO = Ember.Object.create(tmpO);
                _results.push(resArr.addObject(tmpO));
              }
              return _results;
            }
          };
        })(this));
        return resArr;
      },
      defaultTools: [],
      defaultIngreds: [],
      initDefaults: (function() {
        console.log('initDefaults start');
        this.set('defaultTools', this.getSvgsById([21, 252, 360, 10037, 10064, 10014, 292, 283, 2, 284, 240, 10154, 238, 25, 10073, 308, 10116, 24, 239, 426, 10108, 225, 10083, 10063, 216, 255, 10039, 27, 10038, 10141, 10084, 325, 10081, 212, 230, 208]));
        this.set('defaultIngreds', this.getSvgsById([17, 45, 264, 146, 19, 75, 60, 163, 29, 28, 227, 54]));
        return console.log('initDefaults done');
      }).on('init'),
      defaults: (function() {
        var res;
        if (this.get('isTools')) {
          res = this.get('defaultTools');
        } else {
          res = this.get('defaultIngreds');
        }
        return res;
      }).property("isTools", 'defaultIngreds', 'defaultTools'),
      searchResults: [],
      searchResultsWithMarkersWatch: (function() {
        var i, x, _i, _len, _ref, _results;
        _ref = this.get("searchResults");
        _results = [];
        for (i = _i = 0, _len = _ref.length; _i < _len; i = ++_i) {
          x = _ref[i];
          _results.push(x.set("cursorActive", i === this.get("cursor")));
        }
        return _results;
      }).observes("searchResults.@each", "cursor"),
      isVisible: (function() {
        return !!this.get("container");
      }).property("container"),
      isVisibleWatch: (function() {
        if (this.get("isVisible")) {
          this.set("cursor", 0);
          return Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              _this.$().find("input").select();
              return _this.$().find("input").focus();
            };
          })(this));
        }
      }).observes("isVisible"),
      cursorKeyDownHandler: (function(_this) {
        return function(e) {
          var ctrlRef, nextCursor, prevCursor, selectedCursor, selectedItem, _ref;
          ctrlRef = e.data.c;
          if (!ctrlRef.get("isVisible")) {
            return;
          }
          if (e.keyCode === arrowkeysKeyCodeLUT.enter) {
            selectedCursor = ctrlRef.get("cursor");
            selectedItem = ctrlRef.get("searchResults." + selectedCursor);
            if (selectedItem) {
              return ctrlRef.send("selectElement", selectedItem);
            }
          } else if (e.keyCode === arrowkeysKeyCodeLUT.down) {
            ctrlRef.set("cursor", 0);
            e.preventDefault();
            return ctrlRef.$().find("input").blur();
          } else if (e.keyCode === arrowkeysKeyCodeLUT.up) {
            e.preventDefault();
            console.log("unbinding event...");
            ctrlRef.set("cursor", null);
            return Ember.run.scheduleOnce('afterRender', function() {
              ctrlRef.$().find("input").select();
              return ctrlRef.$().find("input").focus();
            });
          } else if (e.keyCode === arrowkeysKeyCodeLUT.right) {
            nextCursor = Math.min((_ref = ctrlRef.get("searchResults.length") - 1) != null ? _ref : 0, ctrlRef.get("cursor") + 1);
            return ctrlRef.set("cursor", nextCursor);
          } else if (e.keyCode === arrowkeysKeyCodeLUT.left) {
            prevCursor = Math.max(0, ctrlRef.get("cursor") - 1);
            return ctrlRef.set("cursor", prevCursor);
          } else if (e.keyCode === arrowkeysKeyCodeLUT.esc) {
            return ctrlRef.send("close");
          } else {
            ctrlRef.set("currentlySearching", true);
            return ctrlRef.get("_debounceFcn")();
          }
        };
      })(this),
      willDestroyElement: function() {
        return $(document).unbind("keydown", this.cursorKeyDownHandler);
      },
      didInsertElement: function() {
        var debounceFcn;
        $(document).bind("keydown", {
          c: this
        }, this.cursorKeyDownHandler);
        debounceFcn = UTILS.debounce((function(_this) {
          return function() {
            return _this.send("searchNow");
          };
        })(this), 500);
        return this.set("_debounceFcn", debounceFcn);
      },
      actions: {
        close: function() {
          var arr, recipeSteps;
          recipeSteps = this.get('ctrl.pubSteps');
          if (recipeSteps) {
            if (recipeSteps[recipeSteps.length - 1]['subs'].length === 0 && recipeSteps[recipeSteps.length - 1]['side'] === 'side') {
              this.set("ctrl.redraw", (new Date()).getTime());
              arr = this.get('model.stepsArr');
              arr[arr.length - 1].destroy();
              console.log("Step didn't contain any instructions so it was destroyed.");
            }
          }
          this.set("container", null);
          this.set("q", "");
          this.set("searchResults", []);
          return this.set("ctrl.redraw", (new Date()).getTime());
        },
        selectElement: function(prm) {
          var tmp;
          if (this.get('createNodeOnly')) {
            nodeDis.createNode(prm.name.toLowerCase());
            this.set("container", null);
            this.set("q", "");
            this.set("searchResults", []);
            return;
          }
          tmp = this.get("model").createStep(prm);
          if (this.get("index") !== void 0 && this.get("container.length") > this.get("index") - 1) {
            this.get("container").insertAt(this.get("index") + 1, tmp);
          } else {
            this.get("container").pushObject(tmp);
          }
          this.set("container", null);
          this.set("q", "");
          this.set("searchResults", []);
          if (!this.get("isTools")) {
            setTimeout((function(_this) {
              return function() {
                var jq;
                jq = _this.get("viewRef").$().find(".ingred-single-container .ingred_container").last();
                _this.get("ctrl").send("showAttrib", tmp, jq);
                return _this.set("ctrl.currAttrib.ignoreClick", false);
              };
            })(this), 100);
          }
          this.send("close");
          return setTimeout((function(_this) {
            return function() {
              return _this.set("ctrl.redraw", (new Date()).getTime());
            };
          })(this), 100);
        },
        searchNow: function() {
          this.set("cursor", null);
          this.set("currentlySearching", true);
          return ajax.getSymbols({
            q: this.get("q")
          }, (function(_this) {
            return function(data) {
              var k, tmpO, v, _results;
              _this.set("currentlySearching", false);
              if (data) {
                _this.get("searchResults").clear();
                _results = [];
                for (k in data) {
                  v = data[k];
                  tmpO = {
                    id: k,
                    svg: v.svg,
                    name: v.name
                  };
                  tmpO = Ember.Object.create(tmpO);
                  _results.push(_this.get("searchResults").addObject(tmpO));
                }
                return _results;
              }
            };
          })(this));
        }
      }
    });

    __exports__["default"] = ModalElementpickerComponent;
  });
define("v2/components/modal-image-view", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ModalImageViewComponent;

    ModalImageViewComponent = Ember.Component.extend({
      isVisible: (function() {
        var retVal;
        retVal = !!this.get("imageModalSrc");
        if (retVal === true && this.get("isVideo")) {
          Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              var _ref;
              if ((_ref = _this.$().find("#modal-video").get(0)) != null) {
                _ref.play();
              }
              return _this.$().find("#modal-video").get(0).addEventListener('webkitendfullscreen', function() {
                return _this.set("imageModalSrc", void 0);
              }, false);
            };
          })(this));
        }
        return retVal;
      }).property("imageModalSrc"),
      isVideo: (function() {
        var m, tmpSrcString;
        tmpSrcString = "" + (this.get("imageModalSrc"));
        m = tmpSrcString.match(/\.(webm|mp4|ogg)($|\?)/);
        if (!m) {
          return void 0;
        }
        return m[1];
      }).property("imageModalSrc", "isVisible"),
      actions: {
        close: function() {
          return this.get("delegate").send("showModalImage", null);
        }
      }
    });

    __exports__["default"] = ModalImageViewComponent;
  });
define("v2/components/modal-search-element", 
  ["v2/utils/ajax-resolver","v2/models/svg-element","v2/utils/utils","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var ajax = __dependency1__["default"];

    var svgElement = __dependency2__["default"];

    var UTILS = __dependency3__["default"];

    var ModalSearchElementComponent;

    ModalSearchElementComponent = Ember.Component.extend({
      applicationController: null,
      owVisible: false,
      elementSet: Ember.A([]),
      elementRef: null,
      firstSearch: true,
      q: "",
      didInsertElement: function() {
        var debounceFcn;
        debounceFcn = UTILS.debounce((function(_this) {
          return function() {
            return _this.send("searchNow");
          };
        })(this), 500);
        setTimeout((function(_this) {
          return function() {
            _this.$().find("input").select();
            return _this.$().find("input").focus();
          };
        })(this), 100);
        return this.$().find("input").on("keydown", (function(_this) {
          return function(e) {
            if (e.keyCode === 27) {
              _this.send("close");
            }
            return debounceFcn();
          };
        })(this));
      },
      elementId: 'gracipe_search_element',
      isVisible: (function() {
        return !!this.get("elementRef");
      }).property("elementRef"),
      elementRefWatch: (function() {
        if (!this.get("elementRef")) {
          return;
        }
        this.set("q", this.get("elementRef.name"));
        this.send("searchNow");
        return Ember.run.scheduleOnce('afterRender', (function(_this) {
          return function() {
            _this.$().find("input").select();
            return _this.$().find("input").focus();
          };
        })(this));
      }).observes("elementRef"),
      click: function() {
        return this.set("exchangeObject.ignoreClick", true);
      },
      actions: {
        close: function() {
          return this.set("elementRef", null);
        },
        selectElement: function(prm) {
          var el;
          el = this.get("elementRef");
          el.set("id", prm.id);
          el.notifyPropertyChange("svgObj.id");
          el.notifyPropertyChange("svgObj");
          return this.send("close");
        },
        searchNow: function() {
          this.set("currentlySearching", true);
          return ajax.getSymbols({
            q: this.get("q")
          }, (function(_this) {
            return function(data) {
              var k, tmpO, v, _results;
              _this.set("firstSearch", false);
              _this.set("currentlySearching", false);
              if (data) {
                _this.get("elementSet").clear();
                _results = [];
                for (k in data) {
                  v = data[k];
                  tmpO = {
                    id: k,
                    svg: v.svg,
                    prm: v.name
                  };
                  _results.push(_this.get("elementSet").addObject(tmpO));
                }
                return _results;
              }
            };
          })(this));
        }
      }
    });

    __exports__["default"] = ModalSearchElementComponent;
  });
define("v2/components/page-flip", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var Page, PageFlip;

    Page = function(view) {
      this.view = view;
      this.margin = 3;
      this.max = this.getId() + this.margin;
      this.min = this.getId() - this.margin;
      this.spawn();
      return $(document).keydown((function(_this) {
        return function(e) {
          return _this.onKey(e.keyCode);
        };
      })(this));
    };

    Page.prototype = {
      spawn: function() {},
      onKey: function(key) {
        if (key === 74 || key === 37) {
          return this.getPage(this.getId() - 1);
        } else if (key === 75 || key === 39) {
          return this.getPage(this.getId() + 1);
        }
      },
      getPage: function(id) {
        var path;
        path = window.location.origin + "/recipe/" + id;
        return window.location.href = path;
      },
      getId: function() {
        return parseInt(window.location.pathname.split("/")[2], 10);
      }
    };

    PageFlip = Ember.Component.extend({
      tagName: 'div',
      classNames: ['page'],
      willInsertElement: (function() {}),
      didInsertElement: (function() {
        var page;
        return page = new Page(this);
      }),
      willDestroyElement: (function() {})
    });

    __exports__["default"] = PageFlip;
  });
define("v2/components/recipe-component", 
  ["ember","v2/mixins/style-switch","v2/models/timer","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var StyleSwitchMixin = __dependency2__["default"];
    var Timer = __dependency3__["default"];

    var RecipeComponentComponent;

    RecipeComponentComponent = Ember.Component.extend(StyleSwitchMixin, {
      classNames: ['recipe-component'],
      unit: "us",
      didInsertElement: function() {
        var _ref;
        if ((_ref = this.get("unit")) !== "us" && _ref !== "metric") {
          return this.set("unit", "metric");
        }
      },
      isUnitMetric: Ember.computed("unit", function() {
        return this.get("unit") === "metric";
      }),
      isUnitUS: Ember.computed("unit", function() {
        return this.get("unit") === "us";
      }),
      viewOnly: true,
      activeStyle: "arrow",
      timerArray: [],
      recipeReady: Ember.computed("recipe", function() {
        var tmpResult;
        tmpResult = this.get("recipe.title") !== void 0;
        if (tmpResult) {
          this.sendAction("recipeReadyEvent");
        }
        return tmpResult;
      }),
      style: (function() {
        return "background-image:url('" + this.get("recipe.srcImgFull") + "')";
      }).property("recipe.srcImg"),
      formatted: (function() {
        var colorLUT, error, idx, newString, tmpArr, tmpColor, tmpStr, val, _i, _len;
        tmpStr = this.get("recipe.title");
        console.log(tmpStr);
        tmpArr = [];
        try {
          tmpArr = tmpStr.split(" ");
        } catch (_error) {
          error = _error;
          return this.get("recipe.title");
        }
        colorLUT = ["#008186", "#BBB648", "#D9816C"];
        newString = "";
        for (idx = _i = 0, _len = tmpArr.length; _i < _len; idx = ++_i) {
          val = tmpArr[idx];
          console.log("-" + val);
          tmpColor = colorLUT[idx % colorLUT.length];
          newString += " <span style='color: " + tmpColor + "'>" + val.toUpperCase() + "</span>";
        }
        return newString;
      }).property("recipe.title"),
      actions: {
        addTimer: function(time, text, stepNum, recipe) {
          var i, match, timer, _i, _len, _ref;
          _ref = this.get('timerArray');
          for (_i = 0, _len = _ref.length; _i < _len; _i++) {
            i = _ref[_i];
            if (i.stepNumber === stepNum) {
              console.log('Timer already spawned');
              i.startToggle();
              return;
            }
          }
          match = time.match(/\d+\.?\d*/);
          if (match === null) {
            time = 5;
          } else {
            time = +match;
          }
          timer = new Timer;
          timer.set('recipe', recipe);
          timer.initTimer(60 * time);
          timer.set('stepNumber', stepNum);
          if (text === void 0) {
            timer.set('stepTitle', '');
          } else {
            timer.set('stepTitle', String(text));
          }
          this.get('timerArray').pushObject(timer);
          if (match !== null) {
            return timer.runFirst();
          }
        },
        showModalImage: function(src) {
          return this.set("imageModalSrc", src);
        },
        testClick: function() {
          return console.log("helloooojoijoijo");
        },
        changeServingSize: function(num) {
          num = +num;
          return this.set("recipe.servings", Math.max(this.get("recipe.servings") + num, 1));
        },
        setUnit: function(u) {
          var allConversion, selectedConversion;
          allConversion = this.get("recipe.conversion.availConv");
          selectedConversion = allConversion.filter(function(i) {
            return i["id"] === u;
          });
          if (selectedConversion.length > 0) {
            this.set("recipe.conversion.currConv", selectedConversion[0]);
          }
          return this.set("unit", u);
        }
      }
    });

    __exports__["default"] = RecipeComponentComponent;
  });
define("v2/components/recipe-of-day", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var RecipeOfDayComponent;

    RecipeOfDayComponent = Ember.Component.extend({
      showFBLikeButtons: true,
      showEditorButtons: false,
      expanded: false,
      cssBorderWidth: void 0,
      isOverflowing: false,
      didInsertElement: function() {
        var heightForExpanded;
        this.set("cssBorderWidth", this.$().find(".hello-middle").css("border-width"));
        heightForExpanded = this.getHeightForExpanded(this.get("expanded"));
        this.changeHeight(heightForExpanded, false);
        this.$().find(".hello-left").height(heightForExpanded);
        if (this.get("showFBLikeButtons")) {
          return FB.XFBML.parse();
        }
      },
      getHeightForExpanded: function(isExpanded) {
        var isMobile;
        if (isExpanded) {
          isMobile = Ember.$(window).width() <= 768;
          return this.$().find("#nice-preview").height() + (isMobile ? 200 : 100);
        } else {
          return Math.min(1000, Math.max(600, Ember.$(window).height() - 151));
        }
      },
      changeHeight: function(h, shouldAnimate) {
        var scrollToPosition;
        if (shouldAnimate) {
          this.$().find(".hello-component").animate({
            height: h
          }, {
            queue: false
          });
          if (this.get("expanded")) {
            return console.log("no scroll");
          } else {
            scrollToPosition = this.$().find(".hello-component").position().top;
            return Ember.$("html, body").animate({
              scrollTop: scrollToPosition
            }, '500', 'swing');
          }
        } else {
          return this.$().find(".hello-component").height(h);
        }
      },
      actions: {
        recipeReady: function() {
          return Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              _this.set("isOverflowing", _this.getHeightForExpanded(false) >= _this.getHeightForExpanded(true));
              return _this.$().find("");
            };
          })(this));
        },
        showMoreGracipes: function() {
          var scrollToPosition;
          scrollToPosition = Ember.$("#container").position().top;
          return Ember.$("html, body").animate({
            scrollTop: scrollToPosition
          }, '500', 'swing');
        },
        makeOwnGracipe: function() {
          return this.get("delegate").transitionToRoute('recipe_edit4', 'new');
        },
        toggleSize: function() {
          var newHeight;
          this.toggleProperty("expanded");
          newHeight = this.getHeightForExpanded(this.get("expanded"));
          return this.changeHeight(newHeight, true);
        }
      }
    });

    __exports__["default"] = RecipeOfDayComponent;
  });
define("v2/components/timers-holder", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var TimersHolderComponent;

    TimersHolderComponent = Ember.Component.extend({
      newestTimer: null,
      originalTitle: document.title,
      titleSetter: (function() {
        return this.set('newestTimer', this.get('timerArray.lastObject'));
      }).observes('timerArray.@each'),
      titleCounter: (function() {
        var timeLeft;
        timeLeft = this.get('newestTimer.timeLeft');
        document.title = this.get('originalTitle') + ' - Time left: ' + timeLeft;
        if (timeLeft === '0m00s') {
          return document.title = this.get('originalTitle');
        }
      }).observes('newestTimer.timeLeft'),
      actions: {
        removeTimer: function(timer) {
          timer.remove();
          if (timer === this.get('newestTimer')) {
            document.title = this.get('originalTitle');
          }
          return this.get('timerArray').removeObject(timer);
        }
      }
    });

    __exports__["default"] = TimersHolderComponent;
  });
