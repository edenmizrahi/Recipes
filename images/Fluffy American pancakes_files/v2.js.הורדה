define("v2/views/cats", 
  ["exports"],
  function(__exports__) {
    "use strict";
    var CatsView;

    CatsView = Ember.View.extend({
      didInsertElement: (function() {
        this.$().css("min-height", document.documentElement.clientHeight - 10);
        return $(window).bind('scroll', (function(_this) {
          return function() {
            if ($(window).scrollTop() + window.outerHeight > 0.8 * $(document).height() || _this.$().height() <= document.documentElement.clientHeight) {
              return _this.get("controller.model").getNext();
            }
          };
        })(this));
      }),
      willDestroyElement: function() {
        $(window).unbind('scroll');
        return 1;
      }
    });

    __exports__["default"] = CatsView;;
  });
define("v2/views/disqus", 
  ["ember","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ENV = __dependency2__["default"];

    var DisqusView, disqusLogin;

    disqusLogin = function(model, c) {
      var tmpDis;
      tmpDis = c.get("user.disqus");
      if (c.get("loggedIn") && tmpDis) {
        window.disqus_config = function() {
          this.page.remote_auth_s3 = tmpDis.message + " " + tmpDis.sig + " " + tmpDis.timestamp;
          return this.page.api_key = tmpDis.pub_key;
        };
      } else {
        window.disqus_config = function() {
          this.page.remote_auth_s3 = "";
          return this.page.api_key = "";
        };
      }
      window.disqus_shortname = 'gracipe';
      window.disqus_identifier = 'recipe_' + model.id;
      window.disqus_title = model.title;
      return window.disqus_url = ENV.rootURL + '/recipe/' + model.id;
    };

    DisqusView = Ember.View.extend({
      elementId: 'disqus_thread',
      rerenderOnLogin: (function() {
        console.log("fooooo");
        disqusLogin(this.get("controller.model"), this.get("controller.controllers.application"));
        return DISQUS.reset({
          reload: true,
          config: window.disqus_config
        });
      }).observes("controller.controllers.application.user.disqus"),
      didInsertElement: (function() {
        disqusLogin(this.get("controller.model"), this.get("controller.controllers.application"));
        return (function() {
          var dsq;
          dsq = document.createElement("script");
          dsq.type = "text/javascript";
          dsq.async = true;
          dsq.src = "//" + disqus_shortname + ".disqus.com/embed.js";
          (document.getElementsByTagName("head")[0] || document.getElementsByTagName("body")[0]).appendChild(dsq);
        })();
      })
    });

    __exports__["default"] = DisqusView;
  });
