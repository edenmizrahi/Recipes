define("v2/tests/unit/components/recipe-component-test", 
  ["ember-qunit","v2/utils/recipe-request-resolver","v2/utils/recipe-utils","v2/tests/fixture-r1","v2/tests/fixture-r2"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __dependency5__) {
    "use strict";
    var test = __dependency1__.test;
    var moduleForComponent = __dependency1__.moduleForComponent;
    var RecipeRequestController = __dependency2__["default"];

    var recipeModel = __dependency3__["default"];

    var r1json = __dependency4__["default"];
    var r2json = __dependency5__["default"];
    var getTestRecipe;

    moduleForComponent('recipe-component', 'RecipeComponentComponent', {});

    getTestRecipe = function(rSelection, overridePreptime, removeTimers) {
      var tmpRecipe;
      switch (rSelection) {
        case "r1":
          tmpRecipe = recipeModel.adapter(r1json.recipeArray[0]);
          break;
        case "r2":
          tmpRecipe = recipeModel.adapter(r2json.recipeArray[0]);
      }
      if (overridePreptime) {
        tmpRecipe.set("preptime", overridePreptime);
      }
      if (removeTimers) {
        delete tmpRecipe.set("stepsArr.0.attributes", []);
        delete tmpRecipe.set("stepsArr.1.subs.0.attributes", []);
      }
      return tmpRecipe;
    };

    test('ingredient count', function() {
      expect(1);
      return equal(getTestRecipe("r2").get("nrIngredients"), 3);
    });

    test('tool count', function() {
      expect(1);
      return equal(getTestRecipe("r2").get("nrTools"), 2);
    });

    test('time count - normal', function() {
      expect(2);
      equal(getTestRecipe("r2").get("nrMinutesCalculated"), 19);
      return equal(getTestRecipe("r2").get("nrMinutes"), 20);
    });

    test('time count - normal - complex recipe', function() {
      expect(2);
      equal(getTestRecipe("r1").get("nrMinutesCalculated"), 14);
      return equal(getTestRecipe("r1").get("nrMinutes"), 15);
    });

    test('time count - no numbers, show < 10 as result', function() {
      expect(1);
      return equal(getTestRecipe("r2", 0, true).get("nrMinutes"), "< 10");
    });

    test('time count - override preptime prm', function() {
      expect(1);
      return equal(getTestRecipe("r2", 9, false).get("nrMinutes"), 9);
    });

    test('it renders', function() {
      var component;
      expect(2);
      component = this.subject();
      equal(component.state, 'preRender');
      this.append();
      return equal(component.state, 'inDOM');
    });
  });
