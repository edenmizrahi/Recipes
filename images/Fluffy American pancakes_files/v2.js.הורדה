        var allConversion, selectedConversion, u;
        u = this.get("controller.unit");
        if (u) {
          allConversion = this.get("controller.conversion.availConv");
          selectedConversion = allConversion.filter(function(i) {
            return i["id"] === u;
          });
          if (selectedConversion.length > 0) {
            return this.set("controller.conversion.currConv", selectedConversion[0]);
          }
        }
      }).on("didInsertElement"),
      formatted: (function() {
        var colorLUT, error, idx, newString, tmpArr, tmpColor, tmpStr, val, _i, _len;
        tmpStr = this.get("controller.model.title");
        console.log(tmpStr);
        tmpArr = [];
        try {
          tmpArr = tmpStr.split(" ");
        } catch (_error) {
          error = _error;
          return this.get("controller.model.title");
        }
        colorLUT = ["#008186", "#BBB648", "#D9816C"];
        newString = "";
        for (idx = _i = 0, _len = tmpArr.length; _i < _len; idx = ++_i) {
          val = tmpArr[idx];
          console.log("-" + val);
          tmpColor = colorLUT[idx % colorLUT.length];
          newString += " <span style='color: " + tmpColor + "'>" + val.toUpperCase() + "</span>";
        }
        console.log(newString);
        return newString;
      }).property("controller.model.title")
    });

    __exports__["default"] = RecipePreview;;
  });
