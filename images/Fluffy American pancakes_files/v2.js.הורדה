              if (i && +i.id === +SymbolLUT.timer.id && ("" + i.prm).match(/\d+/)) {
                timerAccu += +(("" + i.prm).match(/\d+/g).pop());
              }
            }
          }
          if (!("subs" in ref)) {
            return;
          }
          _ref1 = ref.subs;
          _results = [];
          for (_j = 0, _len1 = _ref1.length; _j < _len1; _j++) {
            v = _ref1[_j];
            _results.push(searchTimerRecur(v));
          }
          return _results;
        };
        _ref = this.get("stepsArr");
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          v = _ref[_i];
          searchTimerRecur(v);
        }
        return timerAccu;
      }).property(),
      userAvatarGen: (function() {
        if (this.get("userAvatar")) {
          return this.get("userAvatar");
        }
        return window.get_gravatar(this.get("userMd5"));
      }).property("userAvatar", "userMd5")
    });

    __exports__["default"] = Recipe;
