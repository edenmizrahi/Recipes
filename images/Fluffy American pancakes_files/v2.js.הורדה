define("v2/views/recipe_edit4", 
  ["v2/utils/editor-utils","v2/config/environment","v2/mixins/style-switch","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var UTILS = __dependency1__["default"];

    var V2ENV = __dependency2__["default"];

    var StyleSwitchMixin = __dependency3__["default"];
    var RecipePreview;

    window.autoMove = false;

    $("body").on("mousemove", function(e) {
      var amp, border, max, min, relPos, sign;
      amp = 0;
      border = 0.1;
      min = 10;
      max = 70;
      if (window.autoMove === false) {
        return;
      }
      relPos = ($(window).height() - e.clientY) / $(window).height();
      if (relPos < border) {
        amp = -relPos + border;
      }
      if (relPos > (1 - border)) {
        amp = -relPos + (1 - border);
      }
      sign = amp > 0 ? 1 : -1;
      if (amp === 0) {
        return;
      }
      return window.scrollBy(0, amp * max + sign * min);
    });

    window.initDragDrop = function(cbs) {
      var addAsResult, dragStartEvent, dropEvent, e, groupTime, moveFromAtoB, timeoutSet;
      addAsResult = function(objectThatGetsResult, resultToBeAdded) {
        var a;
        a = resultToBeAdded;
        a.destroy();
        a = objectThatGetsResult.get("_root").createStep(a);
        return objectThatGetsResult.insertAfterSelf(a);
      };
      moveFromAtoB = function(a, a_parent, b, b_before, root) {
        var correctionOffset, newIndex, resultIdx, resultObjectToCarry, tgtArr, tmpIndex;
        tmpIndex = null;
        tgtArr = [];
        if (a.attributes == null) {
          a.attributes = [];
        }
        if (b != null) {
          tgtArr = b.subs;
        } else {
          tgtArr = root.get("stepsArr");
        }
        if (b_before != null) {
          tmpIndex = tgtArr.indexOf(b_before);
        }
        correctionOffset = 0;
        if (tgtArr.indexOf(a) !== -1 && tgtArr.indexOf(a) < tmpIndex) {
          correctionOffset = -1;
        }
        if (b_before != null ? b_before.get("isResult") : void 0) {
          correctionOffset += 1;
        }
        resultObjectToCarry = void 0;
        if (a.get("isMainStep") && ((resultIdx = a.get("isFollowedByResult")) !== -1)) {
          resultObjectToCarry = a.get("_parent.subs")[resultIdx];
        }
        if (!a.get("_parent")) {
          window.nodeDis.removeObjectFromDone(a);
        }
        a.destroy();
        a = root.createStep(a);
        if (resultObjectToCarry != null) {
          resultObjectToCarry.destroy();
          resultObjectToCarry = root.createStep(resultObjectToCarry);
        }
        if (window.lastElement != null) {
          if (window.lastElement === "delete") {
            return console.log("DELTE, do nothing");
          } else {
            if (lastElement.get("item").subs == null) {
              lastElement.get("item").subs = [];
            }
            if (resultObjectToCarry) {
              lastElement.get("item").subs.pushObject(resultObjectToCarry);
            }
            return lastElement.get("item").subs.pushObject(a);
          }
        } else if (tmpIndex != null) {
          newIndex = Math.min(tmpIndex + correctionOffset, tgtArr.length);
          if (resultObjectToCarry != null) {
            tgtArr.insertAt(newIndex, resultObjectToCarry);
          }
          return tgtArr.insertAt(newIndex, a);
        } else {
          tgtArr.pushObject(a);
          if (resultObjectToCarry != null) {
            return tgtArr.pushObject(resultObjectToCarry);
          }
        }
      };
      dropEvent = function($item, container, _super, cbs, isElement) {
        var NCondE, condA, condB, condC, condD, condIsSideproc, root, tmpA, tmpB, tmpB_before, triggerRerender, _ref, _ref1, _ref10, _ref11, _ref12, _ref13, _ref14, _ref15, _ref16, _ref17, _ref18, _ref19, _ref2, _ref20, _ref21, _ref22, _ref23, _ref3, _ref4, _ref5, _ref6, _ref7, _ref8, _ref9;
        window.ga && window.ga('send', 'event', 'Editor', 'drop', 'element');
        window.autoMove = false;
        $item.removeClass("dragged").removeAttr("style");
        $("body").removeClass("dragging");
        tmpA = UTILS.getClosestEmberView($item);
        tmpB = UTILS.getClosestEmberView($(container != null ? container.el[0] : void 0));
        tmpB_before = UTILS.getClosestEmberView((_ref = $item.nextAll(".ingred_container:not(.dummy), .sortme")) != null ? _ref.first() : void 0);
        if (tmpA) {
          root = tmpA.get("controller.model");
        }
        if (tmpB) {
          root = tmpB.get("controller.model");
        }
        if (tmpB_before) {
          root = tmpB_before.get("controller.model");
        }
        if (tmpB_before == null) {
          tmpB_before = UTILS.getClosestEmberView((_ref1 = $item.parent().parent().nextAll(".ingred_container:not(.dummy), .sortme")) != null ? _ref1.first() : void 0);
        }
        condA = (tmpB != null ? tmpB.get("parentView.parentView.parentView") : void 0) && ((_ref2 = tmpA.get("item")) != null ? (_ref3 = _ref2.subs) != null ? _ref3.length : void 0 : void 0) > 0;
        condA = false;
        condB = (tmpB != null ? tmpB.get("parentView") : void 0) && UTILS.subWithSubs((_ref4 = tmpA.get("item")) != null ? _ref4.subs : void 0);
        condC = (window.lastElement != null) && ((_ref5 = tmpA.get("item")) != null ? (_ref6 = _ref5.subs) != null ? _ref6.length : void 0 : void 0) > 0;
        condD = (window.lastElement != null) && (tmpB != null ? tmpB.get("parentView.parentView.parentView") : void 0);
        condD = false;
        NCondE = window.lastElement === "delete";
        condIsSideproc = (tmpB != null ? (_ref7 = tmpB.get("parentView")) != null ? (_ref8 = _ref7.get("item")) != null ? _ref8.name : void 0 : void 0 : void 0) === "Sideprocess";
        if (((condA && !condIsSideproc) || condC || condD) && !NCondE) {
          console.log("NOP jetzt nicht");
          triggerRerender = true;
        } else {
          console.log(">moving");
          if (window.allowResultDrop && window.toolHoverElement) {
            addAsResult(window.toolHoverElement, tmpA != null ? tmpA.get("item") : void 0);
