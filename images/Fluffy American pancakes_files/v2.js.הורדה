          var i, toBeRemoved, _i, _j, _k, _l, _len, _len1, _len2, _len3, _ref, _ref1, _ref2, _ref3, _ref4, _results;
          toBeRemoved = [];
          console.log("clear called !!!!");
          if (prm === "ingred") {
            _ref = this.doneArray;
            for (_i = 0, _len = _ref.length; _i < _len; _i++) {
              i = _ref[_i];
              if (((_ref1 = i.get("item")) != null ? _ref1.type : void 0) !== 2) {
                toBeRemoved.push(i);
              }
            }
          }
          if (prm === "tools") {
            _ref2 = this.doneArray;
            for (_j = 0, _len1 = _ref2.length; _j < _len1; _j++) {
              i = _ref2[_j];
              if (((_ref3 = i.get("item")) != null ? _ref3.type : void 0) === 2) {
                toBeRemoved.push(i);
              }
            }
          }
          if (prm === "unresolved") {
            _ref4 = this.doneArray;
            for (_k = 0, _len2 = _ref4.length; _k < _len2; _k++) {
              i = _ref4[_k];
              if (i.get("status") !== "ok") {
                toBeRemoved.push(i);
              }
            }
          }
          _results = [];
          for (_l = 0, _len3 = toBeRemoved.length; _l < _len3; _l++) {
            i = toBeRemoved[_l];
            _results.push(this.doneArray.removeObject(i));
          }
          return _results;
        },
        showAttrib: function(prm, jq) {
          return this.get("currCtrl").send("showAttrib", prm, jq);
        },
        removeMe: function(obj) {
          return this.get("doneArray").removeObject(obj);
        }
      }
    });

    __exports__["default"] = ToolsOverview;;
  });
define("v2/components/gracipe-timer", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var GracipeTimerComponent;

    GracipeTimerComponent = Ember.Component.extend({
      actions: {
        timerStartToggle: function(timer) {
          return timer.startToggle();
        },
        timerReset: function(timer) {
          return timer.reset();
        },
        timerAdd: function(timer) {
          return timer.add();
        },
        timerSub: function(timer) {
          return timer.sub();
        },
        removeTimer: function(timer) {
          return this.sendAction('removeTimer', timer);
        },
        scrollToTimer: function(step) {
          return $('html, body').stop().animate({
            scrollTop: $('div.step-title-number').filter(function() {
              return $(this).text() === String(step);
            }).offset().top
          }, "slow");
        }
      }
    });

    __exports__["default"] = GracipeTimerComponent;
  });
