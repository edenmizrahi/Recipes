          } else {
            moveFromAtoB(tmpA != null ? tmpA.get("item") : void 0, (_ref9 = window.tmpA_parent) != null ? _ref9.get("item") : void 0, tmpB != null ? tmpB.get("item") : void 0, tmpB_before != null ? tmpB_before.get("item") : void 0, root);
            triggerRerender = false;
          }
        }
        if (!triggerRerender) {
