define("v2/routes/index", 
  ["v2/utils/recipe-utils","v2/utils/recipe-request-resolver","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var recipeModel = __dependency1__["default"];

    var RecipeRequestController = __dependency2__["default"];

    var Index;

    Index = Ember.Route.extend({
      setupController: function(c, m) {
        var tmpRc;
        this._super(c, m);
        c.set("routeRef", this);
        c.set("recipeOfToday", void 0);
        tmpRc = new RecipeRequestController();
        tmpRc.opt = {
          today: 1
        };
        return tmpRc.getNext((function(_this) {
          return function() {
            var firstObject;
            if (firstObject = tmpRc.results.shift()) {
              c.set("recipeOfToday", firstObject);
              c.set("recipeOfToday", firstObject);
              return c.notifyPropertyChange("recipeModel.stepsArr.[]");
            }
          };
        })(this));
      },
      model: function() {
        var indexCtrl, rrc;
        indexCtrl = this.controllerFor("index");
        rrc = indexCtrl.get("rrcIndex");
        if (!rrc) {
          rrc = new RecipeRequestController();
          rrc.pageMaxSize = Math.floor($(document).width() / 270) * 3;
          indexCtrl.set("rrcIndex", rrc);
          rrc.getNext();
        }
        indexCtrl.set("rrc", rrc);
        return rrc.results;
      },
      afterModel: function(m) {
        return $(document).attr('title', "Gracipe - stop reading start cooking");
      }
    });

    __exports__["default"] = Index;;
  });
define("v2/routes/preview", 
  ["v2/utils/recipe-utils","v2/routes/recipe","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var recipes = __dependency1__["default"];

    var recipeRoute = __dependency2__["default"];

    var V2ENV = __dependency3__["default"];

    var tmp;

    tmp = Ember.Route.extend({
      renderTemplate: function() {
        return this.render('preview');
      },
      model: function(p) {
        return recipes.findBySlug(p.gracipe_slug);
      },
      afterModel: function(m) {
        $.ajax(V2ENV.tgtURL + '/mapi/increaseViewCnt/', {
          type: 'GET',
          data: {
            id: m.id
          }
        });
        return window.scrollTo(0, 0);
      }
    });

    __exports__["default"] = tmp;;
  });
define("v2/routes/profile", 
  ["ember","v2/utils/recipe-utils","v2/utils/ajax-resolver","v2/models/user","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var recipes = __dependency2__["default"];

    var ajax = __dependency3__["default"];

    var user = __dependency4__["default"];

    var ProfileRoute;

    ProfileRoute = Ember.Route.extend({
      model: function(p) {
        var tmpO, tmpRes;
        tmpRes = {};
        tmpO = {};
        if (p.user_id && p.user_id !== "me") {
          tmpO.userId = p.user_id;
        }
        return ajax.getUser(tmpO).then((function(_this) {
          return function(tmpRes) {
            if (p.user_id && p.user_id !== "me") {
              _this.controllerFor("profile").set("self", tmpRes.id);
              return user.create(tmpRes);
            } else {
              _this.controllerFor("profile").set("self", "My");
              return _this.controllerFor("application").get("user");
            }
          };
        })(this));
      },
      afterModel: function(m) {
        return $(document).attr('title', "Profile of " + m.get("name"));
      }
    });

    __exports__["default"] = ProfileRoute;
  });
define("v2/routes/profile/profile-bookmark", 
  ["ember","v2/utils/recipe-utils","v2/utils/recipe-request-resolver","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var recipes = __dependency2__["default"];

    var RecipeRequestController = __dependency3__["default"];

    var ProfileMeRoute;

    ProfileMeRoute = Ember.Route.extend({
      model: function(params) {
        var profileController, recipeRequestController, tmpUserId;
        tmpUserId = this.modelFor("profile").id;
        profileController = this.controllerFor("profile");
        recipeRequestController = new RecipeRequestController();
        profileController.set("rrc", recipeRequestController);
        recipeRequestController.opt = {
          bookmark: true
        };
        if (tmpUserId && tmpUserId !== "me") {
          recipeRequestController.opt.userId = tmpUserId;
        }
        recipeRequestController.getNext();
        return recipeRequestController.results;
      }
    });

    __exports__["default"] = ProfileMeRoute;
  });
define("v2/routes/profile/profile-follow", 
  ["ember","v2/utils/ajax-resolver","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ajax = __dependency2__["default"];

    var ProfileProfileFollowRoute;

    ProfileProfileFollowRoute = Ember.Route.extend({
      model: function(p) {
        var profileController;
        profileController = this.controllerFor("profile");
        profileController.set("rrc", void 0);
        return this.setFollowList();
      },
      setFollowList: function() {
        var applicationController, profileController, profileId, userId, userModel;
        profileController = this.controllerFor("profile");
        applicationController = this.controllerFor("application");
        profileId = this.modelFor("profile").id;
        userId = applicationController.get("user").id;
        userModel = Ember.Object.create({
          "followers": Ember.A([]),
          "following": Ember.A([])
        });
        if (userId !== 0) {
          return ajax.getFollowers(userId).then((function(_this) {
            return function(res) {
              res.followers.map(function(i) {
                return userModel.get("followers").addObject(i);
              });
              res.following.map(function(i) {
                return userModel.get("following").addObject(i);
              });
              return _this.getProfileFollowers(profileId, userModel, userId);
            };
          })(this));
        } else {
          return this.getProfileFollowers(profileId, userModel, userId);
        }
      },
      getProfileFollowers: function(profileId, userModel, userId) {
        var model, profileController;
        profileController = this.controllerFor("profile");
        model = Ember.Object.create({
          "followers": Ember.A([]),
          "following": Ember.A([])
        });
        return ajax.getFollowers(profileId).then((function(_this) {
          return function(res) {
            res.followers.map(function(i) {
              i.following = userModel.following.filter(function(element) {
                return element.id === i.id;
              }).length !== 0;
              i.isMe = userId === i.id;
              return model.get("followers").addObject(Ember.Object.create(i));
            });
            res.following.map(function(i) {
              i.following = userModel.following.filter(function(element) {
                return element.id === i.id;
              }).length !== 0;
              i.isMe = userId === i.id;
              return model.get("following").addObject(Ember.Object.create(i));
            });
            profileController.set("social", model.get("followers"));
            return model;
          };
        })(this));
      }
    });

    __exports__["default"] = ProfileProfileFollowRoute;
  });
