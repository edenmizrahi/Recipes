define("v2/tests/acceptance/fb-login-test", 
  ["ember","v2/tests/helpers/start-app"],
  function(__dependency1__, __dependency2__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var startApp = __dependency2__["default"];
    var App;

    App = null;

    module('Acceptance: FbLogin', {
      setup: function() {
        delete localStorage.user;
        return App = startApp();
      },
      teardown: function() {
        return Ember.run(App, 'destroy');
      }
    });

    test('loging in with facebook (user session must be stored)', function() {
      expect(3);
      visit('/');
      return andThen(function() {
        if ($(".gracipe-logout-button").length > 0) {
          click(".gracipe-logout-button");
        }
        return andThen(function() {
          click(".gracipe-login-button");
          return andThen(function() {
            click(".button-gracipe-facebook-login");
            stop();
            return andThen(function() {
              return setTimeout(function() {
                start();
                notEqual(find("#profileText > a").text(), "LoginAbout");
                ok(localStorage.user);
                return andThen(function() {
                  click(".gracipe-logout-button");
                  return andThen(function() {
                    return equal(find("#profileText > a").text(), "LoginAbout");
                  });
                });
              }, 5000);
            });
          });
        });
      });
    });
  });
define("v2/tests/acceptance/login-site-test", 
  ["ember","v2/tests/helpers/start-app"],
  function(__dependency1__, __dependency2__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var startApp = __dependency2__["default"];
    var App;

    App = null;

    module('Acceptance: LoginSite', {
      setup: function() {
        delete localStorage.user;
        return App = startApp();
      },
      teardown: function() {
        return Ember.run(App, 'destroy');
      }
    });

    test('loging and logout in with marri', function() {
      expect(3);
      visit('/');
      return andThen(function() {
        click(".gracipe-login-button");
        return andThen(function() {
          fillIn(".loginForm-user", "marri@marri.is");
          fillIn(".loginForm-password", "marri");
          click(".button-gracipe-login");
          return andThen(function() {
            notEqual(find("#profileText > a").text(), "LoginAbout");
            ok(localStorage.user);
            return andThen(function() {
              click(".gracipe-logout-button");
              return andThen(function() {
                return equal(find("#profileText > a").text(), "LoginAbout");
              });
            });
          });
        });
      });
    });

    test('check if transition from index to route search (apple) is working', function() {
      expect(1);
      visit('/');
      return andThen(function() {
        fillIn(".gracipe-search-form", "apple");
        keyEvent(".gracipe-search-form", "keyup", 48);
        keyEvent(".gracipe-search-form", "keyup", 8);
        keyEvent(".gracipe-search-form", "keypress", 13);
        return andThen(function() {
          click("#searchIcon");
          return andThen(function() {
            ok(find(".cat-tags > li").length > 0);
            return console.log("hello");
          });
        });
      });
    });

    test('check if route search (apple) is working', function() {
      expect(1);
      visit('/search?q=apple');
      return andThen(function() {
        return ok(find(".cat-tags > li").length > 0);
      });
    });
  });
define("v2/tests/acceptance/smoke-test", 
  ["ember","v2/tests/helpers/start-app"],
  function(__dependency1__, __dependency2__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var startApp = __dependency2__["default"];
    var App;

    App = null;

    module('Acceptance: Smoke', {
      setup: function() {
        App = startApp();
        delete localStorage.user;
        return localStorage["DEBUG"] = true;
      },
      teardown: function() {
        return Ember.run(App, 'destroy');
      }
    });

    test('smoketest', function() {
      expect(5);
      visit('/');
      return andThen(function() {
        click(".gracipe-login-button");
        return andThen(function() {
          fillIn(".loginForm-user", "marri@marri.is");
          fillIn(".loginForm-password", "marri");
          click(".button-gracipe-login");
          return andThen(function() {
            notEqual(find("#profileText > a").text(), "LoginAbout");
            click("#newGracipeButton");
            return andThen(function() {
              click("#gryciphy-button");
              return andThen(function() {
                var onionIsThere;
                onionIsThere = $("p.name").text() === "CarrotsCream CheeseOnion";
                ok(onionIsThere);
                click(".editorSidebar > #editGracipeDetails");
                return andThen(function() {
                  fillIn("#recipe-title", "smokétest");
                  click("#save-recipe");
                  return andThen(function() {
                    var vegetableHeadlineIsThere;
                    vegetableHeadlineIsThere = $(".veg_fruit").first().text() === "Vegetables & Fruits";
                    ok(vegetableHeadlineIsThere);
                    click(".gracipe-logout-button");
                    return andThen(function() {
                      var recipeId;
                      equal(find("#profileText > a").text(), "LoginAbout");
                      recipeId = $("#recipe-id-hidden").text();
                      visit("/recipe/" + recipeId + "/edit");
                      return andThen(function() {
                        vegetableHeadlineIsThere = $(".veg_fruit").first().text() === "Vegetables & Fruits";
                        return ok(vegetableHeadlineIsThere);
                      });
                    });
                  });
                });
              });
            });
          });
        });
      });
    });
  });
