    uploadFiles = function(files, cb) {
      var data;
      data = new FormData();
      $.each(files, function(key, value) {
        data.append(key, value);
      });
      console.log(data);
      $.ajax({
        url: V2ENV.tgtURL + "/mapi/uploadIphone/",
        type: "POST",
        data: data,
        dataType: "json",
        processData: false,
        contentType: false,
        success: function(data, textStatus, jqXHR) {
          if (typeof data.error === "undefined") {
            console.log(data.filePath);
            cb(data);
          } else {
            console.log("ERRORS: " + data.error);
          }
        },
        error: function(jqXHR, textStatus, errorThrown) {
          console.log("ERRORS: " + textStatus);
        }
      });
    };

    RecipeWatchdog = (function() {
      function RecipeWatchdog(modelRef) {
        var checkWatch, self;
        this.modelRef = modelRef;
        delete localStorage["recipeWatch"];
        delete localStorage["recipeWatchId"];
        self = this;
        checkWatch = (function(_this) {
          return function() {
            if (self.modelRef && self.enabled) {
              self.modelRef.reindexMeta();
              localStorage["recipeWatch"] = recipeModel.recipeToStorage(self.modelRef);
              localStorage["recipeWatchId"] = self.modelRef.id;
              localStorage["recipeWatchTitle"] = self.modelRef.title;
              localStorage["recipeWatchInitialServing"] = self.modelRef.initialServing;
              localStorage["recipeWatchDescription"] = self.modelRef.description;
              localStorage["recipeWatchSrcImgId"] = self.modelRef.srcImgId;
            }
            return setTimeout(function() {
              return checkWatch();
            }, self.watchIntervall);
          };
        })(this);
        checkWatch();
      }

      RecipeWatchdog.prototype.stopWatchdog = function() {
        this.enabled = false;
        return delete localStorage["recipeWatch"];
      };

      RecipeWatchdog.prototype.enabled = true;

      RecipeWatchdog.prototype.watchIntervall = 2000;

      return RecipeWatchdog;

    })();

    RecipeController = Ember.ObjectController.extend({
      needs: ["application", "index"],
      activeStyle: "block",
      redraw: 0,
      viewOnly: false,
      showRecipeDetails: false,
      currAttrib: {},
      quickAdd: GQuickAdd,
      wd: null,
      recipe: Ember.computed.alias("content"),
      pubSteps: (function() {
        this.get("model");
        return this.get("model.stepsArr");
      }).property("model"),
      initWatchdog: function(m) {
        var wd;
        wd = new RecipeWatchdog(m);
        this.set("wd", wd);
        return wd;
      },
      actions: {
        doneSave: function() {
          var data, tmpId;
          if (!this.get("controllers.application.loggedIn")) {
            this.get("controllers.application").send("doLogin");
            return;
          }
          if (!this.get("model.title")) {
            this.set("showRecipeDetails", 1);
            this.set("errorTitleMissing", "To save the recipe, please give it a title");
            return;
          }
          if (!this.get("model.initialServing")) {
            this.set("model.initialServing", 4);
          }
          tmpId = this.get("model.id");
          if (tmpId == null) {
            tmpId = 0;
          }
          data = {
            id: tmpId,
            title: this.get("model.title"),
            servings: this.get("model.initialServing"),
            description: this.get("model.description"),
            srcImgId: this.get("model.srcImgId"),
            cat: JSON.stringify(this.get("model.cats")),
            json: recipeModel.recipeToStorage(this.get("model"))
          };
          this.get("wd").stopWatchdog();
          return ajax.storeGracipe(data, (function(_this) {
            return function(res) {
              if (res) {
                console.log("doneSave - YEHA");
                return _this.transitionToRoute('recipe', res);
              } else {
                return console.log("doneSave - NOP");
              }
            };
          })(this));
        },
        setShowDetails: function(s) {
          return this.set("showRecipeDetails", s);
        },
        removeMe: function(ref) {
          ref.destroy();
          return this.set("redraw", (new Date()).getTime());
        },
        discardChanges: function() {
          this.get("wd").stopWatchdog();
          if (this.get("model.slug")) {
            return this.transitionToRoute('recipe', this.get("model.slug"));
          } else {
            return this.transitionToRoute('index');
          }
        },
        clickEvent: function() {
          if (!this.get("currAttrib.ignoreClick")) {
            this.set("currAttrib.item", null);
          }
          return this.set("currAttrib.ignoreClick", false);
        },
        showAttrib: function(prm, jq, viewRef) {
          this.set("currAttrib.item", prm);
          this.set("currAttrib.jq", jq);
          this.set("currAttrib.viewRef", viewRef);
          return this.set("currAttrib.ignoreClick", true);
        },
        clearList: function() {
          return nodeDis.doneList.clear();
        },
        quickAddElement: function(oRef) {
          var o;
          window.ga && window.ga('send', 'event', 'Editor', 'quickAdd', oRef.name);
          o = JSON.parse(JSON.stringify(oRef));
          o = this.get("model").createStep(o);
          this.get("model.stepsArr").addObject(o);
          return this.set("redraw", (new Date()).getTime());
        },
        addResultAfterItem: function(item) {
          var pArr, tmpObj;
          window.ga && window.ga('send', 'event', 'Editor', 'addResult', "true");
          tmpObj = this.get("model").createResults(item);
          pArr = item.get("_parent.subs");
          pArr.insertAt(pArr.indexOf(item) + 1, tmpObj);
          this.get("model").reindexMeta();
          return Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              _this.get("model").reindexMeta();
              return _this.set("redraw", (new Date()).getTime());
            };
          })(this));
        },
        uploadPicture: function() {
          window.ga && window.ga('send', 'event', 'Editor', 'uploadPicture', "true");
          $(".fileClick").change((function(_this) {
            return function(event) {
              var files;
              files = event.target.files;
              return uploadFiles(files, function(newURL) {
                if (newURL) {
                  _this.set("model.srcImg", newURL.filePath);
                }
                if (newURL) {
                  return _this.set("model.srcImgId", newURL.id);
                }
              });
            };
          })(this));
          return $(".fileClick").click();
        },
        focusTextbox: function() {
          window.ga && window.ga('send', 'event', 'Editor', 'quickAddMore', "MORE");
          switch (Math.floor(Math.random() * 4)) {
            case 0:
              $("#theSearchPlaceholder").val("blender");
              break;
            case 1:
              $("#theSearchPlaceholder").val("100g ice-cream");
              break;
            case 2:
              $("#theSearchPlaceholder").val("1/2 pineapple");
              break;
            case 3:
              $("#theSearchPlaceholder").val("oven");
          }
          return $("#theSearchPlaceholder").focus();
        },
        trackTutorialClick: function() {
          return window.ga && window.ga('send', 'event', 'Editor', 'openTutorial', "true");
        }
      }
    });

    __exports__["default"] = RecipeController;;
  });
define("v2/controllers/recipe_edit4", 
