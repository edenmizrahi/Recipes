      }).on('willDestroyElement'),
      didInsertElementHandler: (function() {
        return $(document).bind("keydown", {
          c: this
        }, this.cursorKeyDownHandler);
      }).on('didInsertElement'),
      elPicker: {},
      cursorKeyDownHandler: (function(_this) {
        return function(e) {
          var ctrlRef;
          ctrlRef = e.data.c;
          if (e.ctrlKey && e.keyCode === 65) {
            console.log('ctrl a');
            e.preventDefault();
            console.log('Show modal-elementpicker');
            ctrlRef.set("elPicker.container", ctrlRef.get('doneArray'));
            ctrlRef.set("elPicker.viewRef", ctrlRef.get('view'));
            return ctrlRef.set("elPicker.isTools", 0);
          }
        };
      })(this),
      actions: {
