define("v2/models/recipe-step", 
  ["ember","v2/models/svg-element","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];

    var svgElement = __dependency2__["default"];

    var RecipeStep;

    RecipeStep = Ember.Object.extend({
      init: function() {
        var tmpSvgEl;
        tmpSvgEl = svgElement.create();
        tmpSvgEl.set("id", this.id);
        if (!this.id) {
          tmpSvgEl.set("id", this.element_id);
        }
        this.svgObj = tmpSvgEl;
        if (!this.subs) {
          this.subs = [];
        }
        if (!this.subs) {
          return this.attributes = [];
        }
      },
      _root: null,
      idWatch: (function() {
        return this.set("svgObj.id", this.get("id"));
      }).observes("id"),
      isMainStep: (function() {
        return this.get("_root.stepsArr").contains(this) || (this.get("_parent.side") != null);
      }).property("_root.reindex"),
      hasChildren: (function() {
        if (this.get("subs") && this.get("subs").length > 0) {
          return true;
        }
        return false;
      }).property("subs"),
      parentIndex: (function() {
        if (!this.get("_parent.subs")) {
          return -1;
        }
        return this.get("_parent.subs").indexOf(this);
      }).property("_parent.subs.@each"),
      getNextSibling: (function() {
        var nextSiblingIdx;
        if (this.get("parentIndex") - 1 >= this.get("_parent.subs.length")) {
          return this;
        } else {
          nextSiblingIdx = this.get("parentIndex") + 1;
          return this.get("_parent.subs." + nextSiblingIdx);
        }
      }).property("parentIndex"),
      isFollowedByResult: (function() {
        var tgtIdx, tmpSubs, _ref;
        tmpSubs = this.get("_parent.subs");
        tgtIdx = this.get("parentIndex") + 1;
        if ((((_ref = tmpSubs != null ? tmpSubs.length : void 0) != null ? _ref : 0) > tgtIdx) && tmpSubs[tgtIdx].get("isResult")) {
          return tgtIdx;
        } else {
          return -1;
        }
      }).property("_parent.subs.@each"),
      getNextSiblingResult: (function() {
        var nextItem;
        if (this.get("isFollowedByResult") === -1) {
          return null;
        }
        nextItem = this.get("_parent.subs." + (this.get("isFollowedByResult")));
        return this.get("_root.resultsList")[nextItem.get("uid")];
      }).property("isFollowedByResult"),
      cloneWithSameSVG: function() {
        var i, stepClone, _i, _len, _ref;
        stepClone = Ember.Object.create();
        _ref = "svgObj id name unitDescr amount".w();
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          i = _ref[_i];
          stepClone.set(i, this.get(i));
        }
        return stepClone;
      },
      insertAfterSelf: function(step) {
        var tgtIdx, _ref, _ref1;
        tgtIdx = this.get("parentIndex") + 1;
        if (((_ref = (_ref1 = this.get("_parent.subs")) != null ? _ref1.length : void 0) != null ? _ref : 0) > tgtIdx) {
          return this.get("_parent.subs").insertAt(tgtIdx, step);
        } else {
          return this.get("_parent.subs").pushObject(step);
        }
      },
      destroy: function() {
        var e;
        try {
          return this.get("_parent.subs").removeObject(this);
        } catch (_error) {
          e = _error;
          return console.warn(e);
        }
      }
    });

    __exports__["default"] = RecipeStep;
  });
define("v2/models/recipe", 
  ["ember","v2/utils/utils","v2/utils/symbol-lut","v2/models/recipe-step","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var UTILS = __dependency2__["default"];

    var SymbolLUT = __dependency3__["default"];

    var RecipeStep = __dependency4__["default"];

    var Recipe;

    Recipe = Ember.Object.extend({
      init: function() {
        return this.set("stepsArr", Ember.A([]));
      },
      reindex: 0,
      results: {},
      resultsFistLUT: {},

      /**
       * function that caluclates the meta properties in a recipe object, e.g. current parents, children, lvl.
       * All meta properties start with "_" which will not render them as JSON later
       * @return {[type]} [description]
       */
      reindexMeta: function() {
        var checkSubs, emptyResults, emptySideProcs, i, sideCnt, _i, _j, _len, _len1;
        sideCnt = 0;
        this.resultsFistLUT = {};
        checkSubs = (function(_this) {
          return function(p, plvl, pside) {
            var i, _i, _len, _ref, _results;
            if (p.isResult && !_this.resultsFistLUT[p.uid]) {
              _this.resultsFistLUT[p.uid] = p;
            }
            if (!p.attributes) {
              p.attributes = [];
            }
            _ref = p.get("subs");
            _results = [];
            for (_i = 0, _len = _ref.length; _i < _len; _i++) {
              i = _ref[_i];
              i.set("_parent", p);
              i.set("_sideCnt", pside);
              i.set("_lvl", plvl);
              if (i.subproc) {
                sideCnt += 1;
                _results.push(checkSubs(i, plvl + 1, sideCnt));
              } else {
                _results.push(checkSubs(i, plvl + 1, pside));
              }
            }
            return _results;
          };
        })(this);
        emptySideProcs = this.get("stepsArr").filter(function(v) {
          var _ref;
          return v.subproc && ((_ref = v.subs) != null ? _ref.length : void 0) === 0;
        });
        for (_i = 0, _len = emptySideProcs.length; _i < _len; _i++) {
          i = emptySideProcs[_i];
          if (!i._noDelete) {
            this.get("stepsArr").removeObject(i);
          }
        }
        checkSubs(this, 0, 0);
        emptyResults = Object.keys(this.results).filter((function(_this) {
          return function(k) {
            return !(k in _this.resultsFistLUT);
          };
        })(this));
        for (_j = 0, _len1 = emptyResults.length; _j < _len1; _j++) {
          i = emptyResults[_j];
          delete this.results[i];
        }
        return this.notifyPropertyChange("stepsArr");
      },
      reindexMetaWatch: (function() {
        return this.reindexMeta();
      }).observes("reindex"),
      subs: (function() {
        return this.get("stepsArr");
      }).property("stepsArr"),
      deleteResults: function(puid) {
        var collector, i, recur, _i, _len, _results;
        collector = [];
        recur = function(p) {
          var i, _i, _len, _ref, _results;
          _ref = p.get("subs");
          _results = [];
          for (_i = 0, _len = _ref.length; _i < _len; _i++) {
