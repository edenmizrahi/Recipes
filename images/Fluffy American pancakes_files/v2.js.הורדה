          window.allowResultDrop = false;
          window.allowDropOnElement = true;
          window.lastElement = void 0;
          if ((_ref = UTILS.getClosestEmberView($item)) != null ? _ref.get("item.isResult") : void 0) {
            window.allowResultDrop = true;
            $(".small > .ingred-single-container").addClass("notDroppable");
          }
          $(".is-not-first-and-self-result").parent().next().hide();
          return dragStartEvent($item, container, _super, true);
        },
        onDrop: function($item, container, _super) {
          window.allowDropOnElement = false;
          $(".is-not-first-and-self-result").parent().next().show();
          $(".small > .ingred-single-container").removeClass("notDroppable");
          return dropEvent($item, container, _super, cbs, true);
        }
      });
      timeoutSet = null;
      $(".ingred-single-container > .ingred_container").mouseenter(function() {
        if (!window.allowDropOnElement) {
          return;
        }
        timeoutSet = setTimeout(((function(_this) {
          return function() {
            $(_this).css("background-color", "#87FC3E");
            return window.lastElement = UTILS.getClosestEmberView($(_this));
          };
        })(this)), 800);
        return $(this).mouseleave(function() {
          if (timeoutSet !== null) {
            clearTimeout(timeoutSet);
          }
          $(this).css("background-color", "");
          return window.lastElement = null;
        });
      });
      $(".tool").not(".tool-result").mouseenter(function() {
        var _ref;
        if (!window.allowResultDrop) {
          return;
        }
        $(this).css("background-color", "#87FC3E");
        $(".ingred_container-placehoder").hide();
        window.toolHoverElement = (_ref = UTILS.getClosestEmberView($(this))) != null ? _ref.get("item") : void 0;
        return $(this).mouseleave(function(e) {
          if (e.originalEvent.buttons !== 0) {
            window.toolHoverElement = void 0;
          }
          $(".ingred_container-placehoder").show();
          return $(this).css("background-color", "");
        });
      });
      try {
        $(".result").draggable("destroy");
      } catch (_error) {
        e = _error;
        console.log("...");
      }
      window.toolDragElement = void 0;
      window.toolHoverElement = void 0;
      $(".result").draggable({
        helper: "clone",
        start: function() {
          var _ref;
          window.allowResultDrop = true;
          return window.toolDragElement = (_ref = UTILS.getClosestEmberView($(this))) != null ? _ref.get("item") : void 0;
        },
        stop: function() {
          var a, b, c, resultIdx;
          if (window.toolDragElement && window.toolHoverElement) {
            a = window.toolDragElement;
            b = window.toolHoverElement;
            resultIdx = a != null ? a.get("isFollowedByResult") : void 0;
            if (a && b && resultIdx) {
              c = a.get("_parent.subs")[resultIdx];
              a.get("_parent.subs").removeObject(c);
              c = a.get("_root").createStep(c);
              b.insertAfterSelf(c);
              return cbs.drop(true);
            }
          }
        }
      });
      return $("#closeModal").click(function() {
        return $('#tutorialPlayer')[0].contentWindow.postMessage('{"event":"command","func":"' + 'stopVideo' + '","args":""}', '*');
      });
    };

    RecipePreview = Ember.View.extend(StyleSwitchMixin, {
      tagName: 'div',
      classNames: [],
      virtImgSrc: (function() {
        if (!this.get("controller.model.srcImg")) {
          return '/static/ember/img/camera.svg';
        }
        return this.get("controller.model.srcImg");
      }).property("controller.model.srcImg"),
      totalTime: (function() {
        var tmpVar;
        tmpVar = {
          steps: this.get("controller.content.steps")
        };
        return window.getTotalTime(tmpVar);
      }).property("controller.content.steps.@each"),
      click: function() {
        return this.get("controller").send("clickEvent");
      },
      redrawWatch: (function() {
        this.set("controller.model.reindex", this.get("controller.redraw"));
        return Ember.run.schedule('afterRender', (function(_this) {
          return function() {
            return initDragDrop({
              drop: _this.get("dropFunRef")
            });
          };
        })(this));
      }).observes("controller.redraw", "model.stepsArr"),
      didInsertElement: function() {
        var dropFun, url;
        this._super();
        if (this.get("controller.autoConvertURLStart")) {
          url = this.get("controller.autoConvertURL");
          Ember.run.scheduleOnce('afterRender', (function(_this) {
            return function() {
              return _this.get("controller").send("autoConvertFromURL", url);
            };
          })(this));
        }
        window.GracipeLoaded = 1;
        console.log("LOADED");
        console.log(this.get('controller.showRecipeDetails'));
        $('.editorSidebar').css("min-height", "800px");
        $('.affix-inner-container').affix({
          offset: {
            top: 87
          }
        });
        $('#fixedIngred').affix({
          offset: {
            top: function() {
              if ($(".affixSideBar").height() > $(".affixMain").height()) {
                return 999999;
              }
              return $("#fixedIngredAnchor").offset().top;
            }
          }
        });
        dropFun = ((function(_this) {
          return function(rerender) {
            if (rerender) {
              _this.rerender();
            }
            $("body").addClass("scrollFix");
            _this.set("controller.redraw", _this.get("controller.redraw") + 1);
            return Ember.run.schedule('afterRender', function() {
              _this.set("controller.redraw", _this.get("controller.redraw") + 1);
              console.log("render event!");
              initDragDrop({
                drop: dropFun
              });
              return $("body").removeClass("scrollFix");
            });
          };
        })(this));
        this.set("dropFunRef", dropFun);
        initDragDrop({
          drop: dropFun
        });
        return this.set("controller.redraw", (new Date()).getTime());
      }
    });

    __exports__["default"] = RecipePreview;;
  });
define("v2/views/search", 
  ["ember","v2/mixins/bottom-reload","exports"],
