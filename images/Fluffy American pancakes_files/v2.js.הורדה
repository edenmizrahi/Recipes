      hideMenu: false,
      autoPrint: false,
      hideUserAndImages: false,
      recipe: Ember.computed.alias("content"),
      activeStyle: "arrow",
      flip: "none",
      unit: "author",
      rerenderSwitch: 1,
      viewOnly: true,
      experimentMainMessage: (function() {
        return +sessionStorage["PSRwOa0xTQ6HZSKKdEtfyw-varID"] === 1;
      }).property(""),
      allowEdit: (function() {
        var u;
        u = this.get("controllers.application.user");
        return u.get("id") === this.get("model.user") || u.get("isAdmin");
      }).property("controllers.application.user.id", "controllers.application.user.secretAT", "model.user"),
      randoms: (function() {
