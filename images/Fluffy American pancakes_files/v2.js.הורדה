define("v2/routes/quick_view", 
  ["v2/utils/recipe-utils","v2/routes/recipe","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var recipes = __dependency1__["default"];

    var recipeRoute = __dependency2__["default"];

    var V2ENV = __dependency3__["default"];

    var tmp;

    tmp = Ember.Route.extend({
      renderTemplate: function() {
        return this.render('quick_view');
      },
      model: function(p) {
        return recipes.findById(+p.recipe_id);
      },
      afterModel: function(m) {
        $.ajax(V2ENV.tgtURL + '/mapi/increaseViewCnt/', {
          type: 'GET',
          data: {
            id: m.id
          }
        });
        return window.scrollTo(0, 0);
      }
    });

    __exports__["default"] = tmp;;
  });
define("v2/routes/recipe-url-forward", 
  ["ember","v2/utils/recipe-utils","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var recipes = __dependency2__["default"];

    var RecipeUrlForwardRoute;

    RecipeUrlForwardRoute = Ember.Route.extend({
      beforeModel: function(t) {
        var recipeId, tmpRecipe;
        recipeId = t.params["recipe-url-forward"]["recipe_id"];
        return tmpRecipe = recipes.findById(recipeId).then((function(_this) {
          return function(m) {
            return _this.transitionTo('recipe', m);
          };
        })(this));
      }
    });

    __exports__["default"] = RecipeUrlForwardRoute;
  });
define("v2/routes/recipe", 
  ["v2/utils/recipe-utils","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var recipes = __dependency1__["default"];

    var V2ENV = __dependency2__["default"];

    var tmp;

    tmp = Ember.Route.extend({
      model: function(params) {
        if (params.recipe_id) {
          return recipes.findById(+params.recipe_id);
        }
        if (params.gracipe_slug) {
          return recipes.findBySlug(params.gracipe_slug);
        }
      },
      afterModel: function(m) {
        $(document).attr('title', m.title);
        $.ajax(V2ENV.tgtURL + '/mapi/increaseViewCnt/', {
          type: 'GET',
          data: {
            id: m.id
          }
        });
        return window.scrollTo(0, 0);
      },
      serialize: function(m) {
        return {
          "gracipe_slug": m.get("slug")
        };
      },
      actions: {
        didTransition: function() {
          return this.controllerFor("recipe").set("rerenderSwitch", (new Date()).getTime());
        }
      }
    });

    __exports__["default"] = tmp;;
  });
define("v2/routes/recipe_edit", 
  ["v2/utils/editor-utils","v2/utils/recipe-utils","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var UTILS = __dependency1__["default"];

    var recipes = __dependency2__["default"];

    var tmp;

    tmp = Ember.Route.extend({
      afterModel: function(m) {
        var allow, u, wd;
        u = this.controllerFor("application").get("user");
        allow = u.get("id") === m.get("user") || u.get("isAdmin") || m._isNew || !m.get("user");
        wd = this.controllerFor("recipe_edit").initWatchdog(m);
        if (!allow) {
          return this.transitionTo('recipe', m);
        }
      },
      model: function(params) {
        var idCheck, tmp1, tmp2;
        idCheck = (+params.recipe_id === +localStorage["recipeWatchId"]) || (params.recipe_id === "new" && (localStorage["recipeWatchId"] === "undefined" || +localStorage["recipeWatchId"] === 0));
        if (idCheck) {
          if (localStorage["DEBUG"] || confirm('We noticed you didn\'t finish your last Gracipe. Do you want to reload it?')) {
            if (!localStorage["DEBUG"]) {
              window.ga && window.ga('send', 'event', 'Editor', 'recoverRecipe', "true");
            }
            tmp1 = JSON.parse(localStorage["recipeWatch"]);
            tmp2 = {
              steps: tmp1
            };
            tmp2.id = +localStorage["recipeWatchId"];
            if (isNaN(+localStorage["recipeWatchId"])) {
              tmp2.id = 0;
            }
            tmp2.title = localStorage["recipeWatchTitle"];
            tmp2.initialServing = localStorage["recipeWatchInitialServing"];
            tmp2.description = localStorage["recipeWatchDescription"];
            tmp2.srcImgId = localStorage["recipeWatchSrcImgId"];
            return recipes.convertJSON(tmp2);
          }
        }
        if (params.recipe_id === "new") {
          return recipes.convertJSON({
            _isNew: true
          });
        } else {
          return recipes.findById(+params.recipe_id);
        }
      },
      routeTo: function(json) {
        return json;
      }
    });

    __exports__["default"] = tmp;;
  });
define("v2/routes/recipe_edit4", 
  ["v2/utils/utils","v2/utils/recipe-utils","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var UTILS = __dependency1__["default"];

    var recipes = __dependency2__["default"];

    var tmp;

    tmp = Ember.Route.extend({
      afterModel: function(m) {
        var allow, qs, u, wd;
        $(document).attr('title', "Create Gracipes with our creator");
        u = this.controllerFor("application").get("user");
        allow = u.get("id") === m.get("user") || u.get("isAdmin") || m._isNew || !m.get("user");
        wd = this.controllerFor("recipe_edit4").initWatchdog(m);
        qs = UTILS.getQueryParams(document.location.search);
        if (qs["convertURL"]) {
          this.controllerFor("recipe_edit4").set("autoConvertURL", qs["convertURL"]);
          this.controllerFor("recipe_edit4").set("autoConvertURLStart", true);
        }
        if (!allow) {
          return this.transitionTo('recipe', m);
        }
      },
      model: function(params, transition) {
        var autoConvertActive, extendRoute, idCheck, qs, result;
        idCheck = (+params.recipe_id === +localStorage["recipeWatchId"]) || (params.recipe_id === "new" && (localStorage["recipeWatchId"] === "undefined" || +localStorage["recipeWatchId"] === 0));
        qs = UTILS.getQueryParams(document.location.search);
        autoConvertActive = "convertText" in qs || "convertURL" in qs;
        if (idCheck && !autoConvertActive && !localStorage["DEBUG"]) {
          this.controllerFor("modal-confirm").set("text", 'We noticed you didn\'t finish your last Gracipe. Do you wanna reload it?');
          extendRoute = this;
          return new Ember.RSVP.Promise(function(resolve) {
            extendRoute.controllerFor("modal-confirm").set("callback", function(status) {
              var result, tmp1, tmp2;
              if (!window.showModalWorkaround) {
                extendRoute.send("hideModal", "modal-confirm");
              }
              if (status === null) {
                transition.abort();
              }
              if (status) {
                if (!localStorage["DEBUG"]) {
                  window.ga && window.ga('send', 'event', 'Editor', 'recoverRecipe', "true");
                }
                tmp1 = JSON.parse(localStorage["recipeWatch"]);
                tmp2 = {
                  steps: tmp1
                };
                tmp2.id = +localStorage["recipeWatchId"];
                if (isNaN(+localStorage["recipeWatchId"])) {
                  tmp2.id = 0;
                }
                tmp2.title = localStorage["recipeWatchTitle"];
                tmp2.initialServing = localStorage["recipeWatchInitialServing"];
                tmp2.description = localStorage["recipeWatchDescription"];
                tmp2.srcImgId = localStorage["recipeWatchSrcImgId"];
                result = recipes.convertJSON(tmp2);
              } else {
                if (params.recipe_id === "new") {
                  result = recipes.convertJSON({
                    _isNew: true
                  });
                } else {
                  result = recipes.findById(+params.recipe_id);
                }
              }
              return resolve(result);
            });
            return transition.send("showModal", "modal-confirm");
          });
        } else {
          if (params.recipe_id === "new") {
            result = recipes.convertJSON({
              _isNew: true
            });
          } else {
            result = recipes.findById(+params.recipe_id);
          }
        }
        return result;
      }
    });

    __exports__["default"] = tmp;;
  });
define("v2/routes/search", 
  ["ember","v2/utils/recipe-utils","v2/utils/recipe-request-resolver","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var recipeModel = __dependency2__["default"];

    var RecipeRequestController = __dependency3__["default"];

    var SearchRoute;

    SearchRoute = Ember.Route.extend({
      queryParams: {
        category: {
          refreshModel: true
        },
        q: {
          refreshModel: true
        }
      },
      beforeModel: function(t) {
        var category, q, tagList;
        category = t.queryParams.category;
        q = t.queryParams.q;
        tagList = this.controllerFor('search').get('tagList');
        if (!q && category) {
          return this.transitionTo("cats", category);
        }
      },
      model: function(p, t) {
        var rrc;
        $(document).attr('title', "Search results for " + p.q);
        rrc = new RecipeRequestController();
        rrc.clear();
        this.controllerFor("search").set("rrc", rrc);
        if (p.q) {
          rrc.opt["query"] = p.q;
        }
        if (p.category) {
          rrc.opt["cat"] = [p.category];
        }
        rrc.getNext();
        return rrc.results;
      }
    });

    __exports__["default"] = SearchRoute;
  });
