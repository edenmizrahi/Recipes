      tagName: "div",
      classNames: ["sortme"],
      content: (function() {
        return [this.get("item")];
      }).property("item"),
      createChildView: function(viewClass, attrs) {
        attrs.item = attrs.content;
        if (this.get("ref")) {
          attrs.ref = this.get("ref");
        }
        if (attrs.item.subproc) {
          viewClass = side;
        } else if (attrs.item.isResult) {
          viewClass = stepResult;
        } else {
          if (attrs.item._sideCnt > 0) {
            attrs.isSideproc = 1;
          } else {
            attrs.isSideproc = 0;
          }
          viewClass = step;
        }
        return this._super(viewClass, attrs);
      }
    });

    __exports__["default"] = EditStepBaseView;
  });
