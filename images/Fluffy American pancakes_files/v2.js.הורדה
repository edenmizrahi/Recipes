define("v2/views/recipe-preview", 
  ["v2/config/environment","v2/mixins/style-switch","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var V2ENV = __dependency1__["default"];

    var StyleSwitchMixin = __dependency2__["default"];
    var RecipePreview;

    RecipePreview = Ember.View.extend(StyleSwitchMixin, {
      classNames: ["recipe-preview"],
      templateName: (function() {
        return "recipe-preview-old";
        if ((this.get("recipe.srcImgPreview") && (!this.get("recipe.srcImgPreview").match(/default.jpg/i)) && Math.random() > 0.7) && !this.get("onlyStdPreview")) {
          return "recipe-preview";
        } else {
          return "recipe-preview-old";
        }
      }).property('recipe.srcImgPreview').cacheable(),
      style: (function() {
        return "background-image:url('" + this.get("recipe.srcImg") + "')";
      }).property("recipe.srcImgPreview"),
      stylePreview: (function() {
        return "background-position: 0% 0%; background-size: 110% auto; background-repeat: no-repeat; background-image:url('" + this.get("recipe.srcImgPreview") + "')";
        return "background-position: 0% " + this.get("ratio") + "%; background-size: 100% auto; background-repeat: no-repeat; background-image:url('" + this.get("recipe.srcImgPreview") + "')";
      }).property("recipe.srcImgPreview", "ratio"),
      mouseMove: function(e) {
        var ratio;
        if (this.get("recipe.srcImgPreviewDim")) {
          this.set("ratio", 0);
          return;
        }
        ratio = (-this.$().offset().top + e.pageY - 50) / (this.$().height() - 2 * 50 - 100);
        ratio = Math.max(0, ratio);
        ratio = Math.min(1, ratio);
        return this.set("ratio", ratio * 105);
      },
      didInsertElement: function() {
        return this.set("controller.itemRendered", (new Date()).getTime());
      }
    });

    __exports__["default"] = RecipePreview;;
  });
define("v2/views/recipe", 
  ["v2/config/environment","v2/mixins/style-switch","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var V2ENV = __dependency1__["default"];

    var StyleSwitchMixin = __dependency2__["default"];
    var RecipePreview, copyToClipboard;

    copyToClipboard = function(text) {
      return window.prompt("Send this link to your friends (copy with ctrl + c)", text);
    };

    RecipePreview = Ember.View.extend(StyleSwitchMixin, {
      templateName: "recipe",
      tagName: "div",
      style: (function() {
        if (!this.get("isPicture")) {
          return "height: 70px";
        }
        return "background-image:url('" + this.get("controller.model.srcImgFull") + "')";
      }).property("controller.model.srcImg"),
      isPicture: (function() {
        return !Ember.empty(this.get("controller.model.srcImgFull"));
      }).property("controller.model.srcImg"),
      rerenderWatch: (function() {
        return this.rerender();
      }).observes("controller.rerenderSwitch"),
      unitSetupInit: (function() {
        var allConversion, selectedConversion, u;
        u = this.get("controller.unit");
        if (u) {
          allConversion = this.get("controller.conversion.availConv");
          selectedConversion = allConversion.filter(function(i) {
            return i["id"] === u;
          });
          if (selectedConversion.length > 0) {
            return this.set("controller.conversion.currConv", selectedConversion[0]);
          }
        }
      }).on("didInsertElement"),
      didInsertElement: (function() {
        this.set("controller.redraw", (new Date()).getTime());
        window.scrollTo(0, 0);
        return this.share = new Share("#shareButtons", {
          title: "Graphical recipe for \"" + this.get("controller.model.title") + "\"",
          description: this.get("controller.model.description"),
          image: this.get("controller.model.srcImg"),
          url: "www.gracipe.com",
          networks: {
            google_plus: {
              before: function() {
                return window.ga && window.ga('send', 'event', 'Share', 'click', 'google_plus');
              }
            },
            pinterest: {
              before: function() {
                return window.ga && window.ga('send', 'event', 'Share', 'click', 'pinterest');
              }
            },
            twitter: {
              before: function() {
                return window.ga && window.ga('send', 'event', 'Share', 'click', 'twitter');
              }
            },
            facebook: {
              app_id: V2ENV.fbID,
              before: function() {
                return window.ga && window.ga('send', 'event', 'Share', 'click', 'facebook');
              }
            },
            email: {
              description: this.get("controller.model.description") + "\n\n Find the link here: " + window.location.href + "\n\n --A visulized recipe by www.gracipe.com",
              before: function() {
                copyToClipboard(window.location.href);
                return window.ga && window.ga('send', 'event', 'Share', 'click', 'email');
              }
            }
          }
        });
      }),
      actions: {
        onShare: function(name) {
          if (name === "embed") {
            return this.get("controller").send("onEmbed");
          } else {
            return this.share["network_" + name]();
          }
        },
        onEmbed: function() {
          var el;
          el = $("#embed-link");
          return this.get("controller").send("onEmbed");
        }
      }
    });

    __exports__["default"] = RecipePreview;;
  });
define("v2/views/recipe_edit", 
  ["v2/utils/editor-utils","v2/config/environment","v2/mixins/style-switch","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var UTILS = __dependency1__["default"];

    var V2ENV = __dependency2__["default"];

    var StyleSwitchMixin = __dependency3__["default"];
    var RecipePreview;

    window.autoMove = false;

    $("body").on("mousemove", function(e) {
      var amp, border, max, min, relPos, sign;
      amp = 0;
      border = 0.1;
      min = 10;
      max = 70;
      if (window.autoMove === false) {
        return;
      }
      relPos = ($(window).height() - e.clientY) / $(window).height();
      if (relPos < border) {
        amp = -relPos + border;
      }
      if (relPos > (1 - border)) {
        amp = -relPos + (1 - border);
      }
      sign = amp > 0 ? 1 : -1;
      if (amp === 0) {
        return;
      }
      return window.scrollBy(0, amp * max + sign * min);
    });

    window.initDragDrop = function(cbs) {
      var dragStartEvent, dropEvent, groupTime, moveFromAtoB;
      moveFromAtoB = function(a, a_parent, b, b_before, root) {
        var correctionOffset, tgtArr, tmpIndex;
        tmpIndex = null;
        tgtArr = [];
        if (a.attributes == null) {
          a.attributes = [];
        }
        if (b != null) {
          tgtArr = b.subs;
        } else {
          tgtArr = root.get("stepsArr");
        }
        if (b_before != null) {
          tmpIndex = tgtArr.indexOf(b_before);
        }
        correctionOffset = 0;
        if (tgtArr.indexOf(a) !== -1 && tgtArr.indexOf(a) < tmpIndex) {
          correctionOffset = -1;
        }
        if (!a.get("_parent")) {
          window.nodeDis.removeObjectFromDone(a);
        }
        a.destroy();
        a = root.createStep(a);
        if (window.lastElement != null) {
          if (window.lastElement === "delete") {
            return console.log("DELTE, do nothing");
          } else {
            if (lastElement.get("item").subs == null) {
              lastElement.get("item").subs = [];
            }
            return lastElement.get("item").subs.pushObject(a);
          }
        } else {
          if (tmpIndex != null) {
            tgtArr.insertAt(tmpIndex + correctionOffset, a);
          }
          if (tmpIndex == null) {
            return tgtArr.pushObject(a);
          }
        }
      };
      dropEvent = function($item, container, _super, cbs, isElement) {
        var NCondE, condA, condB, condC, condD, condIsSideproc, root, tmpA, tmpB, tmpB_before, triggerRerender, _ref, _ref1, _ref10, _ref11, _ref12, _ref13, _ref14, _ref15, _ref16, _ref17, _ref18, _ref19, _ref2, _ref20, _ref21, _ref22, _ref23, _ref3, _ref4, _ref5, _ref6, _ref7, _ref8, _ref9;
        window.ga && window.ga('send', 'event', 'Editor', 'drop', 'element');
        window.autoMove = false;
        $item.removeClass("dragged").removeAttr("style");
        $("body").removeClass("dragging");
        tmpA = UTILS.getClosestEmberView($item);
        tmpB = UTILS.getClosestEmberView($(container != null ? container.el[0] : void 0));
        tmpB_before = UTILS.getClosestEmberView((_ref = $item.nextAll(".ingred_container:not(.dummy), .sortme")) != null ? _ref.first() : void 0);
        if (tmpA) {
          root = tmpA.get("controller.model");
        }
        if (tmpB) {
          root = tmpB.get("controller.model");
        }
        if (tmpB_before) {
          root = tmpB_before.get("controller.model");
        }
        if (tmpB_before == null) {
          tmpB_before = UTILS.getClosestEmberView((_ref1 = $item.parent().parent().nextAll(".ingred_container:not(.dummy), .sortme")) != null ? _ref1.first() : void 0);
        }
        condA = (tmpB != null ? tmpB.get("parentView.parentView.parentView") : void 0) && ((_ref2 = tmpA.get("item")) != null ? (_ref3 = _ref2.subs) != null ? _ref3.length : void 0 : void 0) > 0;
        condA = false;
