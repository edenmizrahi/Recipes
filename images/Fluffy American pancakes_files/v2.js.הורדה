            recur(i);
            if (i.isResult && i.uid === puid) {
              _results.push(collector.push(i));
            } else {
              _results.push(void 0);
            }
          }
          return _results;
        };
        recur(this);
        _results = [];
        for (_i = 0, _len = collector.length; _i < _len; _i++) {
          i = collector[_i];
          _results.push(i.get("_parent.subs").removeObject(i));
        }
        return _results;
      },
      createResults: function(o) {
        var bar, foo, newUID, retO;
        foo = {
          name: "Result",
          isResult: 1,
          id: o.id,
          unitDescr: "gram"
        };
        newUID = UTILS.generateUUID();
        bar = this.createStep(foo);
        this.results[newUID] = bar;
        retO = this.createStep({
          "isResult": 1,
          "uid": newUID
        });
        return retO;
      },
      createStep: function(o) {
        if ("set" in o) {
          o.set("_root", this);
        } else {
          o["_root"] = this;
        }
        return RecipeStep.create(o);
      },
      resultsList: (function() {
        var k, v, _ref;
        _ref = this.results;
        for (k in _ref) {
          v = _ref[k];
          v.set("_first", this.resultsFistLUT[k]);
        }
        return this.results;
      }).property("reindex"),
      nrSteps: (function() {
        return this.get("stepsArr").length;
      }).property(),
      countId: function(id) {
        var dict, nrIngredRecur, v, _i, _len, _ref;
        nrIngredRecur = function(ref, dict) {
          var tmpID, v, _i, _len, _ref;
          if (!ref) {
            return 0;
          }
          if ('subs' in ref && ref.subs.length > 0) {
            _ref = ref.get('subs');
            for (_i = 0, _len = _ref.length; _i < _len; _i++) {
              v = _ref[_i];
              nrIngredRecur(v, dict);
            }
          }
          if (+ref.isResult === 1) {
            return;
          }
          if (+ref.type === id) {
            tmpID = ref.id;
            if (!ref.id) {
              tmpID = ref.element_id;
            }
            return dict[tmpID] = true;
          }
        };
        dict = {};
        _ref = this.get('stepsArr');
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          v = _ref[_i];
          nrIngredRecur(v, dict);
        }
        return Object.keys(dict).length;
      },
      nrTools: (function() {
        return this.countId(2);
      }).property(),
      nrIngredients: (function() {
        return this.countId(1);
      }).property(),
      nrMinutes: Ember.computed(function() {
        var calcTimeVal;
        if (this.get("preptime")) {
          return this.get("preptime");
        }
        calcTimeVal = this.get("nrMinutesCalculated");
        calcTimeVal = Math.ceil(calcTimeVal / 5) * 5;
        if (calcTimeVal <= 10) {
          return "< 10";
        }
        return calcTimeVal;
      }),
      nrMinutesCalculated: (function() {
        var searchTimerRecur, timerAccu, v, _i, _len, _ref;
        timerAccu = 0;
        searchTimerRecur = function(ref) {
          var i, v, _i, _j, _len, _len1, _ref, _ref1, _results;
          if ("attributes" in ref) {
