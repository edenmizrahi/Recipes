define("v2/utils/editor-utils", 
  ["exports"],
  function(__exports__) {
    "use strict";
    var utils;

    utils = {
      subWithSubs: function(subs) {
        var i, retVal, _i, _len, _ref;
        retVal = false;
        if (subs == null) {
          return false;
        }
        for (_i = 0, _len = subs.length; _i < _len; _i++) {
          i = subs[_i];
          if (((_ref = i.subs) != null ? _ref.length : void 0) > 0) {
            retVal = true;
          }
        }
        return retVal;
      },
      getClosestEmberView: function($el) {
        var id;
        id = $el.closest(".ember-view").attr("id");
        if (!id) {
          return;
        }
        if (Ember.View.views.hasOwnProperty(id)) {
          return Ember.View.views[id];
        }
        return void 0;
      }
    };

    __exports__["default"] = utils;
  });
define("v2/utils/recipe-editor-search-resolver", 
  ["v2/models/svg-element","v2/config/environment","v2/utils/ajax-resolver","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __exports__) {
    "use strict";
    var svgElement = __dependency1__["default"];

    var V2ENV = __dependency2__["default"];

    var ajax = __dependency3__["default"];

    var APISimulator, NodesDispatcher;

    APISimulator = function(lastTextAreaTxt, cb_ok, cb_fail) {
      return ajax.analyzeTextGetSymbol(lastTextAreaTxt, function(rq) {
        if (rq) {
          if (rq.length > 0) {
            return cb_ok(rq[0]);
          }
        }
        return cb_fail();
      });
    };

    NodesDispatcher = (function() {
      NodesDispatcher.prototype.queueList = Ember.A([]);

      NodesDispatcher.prototype.doneList = Ember.A([]);

      NodesDispatcher.prototype.currResolving = 0;

      NodesDispatcher.prototype.cntNum = 0;

      NodesDispatcher.prototype.hashLut = {};

      NodesDispatcher.prototype.maxSyncGetCalls = 3;

      function NodesDispatcher() {
        setInterval(((function(_this) {
          return function() {
            var i, _i, _len, _ref, _results;
            _ref = _this.queueList;
            _results = [];
            for (_i = 0, _len = _ref.length; _i < _len; _i++) {
              i = _ref[_i];
              if (i.get("status") === "new" && _this.currResolving <= _this.maxSyncGetCalls) {
                _this.requestData(i);
                _results.push(_this.currResolving += 1);
              } else {
                _results.push(void 0);
              }
            }
            return _results;
          };
        })(this)), 500);
      }

      NodesDispatcher.prototype.requestData = function(obj) {
        var str, tmpSvgEl;
        obj.set("status", "req");
        str = obj.get("str");
        if (this.hashLut[str]) {
          obj.set("status", "ok");
          obj.set("item", JSON.parse(this.hashLut[str]));
          tmpSvgEl = svgElement.create();
          tmpSvgEl.set("id", obj.get("item").id);
          obj.get("item").svgObj = tmpSvgEl;
          this.doneList.unshiftObject(obj);
          this.currResolving -= 1;
          return;
        }
        APISimulator(obj.get("str"), ((function(_this) {
          return function(retObj) {
            obj.set("status", "ok");
            obj.set("item", retObj);
            tmpSvgEl = svgElement.create();
            tmpSvgEl.set("id", retObj.id);
            retObj.svgObj = tmpSvgEl;
            console.log(retObj);
            _this.hashLut[obj.str] = JSON.stringify(retObj);
            return _this.doneList.unshiftObject(obj);
          };
        })(this)), ((function(_this) {
          return function() {
            var tmpO;
            obj.set("status", "error");
            tmpSvgEl = svgElement.create();
            tmpO = {
              name: obj.get("str"),
              calcAmount: "?"
            };
            obj.set("item", tmpO);
            tmpO.svgObj = tmpSvgEl;
            console.log("ERROR");
            return _this.doneList.unshiftObject(obj);
          };
        })(this)));
        return this.currResolving -= 1;
      };

      NodesDispatcher.prototype.createNode = function(str, replaceObj) {
        var node, obj, objTmp;
        node = Ember.Object.extend({
          isOk: (function() {
            return this.get("status") === "ok";
          }).property("status"),
          isError: (function() {
            return this.get("status") === "error";
          }).property("status"),
          isPending: (function() {
            return this.get("status") === "new" || this.get("status") === "req";
          }).property("status")
        });
        objTmp = {
          str: str,
          status: "new",
          cnt: this.cntNum++
        };
        obj = node.create(objTmp);
        if (replaceObj != null) {
          this.queueList.insertAt(this.queueList.indexOf(replaceObj), obj);
          this.queueList.removeObject(replaceObj);
        } else {
          this.queueList.unshiftObject(obj);
        }
        return obj;
      };

      NodesDispatcher.prototype.removeObjectFromDone = function(o) {
        var i, toBeRemoved, _i, _len, _ref;
        _ref = this.doneList;
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          i = _ref[_i];
          if (i.item.id === o.id) {
            toBeRemoved = i;
          }
        }
        if (toBeRemoved != null) {
          return this.doneList.removeObject(toBeRemoved);
        }
      };

      return NodesDispatcher;

    })();

    if (window.nodeDis == null) {
      window.nodeDis = new NodesDispatcher();
    }

    __exports__["default"] = nodeDis;;
  });
define("v2/utils/recipe-request-resolver", 
  ["ember","v2/utils/ajax-resolver","exports"],
  function(__dependency1__, __dependency2__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];

    var ajax = __dependency2__["default"];

    var RecipeRequestController;

    RecipeRequestController = (function() {
      function RecipeRequestController() {
        this.results = Ember.A([]);
        this.status = Ember.Object.create();
      }

      RecipeRequestController.prototype.currentPage = 0;

      RecipeRequestController.prototype.pageMaxSize = 7;

      RecipeRequestController.prototype.isLoading = false;

      RecipeRequestController.prototype.status = {};

      RecipeRequestController.prototype.opt = {};

      RecipeRequestController.prototype.results = [];

      RecipeRequestController.prototype.tags = [];

      RecipeRequestController.prototype.clear = function() {
        this.opt = {};
        this.results.clear();
        this.currentPage = 0;
        this.totalRecipes = -1;
        return this.tags = [];
      };

      RecipeRequestController.prototype.getNext = function(doneCB) {
        if (this.status.get("everythingLoaded")) {
          return;
        }
        if (this.status.get("isLoading")) {
          return;
        }
        this.status.set("isLoading", true);
        this.currentPage += 1;
        this.opt.page = this.currentPage;
        if (!this.opt.size) {
          this.opt.size = this.pageMaxSize;
        }
        return ajax.getGracipes(this.opt, ((function(_this) {
          return function(data) {
            if (data === null) {
              _this.status.set("hasError", true);
              return;
            }
            window.RUResult.doConversion(data.recipeArray, _this.results);
            _this.tags = data.allTags;
            _this.status.totalRecipes = data.totalRecipes;
            _this.status.totalPages = Math.floor(_this.totalRecipes / _this.pageMaxSize);
            _this.status.pagesLeft = Math.min(_this.pagesTotal - _this.currentPage, 0);
            if (_this.status.totalRecipes === _this.results.length) {
              _this.status.set("everythingLoaded", true);
            }
            if (doneCB) {
              doneCB();
            }
            return _this.status.set("isLoading", false);
          };
        })(this)));
      };

      return RecipeRequestController;

    })();

    __exports__["default"] = RecipeRequestController;
  });
define("v2/utils/recipe-utils", 
  ["ember","v2/config/environment","v2/models/svg-element","v2/models/conversion","v2/models/recipe","v2/utils/recipe-request-resolver","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __dependency5__, __dependency6__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];

    var V2ENV = __dependency2__["default"];

    var svgElement = __dependency3__["default"];

    var conversion = __dependency4__["default"];

    var Recipe = __dependency5__["default"];

    var RecipeRequestController = __dependency6__["default"];


    /**
     * Converts the function to JSON, ready for document storage. (At the moment we're just storing it in a mySQL field :())
     * @param  {Ember.Object} o Our recipe object, contains several computed properties that do not need to be serialized
     * @return {JSON}   returns a javascript object, that can be turned into JSON via JSON.stringify
     */
    var RUResult, adapter, doConversion, recipeToStorage, svg_recur, toJSON, uploadFiles;

    toJSON = function(o) {
      var cache, remSVG, resolveCirc, resolveCircularJSON;
      remSVG = function(o) {
        var k, v;
        if (typeof o !== 'object' || o === null) {
          return;
        }
        if ("svg" in o) {
          delete o.svg;
        }
        if ("sub" in o) {
          delete o.sub;
        }
        if ("svgObj" in o) {
          delete o.svgObj;
        }
        for (k in o) {
          v = o[k];
          if (v === "" || v === null || v === void 0 || v.length === 0) {
            delete o[k];
          }
          if (k === "isSingle" || k === "onlySingle" || k === "multiSubs") {
            delete o[k];
          }
          remSVG(v);
        }
      };
      cache = [];
      resolveCircularJSON = JSON.stringify(o, function(key, value) {
        if (typeof value === "object" && value !== null) {
          if ((("" + key).indexOf("_")) === 0) {
            return;
          }
          if (cache.indexOf(value) !== -1) {
            return;
          }
          cache.push(value);
        }
        return value;
      });
      resolveCirc = JSON.parse(resolveCircularJSON);
      remSVG(resolveCirc);
      return JSON.stringify(resolveCirc);
    };

    svg_recur = function(item) {
      var tmpId, tmpSvgEl, v, _i, _j, _len, _len1, _ref, _ref1, _results;
      if (item == null) {
        return;
      }
      tmpSvgEl = svgElement.create();
      if ("attr" in item) {
        tmpId = item.attr;
      }
      if ("id" in item) {
        tmpId = item.id;
      }
      if ("element_id" in item) {
        tmpId = item.element_id;
      }
      tmpSvgEl.set("itemRef", item);
      tmpSvgEl.set("rawSvg", item.svg);
      tmpSvgEl.set("id", tmpId);
      item.svgObj = tmpSvgEl;
      if ("subs" in item) {
        _ref = item.subs;
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          v = _ref[_i];
          svg_recur(v);
        }
      }
      if ("attributes" in item) {
        _ref1 = item.attributes;
        _results = [];
        for (_j = 0, _len1 = _ref1.length; _j < _len1; _j++) {
          v = _ref1[_j];
          _results.push(svg_recur(v));
        }
        return _results;
      }
    };


    /**
     * Takes in a signle raw RECIPE from server and converts it to an Ember recipe object
     * @param  {object} p_data raw recipe object, from server
     * @return {Ember.Object}        Recipe object, see above Recipe
     */

    adapter = function(p_data) {
      var adapter_recursive, k, recipe, tmp, v, _ref, _ref1, _ref2;
      adapter_recursive = function(item, parent) {
        var i, idx, k, tmpArr, tmpRes, v, vCpy, _i, _j, _len, _len1, _ref, _ref1;
        if (!item) {
          return;
        }
        item = recipe.createStep(item);
        if (!("attributes" in item)) {
          item.attributes = [];
        }
        _ref = item.attributes;
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          i = _ref[_i];
          if (i != null) {
            if (!("id" in i)) {
              i.id = +i.attr;
            }
          }
        }
        if (!("id" in item)) {
          item.set("id", +item.element_id);
        }
        item.set("id", +item.id);
        if ("subs" in item && item.subs.length !== 0) {
          tmpArr = item.subs;
          item.subs = [];
          for (idx = _j = 0, _len1 = tmpArr.length; _j < _len1; idx = ++_j) {
            i = tmpArr[idx];
            tmpRes = adapter_recursive(i, parent);
            if (tmpRes) {
              item.subs.push(tmpRes);
            }
          }
        }
        _ref1 = item.sub;
        for (k in _ref1) {
          v = _ref1[k];
          vCpy = JSON.parse(JSON.stringify(v));
          vCpy = adapter_recursive(vCpy, parent);
          item.subs.push(vCpy);
        }
        item.subs.removeObject(null);
        return item;
      };
      recipe = Recipe.create(p_data);
      recipe.set("cats", (_ref = recipe.get("cats")) != null ? _ref : []);
      if (recipe.get("title") === "undefined") {
        recipe.set("title", "");
      }
      if (recipe.get("description") === "undefined") {
        recipe.set("description", "");
      }
      recipe.set("conversion", Ember.Object.create(conversion));
      if (p_data.steps && "version" in p_data.steps) {
        recipe.set("graciphyInputIngredTxt", p_data.steps.graciphyInputIngredTxt);
        recipe.set("graciphyInputTxt", p_data.steps.graciphyInputTxt);
        recipe.set("overriddenIngredConfig", p_data.steps.overriddenIngredConfig);
        recipe.results = p_data.steps.results;
        _ref1 = recipe.results;
        for (k in _ref1) {
          v = _ref1[k];
          recipe.results[k] = recipe.createStep(v);
        }
        p_data.steps = p_data.steps.steps;
      }
      _ref2 = p_data.steps;
      for (k in _ref2) {
        v = _ref2[k];
        if (k === "_meta") {
          continue;
        }
        tmp = adapter_recursive(v, recipe);
        svg_recur(tmp);
        recipe.get("stepsArr").addObject(tmp);
      }
      recipe.reindexMeta();
      return recipe;
    };


    /**
     * Converts the JSON response from server to array of RECIPES
     * @param  {array} data     JSON response from server, array with raw recipes
     * @param  {Ember.Array} arrayRef Array reference, where the converted RECIPES will be pushed
     */

    doConversion = function(data, arrayRef) {
      var i, _i, _len, _results;
      _results = [];
      for (_i = 0, _len = data.length; _i < _len; _i++) {
        i = data[_i];
        _results.push(arrayRef.pushObject(adapter(i)));
      }
      return _results;
    };

    recipeToStorage = function(m) {
      var i, idx, steps, tmpV2TransitionResults, tmpV2TransitionSteps, v2Storage, _i, _len, _ref;
      steps = {};
      _ref = m.get("stepsArr");
      for (idx = _i = 0, _len = _ref.length; _i < _len; idx = ++_i) {
        i = _ref[idx];
        steps[idx] = i;
      }
      tmpV2TransitionSteps = JSON.parse(toJSON(steps));
      tmpV2TransitionResults = JSON.parse(toJSON(m.results));
      v2Storage = {
        id: m.id,
        graciphyInputTxt: m.graciphyInputTxt,
        graciphyInputIngredTxt: m.graciphyInputIngredTxt,
        overriddenIngredConfig: m.overriddenIngredConfig,
        version: 2,
        steps: tmpV2TransitionSteps,
        results: tmpV2TransitionResults
      };
      return JSON.stringify(v2Storage);
    };

    uploadFiles = function(files, cb) {
      var data;
      data = new FormData();
      $.each(files, function(key, value) {
        data.append(key, value);
      });
      console.log(data);
      $.ajax({
        url: V2ENV.tgtURL + "/mapi/uploadIphone/",
        type: "POST",
        data: data,
        dataType: "json",
        processData: false,
        contentType: false,
        success: function(data, textStatus, jqXHR) {
          if (typeof data.error === "undefined") {
            console.log(data.filePath);
            cb(data);
          } else {
            console.log("ERRORS: " + data.error);
          }
        },
        error: function(jqXHR, textStatus, errorThrown) {
          console.log("ERRORS: " + textStatus);
        }
      });
    };

    RUResult = {
      toJSON: toJSON,
      svg_recur: svg_recur,
      adapter: adapter,
      doConversion: doConversion,
      recipeToStorage: recipeToStorage,
      uploadFiles: uploadFiles,
      convertJSON: (function(_this) {
        return function(json) {
          var tmpArr;
          tmpArr = [];
          doConversion([json], tmpArr);
          return tmpArr[0];
        };
      })(this),
      findById: (function(_this) {
        return function(id) {
          var idReqController;
          idReqController = new RecipeRequestController();
          idReqController.opt = {
            id: id
          };
          return idReqController.getNext().then(function() {
            return idReqController.results[0];
          });
        };
      })(this),
      findBySlug: (function(_this) {
        return function(slug) {
          var idReqController;
          idReqController = new RecipeRequestController();
          idReqController.opt = {
            slug: slug
          };
          return idReqController.getNext().then(function() {
            return idReqController.results[0];
          });
        };
      })(this)
    };

    window.RUResult = RUResult;

    __exports__["default"] = RUResult;
  });
define("v2/utils/svg-element-resolver", 
  ["v2/utils/utils","ember","v2/utils/ajax-resolver","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var utils = __dependency1__["default"];

    var Ember = __dependency2__["default"];

    var ajax = __dependency3__["default"];

    var ENV = __dependency4__["default"];

    var DEBUG, SvgResolver, mySvgResolver, resolver,
      __bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

    DEBUG = 0;

    if (ENV.preventDEBUG) {
      DEBUG = 0;
    }

    localStorage["icons"] = [];

    SvgResolver = (function() {
      SvgResolver.prototype.active = [];

      SvgResolver.prototype.activeBufferWhileBusy = [];

      SvgResolver.prototype.lut = {};

      SvgResolver.prototype.ls = {};

      SvgResolver.prototype.timeoutInDays = 5;

      SvgResolver.prototype.timeout = null;

      SvgResolver.prototype.isResolvingNow = 0;

      function SvgResolver() {
        this.loadLS = __bind(this.loadLS, this);
        this.storeLS = __bind(this.storeLS, this);
        this.addSymbol = __bind(this.addSymbol, this);
        this.resolveNow = __bind(this.resolveNow, this);
        if (!DEBUG) {
          this.loadLS();
        }
      }

      SvgResolver.prototype.resolveNow = function() {
        var doIt, idArr, opt, tmpUniqVal;
        doIt = (function(_this) {
          return function(updateVals) {
            var i, k, triggerUpdate, v, _i, _j, _len, _len1, _ref, _ref1;
            for (k in updateVals) {
              v = updateVals[k];
              _this.ls[k] = v;
            }
            if (!DEBUG) {
              _this.storeLS();
            }
            _ref = _this.active;
            for (_i = 0, _len = _ref.length; _i < _len; _i++) {
              v = _ref[_i];
              if (v.id in _this.ls) {
                if (v.cb) {
                  v.cb(_this.ls[v.id]);
                }
              }
            }
            _this.timeout = null;
            _this.active = [];
            triggerUpdate = 0;
            _ref1 = _this.activeBufferWhileBusy;
            for (_j = 0, _len1 = _ref1.length; _j < _len1; _j++) {
              i = _ref1[_j];
              _this.active.push(i);
              triggerUpdate = 1;
            }
            _this.isResolvingNow = 0;
            _this.activeBufferWhileBusy = [];
            if (triggerUpdate) {
              return _this.addSymbol(null, null);
            }
          };
        })(this);
        this.isResolvingNow = 1;
        idArr = this.active.map(function(i) {
          return i.id;
        });
        tmpUniqVal = idArr.unique();
        opt = {
          svgs: JSON.stringify(tmpUniqVal)
        };
        return ajax.getSymbols(opt, doIt);
      };

      SvgResolver.prototype.addSymbol = function(id, cb) {
        if (id !== null && (id in this.ls)) {
          cb(this.ls[id]);
          return;
        }
        if (!this.isResolvingNow) {
          this.active.push({
            cb: cb,
            id: id
          });
          if (this.timeout) {
            clearTimeout(this.timeout);
          }
          return this.timeout = setTimeout(this.resolveNow, 5);
        } else {
          return this.activeBufferWhileBusy.push({
            cb: cb,
            id: id
          });
        }
      };

      SvgResolver.prototype.storeLS = function() {
        if (utils.supports_html5_storage()) {
          return localStorage["icons"] = JSON.stringify(this.ls);
        }
      };

      SvgResolver.prototype.loadLS = function() {
        var a;
        if (utils.supports_html5_storage() && localStorage["icons"]) {
          if ("iconsTimeout" in localStorage && +localStorage["iconsTimeout"] < (new Date()).getTime()) {
            delete localStorage["iconsTimeout"];
            delete localStorage["icons"];
            console.log("#####deleting everything....");
          }
          if (!("iconsTimeout" in localStorage)) {
            a = new Date();
            a.setDate(a.getDate() + this.timeoutInDays);
            localStorage["iconsTimeout"] = a.getTime();
            localStorage["icons"] = "{}";
            console.log("#####item set new @timeout....");
          }
          return this.ls = JSON.parse(localStorage["icons"]);
        }
      };

      return SvgResolver;

    })();

    mySvgResolver = new SvgResolver();

    resolver = mySvgResolver;

    __exports__["default"] = resolver;
  });
define("v2/utils/symbol-lut", 
