define("v2/app", 
  ["ember","ember/resolver","ember/load-initializers","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var Resolver = __dependency2__["default"];
    var loadInitializers = __dependency3__["default"];
    var config = __dependency4__["default"];

    Ember.MODEL_FACTORY_INJECTIONS = true;

    var GRACIPE_ROOT = window.GRACIPE_ROOT ? window.GRACIPE_ROOT : 'body'; // ##POSTPROCESSOR_REMOVE##

    var App = Ember.Application.extend({
      modulePrefix: config.modulePrefix,
      podModulePrefix: config.podModulePrefix,
      Resolver: Resolver,

      rootElement: GRACIPE_ROOT,

      ready: function () {
        $("#loader").remove();
      }
    });



    loadInitializers(App, config.modulePrefix);


    __exports__["default"] = App;
  });
define("v2/components/content-editable", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var ContentEditableComponent;

    ContentEditableComponent = Ember.Component.extend({
      classNames: ['ember-content-editable'],
      classNameBindings: ['extraClass'],
      attributeBindings: ['contenteditable', 'placeholder', 'spellcheck', 'tabindex', 'readonly', 'disabled'],
      contenteditable: Ember.computed('editable', 'disabled', function() {
        if (this.get('editable') != null) {
          if (this.get('editable')) {
            Ember.deprecate('You set editable=true on content-editable, ' + 'but this has been deprecated in favour of disabled=false');
          } else {
            Ember.deprecate('You set editable=false on content-editable, ' + 'but this has been deprecated in favour of disabled=true');
          }
          return this.get('editable');
        } else {
          return !this.get('disabled');
        }
      }),
      editable: null,
      disabled: null,
      spellcheck: null,
      isText: null,
      type: null,
      readonly: null,
      inputType: Ember.computed('type', 'isText', function() {
        if (this.get('isText') != null) {
          if (this.get('isText')) {
            Ember.deprecate('You set isText=true on content-editable, ' + 'but this has been deprecated in favour of type=\'text\'');
            return 'text';
          } else {
            Ember.deprecate('You set isText=false on content-editable, ' + 'but this has been deprecated in favour of type=\'html\'');
            return 'html';
          }
        } else {
          return this.get('type') || 'html';
        }
      }),
      setup: Ember.on('didInsertElement', function() {
        this.setValue();
        Ember.run.once((function(_this) {
          return function() {
            _this._processInput();
          };
        })(this));
        this.$().on('paste', (function(_this) {
          return function(event) {
            _this.handlePaste(event, _this);
          };
        })(this));
      }),
      tidy: Ember.on('willDestroyElement', function() {
        this.$().off('paste');
      }),
      _observeValue: true,
      valueChanged: Ember.observer('value', function() {
        if (this.get('_observeValue')) {
          this.setValue();
        }
      }),
      setValue: function() {
        var a, b, c, regex, x;
        if (this.element) {
          if (this.get('value')) {
            if (this.get("splitIntoList")) {
              a = (function() {
                var _i, _len, _ref, _results;
                _ref = this.get("value").split("\n");
                _results = [];
                for (_i = 0, _len = _ref.length; _i < _len; _i++) {
                  x = _ref[_i];
                  _results.push("<li>" + x + "</li>");
                }
                return _results;
              }).call(this);
              b = a.join("");
              c = "<ol>" + b + "</ol>";
              this.$().html(c);
            } else {
              regex = new RegExp("\n", "g");
              this.$().html(this.get('value').replace(regex, "\n<br>"));
            }
          } else {
            this.$().text(this.get('value'));
          }
        }
      },
      stringInterpolator: function(s) {
        return s;
      },
      _getInputValue: function() {
        var val;
        if (this.get('inputType') == 'html') {
          val = this.$().html();
          val = this.$('<div/>').html(val).text();
          return val;
        } else {
          return this.element.innerText || this.element.textContent;
        }
      },
      _processInput: function() {
        var val;
        val = this._getInputValue();
        val = this.stringInterpolator(val);
        val = this.htmlSafe(val);
        this.set('_observeValue', false);
        this.set('value', val);
        this.set('_observeValue', true);
      },
      htmlSafe: function(val) {
        if (this.get('inputType') == 'html') {
          return Ember.String.htmlSafe(val).toString();
        } else {
          return val;
        }
      },
      isUnderMaxLength: function(val) {
        return Ember.isEmpty(this.get('maxlength')) || val.length < this.get('maxlength');
      },
      updateValue: Ember.on('keyUp', function(event) {
        this._processInput();
        this.handleKeyUp(event);
      }),
      handleKeyUp: function(event) {
        if (this.get('readonly')) {
          event.preventDefault();
          return false;
        }
        this.sendAction('key-up', this.get('value'), event);
      },
      handlePaste: function(e, _this) {
        var allGoodLis, text, w, x, _ref;
        e.preventDefault();
        text = e.originalEvent.clipboardData.getData("text/plain");
        w = (_ref = _this.get("pasteWrapper")) != null ? _ref : "div";
        text = ((function() {
          var _i, _len, _ref1, _results;
          _ref1 = text.split("\n");
          _results = [];
          for (_i = 0, _len = _ref1.length; _i < _len; _i++) {
            x = _ref1[_i];
            _results.push(("<" + w + ">") + x + ("</" + w + ">"));
          }
          return _results;
        })()).join("");
        document.execCommand("insertHTML", false, text);
        Ember.run.scheduleOnce('afterRender', (function(_this) {
          return function() {};
        })(this));
        if (this.get("splitIntoList")) {
          allGoodLis = this.$().find('*:not(:has(*))');
          this.set('value', ((function() {
            var _i, _len, _results;
            _results = [];
            for (_i = 0, _len = allGoodLis.length; _i < _len; _i++) {
              x = allGoodLis[_i];
              _results.push($(x).text());
            }
            return _results;
          })()).join("\n"));
          this.setValue();
        }
      },
      keyDown: function(event) {
        if (this.get('readonly')) {
          event.preventDefault();
          return false;
        }
        if (event.keyCode == 27) {
          this.sendAction('escape-press', this, event);
        } else if (event.keyCode == 13) {
          this.sendAction('enter', this, event);
          this.sendAction('insert-newline', this, event);
        }
        this.sendAction('key-down', this.get('value'), event);
      },
      keyPress: function(event) {
        var end, key, start, val;
        if (this.get('readonly')) {
          event.preventDefault();
          return false;
        }
        val = this._getInputValue();
        if (!this.isUnderMaxLength(val)) {
          if (window.getSelection().rangeCount > 0) {
            start = window.getSelection().getRangeAt(0).startOffset;
            end = window.getSelection().getRangeAt(0).endOffset;
            if (start == end) {
              event.preventDefault();
            }
          } else {
            event.preventDefault();
          }
        }
        if (this.get('type') == 'number') {
          key = event.which || event.keyCode;
          if (key < 48 || key >= 58) {
            event.preventDefault();
            return false;
          }
        }
        this.sendAction('key-press', this, event);
      },
      focusIn: function(event) {
        this.sendAction('focus-in', this, event);
      },
      focusOut: function(event) {
        this.sendAction('focus-out', this, event);
      },
      mouseEnter: function(event) {
        this.sendAction('mouse-enter', this, event);
      },
      mouseLeave: function(event) {
        this.sendAction('mouse-leave', this, event);
      }
    });

    __exports__["default"] = ContentEditableComponent;
  });
define("v2/components/edit-ingredient-ctrl", 
  ["ember","exports"],
  function(__dependency1__, __exports__) {
    "use strict";
    var Ember = __dependency1__["default"];
    var EditIngredientCtrlComponent;

    EditIngredientCtrlComponent = Ember.Component.extend({
      attributeWatch: Ember.observer("el.type", function() {
        var elId, elType;
        if (!this.get("el.type")) {
          return;
        }
        elId = this.get("el.id");
        elType = this.get("el.type");
        if (!this.get("recipe.overriddenIngredConfig")) {
          this.set("recipe.overriddenIngredConfig", {});
        }
        this.set("recipe.overriddenIngredConfig." + elId, elType);
        Ember.run.scheduleOnce('afterRender', (function(_this) {
          return function() {
            _this.get("recipe").notifyPropertyChange("overriddenIngredConfig");
            return _this.set("recipe.reindex", new Date());
          };
        })(this));
        return console.log(this.get("recipe.overriddenIngredConfig"));
      })
    });

    __exports__["default"] = EditIngredientCtrlComponent;
  });
