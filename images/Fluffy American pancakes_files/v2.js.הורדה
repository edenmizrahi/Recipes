            $(_this).css("background-color", "rgb(188,174,149)");
            return window.lastElement = UTILS.getClosestEmberView($(_this));
          };
        })(this), 1000);
        return $(this).mouseout(function() {
          clearTimeout(lastElementTO);
          $(this).css("background-color", "");
