define("v2/controllers/application", 
  ["v2/utils/ajax-resolver","v2/models/user","v2/utils/utils","v2/config/environment","exports"],
  function(__dependency1__, __dependency2__, __dependency3__, __dependency4__, __exports__) {
    "use strict";
    var ajax = __dependency1__["default"];

    var user = __dependency2__["default"];

    var utils = __dependency3__["default"];

    var V2ENV = __dependency4__["default"];

    var App, currUser, e, initVal;

    initVal = {};

    if (utils.supports_html5_storage() && localStorage["user"]) {
      try {
        initVal = JSON.parse(localStorage["user"]);
      } catch (_error) {
        e = _error;
        console.warn(localStorage["user"]);
        console.warn(e);
      }
    }

    currUser = user.create(initVal);

    currUser.isMe = true;

    ajax.setOAuthAccessToken(currUser.get("secretAT"));

    App = Ember.ObjectController.extend({
      needs: ["index"],
      queryParams: ["hideMenu"],
      hideMenu: false,
      searchQuery: "",
      ctrlPressed: 1,
      modalActive: false,
      user: currUser,
      recipe: {},
      searchAuto: (function() {
        return jQuery.getJSON(V2ENV.tgtURL + "/mapi/autocomplete");
      }).property(""),
      noTracking: (function() {
        return !!localStorage.noGA;
      }).property(""),
      loggedIn: (function() {
        return !!this.get("user.secretAT");
      }).property("user.secretAT", "user"),
      mobileKey: (function() {
        return alert(this.get("search"));
      }).property("search"),
      actions: {
        userLogout: function() {
          this.set("user.secretAT", null);
          this.set("user", user.create());
          if (utils.supports_html5_storage()) {
            return localStorage["user"] = null;
          }
        },
        searchQueryTrigger: function() {
          console.log("Search for.... " + (this.get('searchQuery')));
          $(".form-control").blur();
          $(".loading").focus();
          return this.transitionTo('search', {
            queryParams: {
              q: this.get('searchQuery'),
              category: null
            }
          });
        },
        gotoHome: function() {
          this.transitionToRoute('index');
          this.set("searchQuery", "");
          return this.get('controllers.index').send("triggerHome");
        },
        createNewGracipe: function() {
          return this.transitionToRoute('recipe_edit4', 'new');
        }
      },
      informAjaxIfSecretATChanged: (function() {
        var currEmail;
        ajax.setOAuthAccessToken(this.get("user.secretAT"));
        currEmail = this.get("user.email");
        return ajax.getUser({
          userName: currEmail
        }, (function(_this) {
          return function(d) {
            var tmpTest;
            if (d == null) {
              console.log("Could not get new user details");
              return;
            }
            _this.set("user.id", d.id);
            _this.set("user.first_name", d.first_name);
            _this.set("user.last_name", d.last_name);
            _this.set("user.disqus", d.disqus);
            _this.set("user.avatar", d.avatar);
            if (utils.supports_html5_storage()) {
              tmpTest = _this.user.serialize();
              localStorage["user"] = JSON.stringify(tmpTest);
              return console.log(tmpTest);
            }
          };
        })(this));
      }).observes('user.secretAT'),
      init: function() {
        if (this.get("user.id")) {
          ajax.getUser({
            id: this.get("user.id")
          }, (function(_this) {
            return function(o) {
              if (!o) {
                return _this.send("userLogout");
              }
            };
          })(this));
        }
        return this._super();
      },
      currentPathChanged: (function() {}).observes('currentPath')
    });

    __exports__["default"] = App;;
  });
